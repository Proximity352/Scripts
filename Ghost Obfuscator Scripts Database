ion\" then Main.Executor = identifyexecutor() end \t\t\t\tMain.GuiHolder = Main.Elevated and service.CoreGui or plr:FindFirstChildWhichIsA(\"PlayerGui\") \t\t\t\tsetmetatable(env, nil) \t\tend \tMain.LoadSettings = function() \t\tlocal s,data = pcall(env.readfile or error,\"DexSettings.json\") \t\tif s and data and data ~= \"\" then \t\t\tlocal s,decoded = service.HttpService:JSONDecode(data) \t\t\tif s and decoded then \t\t\t\tfor i,v in next,decoded do \t\t\t\t\t \t\t\t\tend \t\t\telse \t\t\t\t-- TODO: Notification \t\t\tend \t\telse \t\t\tMain.ResetSettings() \t\tend \tend \t \tMain.ResetSettings = function() \t\tlocal function recur(t,res) \t\t\tfor set,val in pairs(t) do \t\t\t\tif type(val) == \"table\" and val._Recurse then \t\t\t\t\tif type(res[set]) ~= \"table\" then \t\t\t\t\t\tres[set] = {} \t\t\t\t\tend \t\t\t\t\trecur(val,res[set]) \t\t\t\telse \t\t\t\t\tres[set] = val \t\t\t\tend \t\t\tend \t\t\treturn res \t\tend \t\trecur(DefaultSettings,Settings) \tend \t \tMain.FetchAPI = function() \t\tlocal api,rawAPI \t\tif Main.Elevated then \t\t\tif Main.LocalDepsUpToDate() then \t\t\t\tlocal localAPI = Lib.ReadFile(\"dex/rbx_api.dat\") \t\t\t\tif localAPI then \t\t\t\t\trawAPI = localAPI \t\t\t\telse \t\t\t\t\tMain.DepsVersionData[1] = \"\" \t\t\t\tend \t\t\tend \t\t\trawAPI = rawAPI or game:HttpGet(\"http://setup.roblox.com/\"..Main.RobloxVersion..\"-API-Dump.json\") \t\telse \t\t\tif script:FindFirstChild(\"API\") then \t\t\t\trawAPI = require(script.API) \t\t\telse \t\t\t\terror(\"NO API EXISTS\") \t\t\tend \t\tend \t\tMain.RawAPI = rawAPI \t\tapi = service.HttpService:JSONDecode(rawAPI) \t\t \t\tlocal classes,enums = {},{} \t\tlocal categoryOrder,seenCategories = {},{} \t\t \t\tlocal function insertAbove(t,item,aboveItem) \t\t\tlocal findPos = table.find(t,item) \t\t\tif not findPos then return end \t\t\ttable.remove(t,findPos) \t\t\tlocal pos = table.find(t,aboveItem) \t\t\tif not pos then return end \t\t\ttable.insert(t,pos,item) \t\tend \t\t \t\tfor _,class in pairs(api.Classes) do \t\t\tlocal newClass = {} \t\t\tnewClass.Name = class.Name \t\t\tnewClass.Superclass = class.Superclass \t\t\tnewClass.Properties = {} \t\t\tnewClass.Functions = {} \t\t\tnewClass.Events = {} \t\t\tnewClass.Callbacks = {} \t\t\tnewClass.Tags = {} \t\t\t \t\t\tif class.Tags then for c,tag in pairs(class.Tags) do newClass.Tags[tag] = true end end \t\t\tfor __,member in pairs(class.Members) do \t\t\t\tlocal newMember = {} \t\t\t\tnewMember.Name = member.Name \t\t\t\tnewMember.Class = class.Name \t\t\t\tnewMember.Security = member.Security \t\t\t\tnewMember.Tags ={} \t\t\t\tif member.Tags then for c,tag in pairs(member.Tags) do newMember.Tags[tag] = true end end \t\t\t\t \t\t\t\tlocal mType = member.MemberType \t\t\t\tif mType == \"Property\" then \t\t\t\t\tlocal propCategory = member.Category or \"Other\" \t\t\t\t\tpropCategory = propCategory:match(\"^%s*(.-)%s*$\") \t\t\t\t\tif not seenCategories[propCategory] then \t\t\t\t\t\tcategoryOrder[#categoryOrder+1] = propCategory \t\t\t\t\t\tseenCategories[propCategory] = true \t\t\t\t\tend \t\t\t\t\tnewMember.ValueType = member.ValueType \t\t\t\t\tnewMember.Category = propCategory \t\t\t\t\tnewMember.Serialization = member.Serialization \t\t\t\t\ttable.insert(newClass.Properties,newMember) \t\t\t\telseif mType == \"Function\" then \t\t\t\t\tnewMember.Parameters = {} \t\t\t\t\tnewMember.ReturnType = member.ReturnType.Name \t\t\t\t\tfor c,param in pairs(member.Parameters) do \t\t\t\t\t\ttable.insert(newMember.Parameters,{Name = param.Name, Type = param.Type.Name}) \t\t\t\t\tend \t\t\t\t\ttable.insert(newClass.Functions,newMember) \t\t\t\telseif mType == \"Event\" then \t\t\t\t\tnewMember.Parameters = {} \t\t\t\t\tfor c,param in pairs(member.Parameters) do \t\t\t\t\t\ttable.insert(newMember.Parameters,{Name = param.Name, Type = param.Type.Name}) \t\t\t\t\tend \t\t\t\t\ttable.insert(newClass.Events,newMember) \t\t\t\tend \t\t\tend \t\t\t \t\t\tclasses[class.Name] = newClass \t\tend \t\t \t\tfor _,class in pairs(classes) do \t\t\tclass.Superclass = classes[class.Superclass] \t\tend \t\t \t\tfor _,enum in pairs(api.Enums) do \t\t\tlocal newEnum = {} \t\t\tnewEnum.Name = enum.Name \t\t\tnewEnum.Items = {} \t\t\tnewEnum.Tags = {} \t\t\t \t\t\tif enum.Tags then for c,tag in pairs(enum.Tags) do newEnum.Tags[tag] = true end end \t\t\tfor __,item in pairs(enum.Items) do \t\t\t\tlocal newItem = {} \t\t\t\tnewItem.Name = item.Name \t\t\t\tnewItem.Value = item.Value \t\t\t\ttable.insert(newEnum.Items,newItem) \t\t\tend \t\t\t \t\t\tenums[enum.Name] = newEnum \t\tend \t\t \t\tlocal function getMember(class,member) \t\t\tif not classes[class] or not classes[class][member] then return end \t\t\tlocal result = {} \t\t\t \t\t\tlocal currentClass = classes[class] \t\t\twhile currentClass do \t\t\t\tfor _,entry in pairs(currentClass[member]) do \t\t\t\t\tresult[#result+1] = entry \t\t\t\tend \t\t\t\tcurrentClass = currentClass.Superclass \t\t\tend \t\t\t \t\t\ttable.sort(result,function(a,b) return a.Name < b.Name end) \t\t\treturn result \t\tend \t\t \t\tinsertAbove(categoryOrder,\"Behavior\",\"Tuning\") \t\tinsertAbove(categoryOrder,\"Appearance\",\"Data\") \t\tinsertAbove(categoryOrder,\"Attachments\",\"Axes\") \t\tinsertAbove(categoryOrder,\"Cylinder\",\"Slider\") \t\tinsertAbove(categoryOrder,\"Localization\",\"Jump Settings\") \t\tinsertAbove(categoryOrder,\"Surface\",\"Motion\") \t\tinsertAbove(categoryOrder,\"Surface Inputs\",\"Surface\") \t\tinsertAbove(categoryOrder,\"Part\",\"Surface Inputs\") \t\tinsertAbove(categoryOrder,\"Assembly\",\"Surface Inputs\") \t\tinsertAbove(categoryOrder,\"Character\",\"Controls\") \t\tcategoryOrder[#categoryOrder+1] = \"Unscriptable\" \t\tcategoryOrder[#categoryOrder+1] = \"Attributes\" \t\t \t\tlocal categoryOrderMap = {} \t\tfor i = 1,#categoryOrder do \t\t\tcategoryOrderMap[categoryOrder[i]] = i \t\tend \t\t \t\treturn { \t\t\tClasses = classes, \t\t\tEnums = enums, \t\t\tCategoryOrder = categoryOrderMap, \t\t\tGetMember = getMember \t\t} \tend \t \tMain.FetchRMD = function() \t\tlocal rawXML \t\tif Main.Elevated then \t\t\tif Main.LocalDepsUpToDate() then \t\t\t\tlocal localRMD = Lib.ReadFile(\"dex/rbx_rmd.dat\") \t\t\t\tif localRMD then \t\t\t\t\trawXML = localRMD \t\t\t\telse \t\t\t\t\tMain.DepsVersionData[1] = \"\" \t\t\t\tend \t\t\tend \t\t\trawXML = rawXML or game:HttpGet(\"https://raw.githubusercontent.com/CloneTrooper1019/Roblox-Client-Tracker/roblox/ReflectionMetadata.xml\") \t\telse \t\t\tif script:FindFirstChild(\"RMD\") then \t\t\t\trawXML = require(script.RMD) \t\t\telse \t\t\t\terror(\"NO RMD EXISTS\") \t\t\tend \t\tend \t\tMain.RawRMD = rawXML \t\tlocal parsed = Lib.ParseXML(rawXML) \t\tlocal classList = parsed.children[1].children[1].children \t\tlocal enumList = parsed.children[1].children[2].children \t\tlocal propertyOrders = {} \t\t \t\tlocal classes,enums = {},{} \t\tfor _,class in pairs(classList) do \t\t\tlocal className = \"\" \t\t\tfor _,child in pairs(class.children) do \t\t\t\tif child.tag == \"Properties\" then \t\t\t\t\tlocal data = {Properties = {}, Functions = {}} \t\t\t\t\tlocal props = child.children \t\t\t\t\tfor _,prop in pairs(props) do \t\t\t\t\t\tlocal name = prop.attrs.name \t\t\t\t\t\tname = name:sub(1,1):upper()..name:sub(2) \t\t\t\t\t\tdata[name] = prop.children[1].text \t\t\t\t\tend \t\t\t\t\tclassName = data.Name \t\t\t\t\tclasses[className] = data \t\t\t\telseif child.attrs.class == \"ReflectionMetadataProperties\" then \t\t\t\t\tlocal members = child.children \t\t\t\t\tfor _,member in pairs(members) do \t\t\t\t\t\tif member.attrs.class == \"ReflectionMetadataMember\" then \t\t\t\t\t\t\tlocal data = {} \t\t\t\t\t\t\tif member.children[1].tag == \"Properties\" then \t\t\t\t\t\t\t\tlocal props = member.children[1].children \t\t\t\t\t\t\t\tfor _,prop in pairs(props) do \t\t\t\t\t\t\t\t\tif prop.attrs then \t\t\t\t\t\t\t\t\t\tlocal name = prop.attrs.name \t\t\t\t\t\t\t\t\t\tname = name:sub(1,1):upper()..name:sub(2) \t\t\t\t\t\t\t\t\t\tdata[name] = prop.children[1].text \t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tif data.PropertyOrder then \t\t\t\t\t\t\t\t\tlocal orders = propertyOrders[className] \t\t\t\t\t\t\t\t\tif not orders then orders = {} propertyOrders[className] = orders end \t\t\t\t\t\t\t\t\torders[data.Name] = tonumber(data.PropertyOrder) \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tclasses[className].Properties[data.Name] = data \t\t\t\t\t\t\tend \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\telseif child.attrs.class == \"ReflectionMetadataFunctions\" then \t\t\t\t\tlocal members = child.children \t\t\t\t\tfor _,member in pairs(members) do \t\t\t\t\t\tif member.attrs.class == \"ReflectionMetadataMember\" then \t\t\t\t\t\t\tlocal data = {} \t\t\t\t\t\t\tif member.children[1].tag == \"Properties\" then \t\t\t\t\t\t\t\tlocal props = member.children[1].children \t\t\t\t\t\t\t\tfor _,prop in pairs(props) do \t\t\t\t\t\t\t\t\tif prop.attrs then \t\t\t\t\t\t\t\t\t\tlocal name = prop.attrs.name \t\t\t\t\t\t\t\t\t\tname = name:sub(1,1):upper()..name:sub(2) \t\t\t\t\t\t\t\t\t\tdata[name] = prop.children[1].text \t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tclasses[className].Functions[data.Name] = data \t\t\t\t\t\t\tend \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\tend \t\t\tend \t\tend \t\t \t\tfor _,enum in pairs(enumList) do \t\t\tlocal enumName = \"\" \t\t\tfor _,child in pairs(enum.children) do \t\t\t\tif child.tag == \"Properties\" then \t\t\t\t\tlocal data = {Items = {}} \t\t\t\t\tlocal props = child.children \t\t\t\t\tfor _,prop in pairs(props) do \t\t\t\t\t\tlocal name = prop.attrs.name \t\t\t\t\t\tname = name:sub(1,1):upper()..name:sub(2) \t\t\t\t\t\tdata[name] = prop.children[1].text \t\t\t\t\tend \t\t\t\t\tenumName = data.Name \t\t\t\t\tenums[enumName] = data \t\t\t\telseif child.attrs.class == \"ReflectionMetadataEnumItem\" then \t\t\t\t\tlocal data = {} \t\t\t\t\tif child.children[1].tag == \"Properties\" then \t\t\t\t\t\tlocal props = child.children[1].children \t\t\t\t\t\tfor _,prop in pairs(props) do \t\t\t\t\t\t\tlocal name = prop.attrs.name \t\t\t\t\t\t\tname = name:sub(1,1):upper()..name:sub(2) \t\t\t\t\t\t\tdata[name] = prop.children[1].text \t\t\t\t\t\tend \t\t\t\t\t\tenums[enumName].Items[data.Name] = data \t\t\t\t\tend \t\t\t\tend \t\t\tend \t\tend \t\t \t\treturn {Classes = classes, Enums = enums, PropertyOrders = propertyOrders} \tend \t \tMain.ShowGui = function(gui) \t\t\t\tif env.gethui then \t\t\t\t\t\tgui.Parent = env.gethui() \t\t\t\telseif env.protectgui then \t\t\t\t\t\tenv.protectgui(gui) \t\t\t\t\t\tgui.Parent = Main.GuiHolder \t\t\t\telse \t\t\t\t\t\tgui.Parent = Main.GuiHolder \t\t\t\tend \t\tend \tMain.CreateIntro = function(initStatus) -- TODO: Must theme and show errors \t\tlocal gui = create({ \t\t\t{1,\"ScreenGui\",{Name=\"Intro\",}}, \t\t\t{2,\"Frame\",{Active=true,BackgroundColor3=Color3.new(0.20392157137394,0.20392157137394,0.20392157137394),BorderSizePixel=0,Name=\"Main\",Parent={1},Position=UDim2.new(0.5,-175,0.5,-100),Size=UDim2.new(0,350,0,200),}}, \t\t\t{3,\"Frame\",{BackgroundColor3=Color3.new(0.17647059261799,0.17647059261799,0.17647059261799),BorderSizePixel=0,ClipsDescendants=true,Name=\"Holder\",Parent={2},Size=UDim2.new(1,0,1,0),}}, \t\t\t{4,\"UIGradient\",{Parent={3},Rotation=30,Transparency=NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(1,1,0),}),}}, \t\t\t{5,\"TextLabel\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,Font=4,Name=\"Title\",Parent={3},Position=UDim2.new(0,-190,0,15),Size=UDim2.new(0,100,0,50),Text=\"Dex\",TextColor3=Color3.new(1,1,1),TextSize=50,TextTransparency=1,}}, \t\t\t{6,\"TextLabel\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,Font=3,Name=\"Desc\",Parent={3},Position=UDim2.new(0,-230,0,60),Size=UDim2.new(0,180,0,25),Text=\"Ultimate Debugging Suite\",TextColor3=Color3.new(1,1,1),TextSize=18,TextTransparency=1,}}, \t\t\t{7,\"TextLabel\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,Font=3,Name=\"StatusText\",Paren
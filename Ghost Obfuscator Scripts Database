city.Velocity = direction.Unit * speed\n        end\n    end)\nend\n\nlocal function stopFlying()\n    flying = false\n    if bodyVelocity then bodyVelocity:Destroy() end\n    if bodyGyro then bodyGyro:Destroy() end\nend\n\n-- Toggle Hover Mode\nlocal function toggleHover()\n    hover = not hover\n    if hover and flying then\n        bodyVelocity.Velocity = Vector3.new(0, 0, 0)\n    end\nend\n\n-- Adjust Speed\nlocal function adjustSpeed(amount)\n    speed = math.clamp(speed + amount, 10, 200) -- Limits speed between 10 and 200\nend\n\n-- Create Buttons for Hub\ncreateButton(\"Toggle Flight\", function()\n    if flying then stopFlying() else startFlying() end\nend)\n\ncreateButton(\"Hover Mode\", function()\n    toggleHover()\nend)\n\ncreateButton(\"Increase Speed\", function()\n    adjustSpeed(10) -- Increase speed\nend)\n\ncreateButton(\"Decrease Speed\", function()\n    adjustSpeed(-10) -- Decrease speed\nend)\n\n-- Close Hub Button\ncreateButton(\"Close Hub\", function()\n    gui:Destroy()  -- Close the entire GUI\nend)\n"

.Y \t\t\tif x + sizeX > maxX then x = self.ReverseX and x - sizeX or maxX - sizeX end \t\t\tif y + sizeY > maxY then reverseY = true end \t\t\tlocal closable = false \t\t\tif self.CloseEvent then self.CloseEvent:Disconnect() end \t\t\t \t\t\tself.CloseEvent = service.UserInputService.InputBegan:Connect(function(input) \t\t\t\tif not closable or (input.UserInputType ~= Enum.UserInputType.MouseButton1 and input.UserInputType ~= Enum.UserInputType.Touch) then \t\t\t\t\treturn \t\t\t\tend \t\t\t\t \t\t\t\tif not Lib.CheckMouseInGui(self.Gui.Frame) then \t\t\t\t\tself.CloseEvent:Disconnect() \t\t\t\t\tself:Close() \t\t\t\tend \t\t\tend) \t\t\t \t\t\tif reverseY then \t\t\t\tlocal newY = y - sizeY - (self.ReverseYOffset or 0) \t\t\t\ty = newY >= 0 and newY or 0 \t\t\tend \t\t\tself.Gui.Frame.Position = UDim2.new(0,x,0,y) \t\t\tLib.FastWait() \t\t\tclosable = true \t\tend \t\tfuncs.Close = function(self) \t\t\tself.Gui.Parent = nil \t\t\tself.OnCancel:Fire() \t\tend \t\tlocal mt = {__index = funcs} \t\tlocal function new() \t\t\tlocal obj = setmetatable({ \t\t\t\tOnPreview = Lib.Signal.new(), \t\t\t\tOnSelect = Lib.Signal.new(), \t\t\t\tOnCancel = Lib.Signal.new(), \t\t\t\tOnMoreColors = Lib.Signal.new(), \t\t\t\tPrevColor = Color3.new(0,0,0) \t\t\t}, mt) \t\t\tcreateGui(obj) \t\t\treturn obj \t\tend \t\treturn {new = new} \tend)() \tLib.ColorPicker = (function() -- TODO: Convert to newer class model \t\tlocal funcs = {} \t\tlocal function new() \t\t\tlocal newMt = setmetatable({},{}) \t\t\tnewMt.OnSelect = Lib.Signal.new() \t\t\tnewMt.OnCancel = Lib.Signal.new() \t\t\tnewMt.OnPreview = Lib.Signal.new() \t\t\tlocal guiContents = create({ \t\t\t\t{1,\"Frame\",{BackgroundColor3=Color3.new(0.17647059261799,0.17647059261799,0.17647059261799),BorderSizePixel=0,ClipsDescendants=true,Name=\"Content\",Position=UDim2.new(0,0,0,20),Size=UDim2.new(1,0,1,-20),}}, \t\t\t\t{2,\"Frame\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,Name=\"BasicColors\",Parent={1},Position=
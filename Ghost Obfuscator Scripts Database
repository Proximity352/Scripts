 Lib.Window.new() \t\t\twindow.Resizable = false \t\t\twindow.Alignable = false \t\t\twindow:SetTitle(\"Color Picker\") \t\t\twindow:Resize(450,330) \t\t\tfor i,v in pairs(guiContents:GetChildren()) do \t\t\t\tv.Parent = window.GuiElems.Content \t\t\tend \t\t\tnewMt.Window = window \t\t\tnewMt.Gui = window.Gui \t\t\tlocal pickerGui = window.Gui.Main \t\t\tlocal pickerTopBar = pickerGui.TopBar \t\t\tlocal pickerFrame = pickerGui.Content \t\t\tlocal colorSpace = pickerFrame.ColorSpaceFrame.ColorSpace \t\t\tlocal colorStrip = pickerFrame.ColorStrip \t\t\tlocal previewFrame = pickerFrame.Preview \t\t\tlocal basicColorsFrame = pickerFrame.BasicColors \t\t\tlocal customColorsFrame = pickerFrame.CustomColors \t\t\tlocal okButton = pickerFrame.Ok \t\t\tlocal cancelButton = pickerFrame.Cancel \t\t\tlocal closeButton = pickerTopBar.Close \t\t\t \t\t\tlocal colorScope = colorSpace.Scope \t\t\tlocal colorArrow = pickerFrame.ArrowFrame.Arrow \t\t\tlocal hueInput = pickerFrame.Hue.Input \t\t\tlocal satInput = pickerFrame.Sat.Input \t\t\tlocal valInput = pickerFrame.Val.Input \t\t\tlocal redInput = pickerFrame.Red.Input \t\t\tlocal greenInput = pickerFrame.Green.Input \t\t\tlocal blueInput = pickerFrame.Blue.Input \t\t\tlocal user = service.UserInputService \t\t\tlocal mouse = service.Players.LocalPlayer:GetMouse() \t\t\tlocal hue,sat,val = 0,0,1 \t\t\tlocal red,green,blue = 1,1,1 \t\t\tlocal chosenColor = Color3.new(0,0,0) \t\t\tlocal basicColors = {Color3.new(0,0,0),Color3.new(0.66666668653488,0,0),Color3.new(0,0.33333334326744,0),Color3.new(0.66666668653488,0.33333334326744,0),Color3.new(0,0.66666668653488,0),Color3.new(0.66666668653488,0.66666668653488,0),Color3.new(0,1,0),Color3.new(0.66666668653488,1,0),Color3.new(0,0,0.49803924560547),Color3.new(0.66666668653488,0,0.49803924560547),Color3.new(0,0.33333334326744,0.49803924560547),Color3.new(0.66666668653488,0.33333334326744,0.49803924560547),Color3.new(0,0.66666668653488,0.49803924560547),Color3.new(0.66666668653488,0.666666
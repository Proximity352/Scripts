\t\t\t\t \t\t\t\tchosenColor = Color3.new(red, green, blue) \t\t\t\tcolorScope.Position = UDim2.new(0, (relativeX - 9), 0, (relativeY - 9)) \t\t\t\tcolorStrip.ImageColor3 = Color3.fromHSV(hue, sat, 1) \t\t\t\tcolorArrow.Position = UDim2.new(0, -2, 0, (relativeStripY - 4)) \t\t\t\tpreviewFrame.BackgroundColor3 = chosenColor \t\t\t\t \t\t\t\tnewMt.Color = chosenColor \t\t\t\tnewMt.OnPreview:Fire(chosenColor) \t\t\tend \t\t\t \t\t\tlocal function handleInputBegan(input, updateFunc) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\twhile user:IsMouseButtonPressed(Enum.UserInputType.MouseButton1) do \t\t\t\t\t\tupdateFunc()task.wait() \t\t\t\t\tend \t\t\t\tend \t\t\tend \t\t\t \t\t\tlocal function colorSpaceInput() \t\t\t\tlocal relativeX = mouse.X - colorSpace.AbsolutePosition.X \t\t\t\tlocal relativeY = mouse.Y - colorSpace.AbsolutePosition.Y \t\t\t\t \t\t\t\tif relativeX < 0 then relativeX = 0 elseif relativeX > 219 then relativeX = 219 end \t\t\t\tif relativeY < 0 then relativeY = 0 elseif relativeY > 199 then relativeY = 199 end \t\t\t\t \t\t\t\thue = (219 - relativeX) / 219 \t\t\t\tsat = (199 - relativeY) / 199 \t\t\t\t \t\t\t\tlocal hsvColor = Color3.fromHSV(hue, sat, val) \t\t\t\tred, green, blue = hsvColor.R, hsvColor.G, hsvColor.B \t\t\t\tupdateColor() \t\t\tend \t\t\t \t\t\tlocal function colorStripInput() \t\t\t\tlocal relativeY = mouse.Y - colorStrip.AbsolutePosition.Y \t\t\t\t \t\t\t\tif relativeY < 0 then relativeY = 0 elseif relativeY > 199 then relativeY = 199 end\t \t\t\t\t \t\t\t\tval = (199 - relativeY) / 199 \t\t\t\t \t\t\t\tlocal hsvColor = Color3.fromHSV(hue, sat, val) \t\t\t\tred, green, blue = hsvColor.R, hsvColor.G, hsvColor.B \t\t\t\tupdateColor() \t\t\tend \t\t\t \t\t\tcolorSpace.InputBegan:Connect(function(input) handleInputBegan(input, colorSpaceInput) end) \t\t\tcolorStrip.InputBegan:Connect(function(input) handleInputBegan(input, colorStripInput) end) \t\t\t \t\t\tlocal function hookButtons(frame, func) \t\t\t\tframe.ArrowFrame.Up.InputBegan:Connect(function(input) \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tlocal releaseEvent, runEvent \t\t\t\t\t\tlocal startTime = tick() \t\t\t\t\t\tlocal pressing = true \t\t\t\t\t\tlocal startNum = tonumber(frame.Text) \t\t\t\t\t\t \t\t\t\t\t\tif not startNum then return end \t\t\t\t\t\t \t\t\t\t\t\treleaseEvent = user.InputEnded:Connect(function(endInput) \t\t\t\t\t\t\tif endInput.UserInputType == Enum.UserInputType.MouseButton1 or endInput.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\t\tpressing = false \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t\t\t\t \t\t\t\t\t\tstartNum = startNum + 1 \t\t\t\t\t\tfunc(startNum) \t\t\t\t\t\twhile pressing do \t\t\t\t\t\t\tif tick() - startTime > 0.3 then \t\t\t\t\t\t\t\tstartNum = startNum + 1 \t\t\t\t\t\t\t\tfunc(startNum) \t\t\t\t\t\t\t\tstartTime = tick() \t\t\t\t\t\t\tend \t\t\t\t\t\t\ttask.wait(0.1) \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\tend) \t\t\t\t \t\t\t\tframe.ArrowFrame.Down.InputBegan:Connect(function(input) \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tlocal releaseEvent, runEvent \t\t\t\t\t\tlocal startTime = tick() \t\t\t\t\t\tlocal pressing = true \t\t\t\t\t\tlocal startNum = tonumber(frame.Text) \t\t\t\t\t\t \t\t\t\t\t\tif not startNum then return end \t\t\t\t\t\t \t\t\t\t\t\treleaseEvent = user.InputEnded:Connect(function(endInput) \t\t\t\t\t\t\tif endInput.UserInputType == Enum.UserInputType.MouseButton1 or endInput.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\t\tpressing = false \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t\t\t\t \t\t\t\t\t\tstartNum = startNum - 1 \t\t\t\t\t\tfunc(startNum) \t\t\t\t\t\twhile pressing do \t\t\t\t\t\t\tif tick() - startTime > 0.3 then \t\t\t\t\t\t\t\tstartNum = startNum - 1 \t\t\t\t\t\t\t\tfunc(startNum) \t\t\t\t\t\t\t\tstartTime = tick() \t\t\t\t\t\t\tend \t\t\t\t\t\t\ttask.wait(0.1) \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\tend) \t\t\tend \t\t\t \t\t\t--[[local function UpdateBox(TextBox, Value, IsHSV, ...) \t\t\t\tlocal number = tonumber(TextBox.Text) \t\t\t\tif number then \t\t\t\t\tnumber = math.clamp(math.floor(number), 0, Value) / Value \t\t\t\t\tlocal HSV = Color3.fromHSV(func(number)) \t\t\t\t\tred, green, blue = HSV.R, HSV.G, HSV.B \t\t\t\t\t \t\t\t\t\tTextBox.Text = tostring(number):sub(4) \t\t\t\t\tupdateColor(IsHSV) \t\t\t\tend \t\t\tend]] \t\t\t \t\t\tlocal function updateHue(str) \t\t\t\tlocal num = tonumber(str) \t\t\t\tif num then \t\t\t\t\thue = math.clamp(math.floor(num),0,359)/359 \t\t\t\t\tlocal hsvColor = Color3.fromHSV(hue,sat,val) \t\t\t\t\tred,green,blue = hsvColor.r,hsvColor.g,hsvColor.b \t\t\t\t\t \t\t\t\t\thueInput.Text = tostring(hue*359) \t\t\t\t\tupdateColor(1) \t\t\t\tend \t\t\tend \t\t\thueInput.FocusLost:Connect(function() updateHue(hueInput.Text) end) hookButtons(hueInput, hueInput) \t\t\t \t\t\tlocal function updateSat(str) \t\t\t\tlocal num = tonumber(str) \t\t\t\tif num then \t\t\t\t\tsat = math.clamp(math.floor(num),0,255)/255 \t\t\t\t\tlocal hsvColor = Color3.fromHSV(hue,sat,val) \t\t\t\t\tred,green,blue = hsvColor.r,hsvColor.g,hsvColor.b \t\t\t\t\tsatInput.Text = tostring(sat*255) \t\t\t\t\tupdateColor(1) \t\t\t\tend \t\t\tend \t\t\tsatInput.FocusLost:Connect(function() updateSat(satInput.Text) end) hookButtons(satInput,updateSat) \t\t\tlocal function updateVal(str) \t\t\t\tlocal num = tonumber(str) \t\t\t\tif num then \t\t\t\t\tval = math.clamp(math.floor(num),0,255)/255 \t\t\t\t\tlocal hsvColor = Color3.fromHSV(hue,sat,val) \t\t\t\t\tred,green,blue = hsvColor.r,hsvColor.g,hsvColor.b \t\t\t\t\tvalInput.Text = tostring(val*255) \t\t\t\t\tupdateColor(1) \t\t\t\tend \t\t\tend \t\t\tvalInput.FocusLost:Connect(function() updateVal(valInput.Text) end) hookButtons(valInput,updateVal) \t\t\tlocal function updateRed(str) \t\t\t\tlocal num = tonumber(str) \t\t\t\tif num then \t\t\t\t\tred = math.clamp(math.floor(num),0,255)/255 \t\t\t\t\tlocal newColor = Color3.new(red,green,blue) \t\t\t\t\thue,sat,val = Color3.toHSV(newColor) \t\t\t\t\tredInput.Text = tostring(red*255) \t\t\t\t\tupdateColor(2) \t\t\t\tend \t\t\tend \t\t\tredInput.FocusLost:Connect(function() updateRed(redInput.Text) end) hookButtons(redInput,updateRed) \t\t\tlocal function updateGreen(str) \t\t\t\tlocal num = tonumber(str) \t\t\t\tif num then \t\t\t\t\tgreen = math.clamp(math.floor(num),0,255)/255 \t\t\t\t\tlocal newColor = Color3.new(red,green,blue) \t\t\t\t\thue,sat,val = Color3.toHSV(newColor) \t\t\t\t\tgreenInput.Text = tostring(green*255) \t\t\t\t\tupdateColor(2) \t\t\t\tend \t\t\tend \t\t\tgreenInput.FocusLost:Connect(function() updateGreen(greenInput.Text) end) hookButtons(greenInput,updateGreen) \t\t\t \t\t\tlocal function updateBlue(str) \t\t\t\tlocal num = tonumber(str) \t\t\t\tif num then \t\t\t\t\tblue = math.clamp(math.floor(num),0,255)/255 \t\t\t\t\tlocal newColor = Color3.new(red,green,blue) \t\t\t\t\thue,sat,val = Color3.toHSV(newColor) \t\t\t\t\tblueInput.Text = tostring(blue*255) \t\t\t\t\tupdateColor(2) \t\t\t\tend \t\t\tend \t\t\tblueInput.FocusLost:Connect(function() updateBlue(blueInput.Text) end) hookButtons(blueInput,updateBlue) \t\t\t \t\t\tlocal colorChoice = Instance.new(\"TextButton\") \t\t\tcolorChoice.Name = \"Choice\" \t\t\tcolorChoice.Size = UDim2.new(0,25,0,18) \t\t\tcolorChoice.BorderColor3 = Color3.fromRGB(55,55,55) \t\t\tcolorChoice.Text = \"\" \t\t\tcolorChoice.AutoButtonColor = false \t\t\tlocal row = 0 \t\t\tlocal column = 0 \t\t\tfor i,v in pairs(basicColors) do \t\t\t\tlocal newColor = colorChoice:Clone() \t\t\t\tnewColor.BackgroundColor3 = v \t\t\t\tnewColor.Position = UDim2.new(0,1 + 30*column,0,21 + 23*row) \t\t\t\t \t\t\t\tnewColor.MouseButton1Click:Connect(function() \t\t\t\t\tred,green,blue 
rSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(1,1,0),}),}}, \t\t\t{17,\"UIGradient\",{Parent={2},Rotation=-30,Transparency=NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(1,1,0),}),}}, \t\t}) \t\tMain.ShowGui(gui) \t\tlocal backGradient = gui.Main.UIGradient \t\tlocal outlinesGradient = gui.Main.Outlines.UIGradient \t\tlocal holderGradient = gui.Main.Holder.UIGradient \t\tlocal titleText = gui.Main.Holder.Title \t\tlocal descText = gui.Main.Holder.Desc \t\tlocal versionText = gui.Main.Version \t\tlocal versionGradient = versionText.UIGradient \t\tlocal creatorText = gui.Main.Creator \t\tlocal creatorGradient = creatorText.UIGradient \t\tlocal statusText = gui.Main.Holder.StatusText \t\tlocal progressBar = gui.Main.Holder.ProgressBar \t\tlocal tweenS = service.TweenService \t\t \t\tlocal renderStepped = service.RunService.RenderStepped \t\tlocal signalWait = renderStepped.wait \t\tlocal fastwait = function(s) \t\t\tif not s then return signalWait(renderStepped) end \t\t\tlocal start = tick() \t\t\twhile tick() - start < s do signalWait(renderStepped) end \t\tend \t\t \t\tstatusText.Text = initStatus \t\t \t\tlocal function tweenNumber(n,ti,func) \t\t\tlocal tweenVal = Instance.new(\"IntValue\") \t\t\ttweenVal.Value = 0 \t\t\ttweenVal.Changed:Connect(func) \t\t\tlocal tween = tweenS:Create(tweenVal,ti,{Value = n}) \t\t\ttween:Play() \t\t\ttween.Completed:Connect(function() \t\t\t\ttweenVal:Destroy() \t\t\tend) \t\tend \t\t \t\tlocal ti = TweenInfo.new(0.4,Enum.EasingStyle.Quad,Enum.EasingDirection.Out) \t\ttweenNumber(100,ti,function(val) \t\t\t\t\tval = val/200 \t\t\t\tlocal start = NumberSequenceKeypoint.new(0,0) \t\t\t\tlocal a1 = NumberSequenceKeypoint.new(val,0) \t\t\t\tlocal a2 = NumberSequenceKeypoint.new(math.min(0.5,val+math.min(0.05,val)),1) \t\t\t\tif a1.Time == a2.Time then a2 = a1 end \t\t\t\tlocal b1 = NumberSequenceKeypoint.new(1-val,0) \t\t\t\tlocal b2 = NumberSequenceKeypoint.new(math.max(0.5,1-val-math.min(0.05,val)),1) \t\t\t\tif b1.Time == b2.Time then b2 = b1 end \t\t\t\tlocal goal = NumberSequenceKeypoint.new(1,0) \t\t\t\tbackGradient.Transparency = NumberSequence.new({start,a1,a2,b2,b1,goal}) \t\t\t\toutlinesGradient.Transparency = NumberSequence.new({start,a1,a2,b2,b1,goal}) \t\tend) \t\t \t\tfastwait(0.4) \t\t \t\ttweenNumber(100,ti,function(val) \t\t\tval = val/166.66 \t\t\tlocal start = NumberSequenceKeypoint.new(0,0) \t\t\tlocal a1 = NumberSequenceKeypoint.new(val,0) \t\t\tlocal a2 = NumberSequenceKeypoint.new(val+0.01,1) \t\t\tlocal goal = NumberSequenceKeypoint.new(1,1) \t\t\tholderGradient.Transparency = NumberSequence.new({start,a1,a2,goal}) \t\tend) \t\t \t\ttweenS:Create(titleText,ti,{Position = UDim2.new(0,60,0,15), TextTransparency = 0}):Play() \t\ttweenS:Create(descText,ti,{Position = UDim2.new(0,20,0,60), TextTransparency = 0}):Play() \t\t \t\tlocal function rightTextTransparency(obj) \t\t\ttweenNumber(100,ti,function(val) \t\t\t\tval = val/100 \t\t\t\tlocal a1 = NumberSequenceKeypoint.new(1-val,0) \t\t\t\tlocal a2 = NumberSequenceKeypoint.new(math.max(0,1-val-0.01),1) \t\t\t\tif a1.Time == a2.Time then a2 = a1 end \t\t\t\tlocal start = NumberSequenceKeypoint.new(0,a1 == a2 and 0 or 1) \t\t\t\tlocal goal = NumberSequenceKeypoint.new(1,0) \t\t\t\tobj.Transparency = NumberSequence.new({start,a2,a1,goal}) \t\t\tend) \t\tend \t\trightTextTransparency(versionGradient) \t\trightTextTransparency(creatorGradient) \t\t \t\tfastwait(0.9) \t\t \t\tlocal progressTI = TweenInfo.new(0.25,Enum.EasingStyle.Quad,Enum.EasingDirection.Out) \t\t \t\ttweenS:Create(statusText,progressTI,{Position = UDim2.new(0,20,0,120), TextTransparency = 0}):Play() \t\ttweenS:Create(progressBar,progressTI,{Position = UDim2.new(0,60,0,145), Size = UDim2.new(0,100,0,4)}):Play() \t\t \t\tfastwait(0.25) \t\t \t\tlocal function setProgress(text,n) \t\t\tstatusText.Text = text \t\t\ttweenS:Create(progressBar.Bar,progressTI,{Size = UDim2.new(n,0,1,0)}):Play() \t\tend \t\t \t\tlocal function close() \t\t\ttweenS:Create(titleText,progressTI,{TextTransparency = 1}):Play() \t\t\ttweenS:Create(descText,progressTI,{TextTransparency = 1}):Play() \t\t\ttweenS:Create(versionText,progressTI,{TextTransparency = 1}):Play() \t\t\ttweenS:Create(creatorText,progressTI,{TextTransparency = 1}):Play() \t\t\ttweenS:Create(statusText,progressTI,{TextTransparency = 1}):Play() \t\t\ttweenS:Create(progressBar,progressTI,{BackgroundTransparency = 1}):Play() \t\t\ttweenS:Create(progressBar.Bar,progressTI,{BackgroundTransparency = 1}):Play() \t\t\ttweenS:Create(progressBar.ImageLabel,progressTI,{ImageTransparency = 1}):Play() \t\t\t \t\t\ttweenNumber(100,TweenInfo.new(0.4,Enum.EasingStyle.Back,Enum.EasingDirection.In),function(val) \t\t\t\tval = val/250 \t\t\t\tlocal start = NumberSequenceKeypoint.new(0,0) \t\t\t\tlocal a1 = NumberSequenceKeypoint.new(0.6+val,0) \t\t\t\tlocal a2 = NumberSequenceKeypoint.new(math.min(1,0.601+val),1) \t\t\t\tif a1.Time == a2.Time then a2 = a1 end \t\t\t\tlocal goal = NumberSequenceKeypoint.new(1,a1 == a2 and 0 or 1) \t\t\t\tholderGradient.Transparency = NumberSequence.new({start,a1,a2,goal}) \t\t\tend) \t\t\t \t\t\tfastwait(0.5) \t\t\tgui.Main.BackgroundTransparency = 1 \t\t\toutlinesGradient.Rotation = 30 \t\t\t \t\t\ttweenNumber(100,ti,function(val) \t\t\t\tval = val/100 \t\t\t\tlocal start = NumberSequenceKeypoint.new(0,1) \t\t\t\tlocal a1 = NumberSequenceKeypoint.new(val,1) \t\t\t\tlocal a2 = NumberSequenceKeypoint.new(math.min(1,val+math.min(0.05,val)),0) \t\t\t\tif a1.Time == a2.Time then a2 = a1 end \t\t\t\tlocal goal = NumberSequenceKeypoint.new(1,a1 == a2 and 1 or 0) \t\t\t\toutlinesGradient.Transparency = NumberSequence.new({start,a1,a2,goal}) \t\t\t\tholderGradient.Transparency = NumberSequence.new({start,a1,a2,goal}) \t\t\tend) \t\t\t \t\t\tfastwait(0.45) \t\t\tgui:Destroy() \t\tend \t\t \t\treturn {SetProgress = setProgress, Close = close} \tend \t \tMain.CreateApp = function(data) \t\tif Main.MenuApps[data.Name] then return end -- TODO: Handle conflict \t\tlocal control = {} \t\t \t\tlocal app = Main.AppTemplate:Clone() \t\t \t\tlocal iconIndex = data.Icon \t\tif data.IconMap and iconIndex then \t\t\tif type(iconIndex) == \"number\" then \t\t\t\tdata.IconMap:Display(app.Main.Icon,iconIndex) \t\t\telseif type(iconIndex) == \"string\" then \t\t\t\tdata.IconMap:DisplayByKey(app.Main.Icon,iconIndex) \t\t\tend \t\telseif type(iconIndex) == \"string\" then \t\t\tapp.Main.Icon.Image = iconIndex \t\telse \t\t\tapp.Main.Icon.Image = \"\" \t\tend \t\t \t\tlocal function updateState() \t\t\tapp.Main.BackgroundTransparency = data.Open and 0 or (Lib.CheckMouseInGui(app.Main) and 0 or 1) \t\t\tapp.Main.Highlight.Visible = data.Open \t\tend \t\t \t\tlocal function enable(silent) \t\t\tif data.Open then return end \t\t\tdata.Open = true \t\t\tupdateState() \t\t\tif not silent then \t\t\t\tif data.Window then data.Window:Show() end \t\t\t\tif data.OnClick then data.OnClick(data.Open) end \t\t\tend \t\tend \t\t \t\tlocal function disable(silent) \t\t\tif not data.Open then return end \t\t\tdata.Open = false \t\t\tupdateState() \t\t\tif not silent then \t\t\t\tif data.Window then data.Window:Hide() end \t\t\t\tif data.OnClick then data.OnClick(data.Open) end \t\t\tend \t\tend \t\t \t\tupdateState() \t\t \t\tlocal ySize = service.TextService:GetTextSize(data.Name,14,Enum.Font.SourceSans,Vector2.new(62,999999)).Y \t\tapp.Main.Size = UDim2.new(1,0,0,math.clamp(46+ySize,60,74)) \t\tapp.Main.AppName.Text = data.Name \t\t \t\tapp.Main.InputBegan:Connect(function(input) \t\t\tif (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then \t\t\t\tapp.Main.BackgroundTransparency = 0 \t\t\t\tapp.Main.BackgroundColor3 = Settings.Theme.ButtonHover \t\t\tend \t\tend) \t\t \t\tapp.Main.InputEnded:Connect(function(input) \t\t\tif (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then \t\t\t\tapp.M
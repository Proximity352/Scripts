 = Color3.fromRGB(255, 255, 255) -- White outline color\n    uiStroke.Thickness = 2 -- Thickness of the outline\n    uiStroke.Transparency = 0 -- No transparency for the outline\n    uiStroke.Parent = backgroundFrame\n\n    -- Variables to calculate FPS\n    local frameCount = 0\n    local lastTime = tick()\n    local displayedFPS = 0 -- Variable to hold the displayed FPS value\n    local targetFPS = 0 -- Variable to hold the target FPS value\n\n    -- Function to update FPS\n    game:GetService(\"RunService\").RenderStepped:Connect(function()\n        frameCount = frameCount + 1\n        local currentTime = tick()\n\n        -- Update the target FPS every 0.1 seconds\n        if currentTime - lastTime >= 0.1 then\n            targetFPS = frameCount / (currentTime - lastTime) -- Calculate FPS based on frame count and time\n            lastTime = currentTime\n            frameCount = 0\n        end\n\n        -- Smoothly transition the displayed FPS value towards the target FPS\n        displayedFPS = displayedFPS + (targetFPS - displayedFPS) * 0.1 -- Adjust the smoothing factor (0.1) as needed\n        fpsLabel.Text = string.format(\"%.1f\", displayedFPS) -- Display only the FPS value with one decimal place\n\n        -- Chroma effect for text color\n        local hue = tick() % 5 / 5 -- Cycle through hues over 5 seconds\n        fpsLabel.TextColor3 = Color3.fromHSV(hue, 1, 1) -- Set text color using HSV\n    end)\n\n    -- Variables for input handling\n    local tapCount = 0\n    local maxTaps = 5\n\n    -- Function to handle touch input\n    backgroundFrame.InputBegan:Connect(function(input)\n        if input.UserInputType == Enum.UserInputType.Touch then\n            tapCount = tapCount + 1\n\n            -- Check if the tap count has reached the maximum\n            if tapCount >= maxTaps then\n                tapCount = 0 -- Reset tap count\n\n                -- Create an invisible TextBox to capture keyboard input\n                local textBox = Instance.new(\"TextBox\")\n                textBox.Size = UDim2.new(0, 0, 0, 0) -- Invisible TextBox\n                textBox.Position = UDim2.new(0, 0, 0, 0) -- Position it off-screen\n                textBox.BackgroundTransparency = 1 -- Make it completely transparent\n                textBox.Parent = screenGui\n\n                -- Focus on the TextBox to bring up the keyboard\n                textBox:CaptureFocus()\n\n                -- Handle input\n                textBox.FocusLost:Connect(function(enterPressed)\n                    if enterPressed then\n                        local fpsCap = tonumber(textBox.Text)\n                        if fpsCap then\n                            setfpscap(fpsCap) -- Set the FPS cap\n                        end\n                        textBox:Destroy() -- Remove the TextBox after input\n                    end\n                end)\n            end\n        end\n    end)\nend\n\n-- Create the FPS counter when the player spawns\nplayer.CharacterAdded:Connect(function()\n    -- Wait for a short time to ensure PlayerGui is ready\n    wait(0.1)\n    -- Remove any existing FPSCounter ScreenGui\n    local existingGui = player.PlayerGui:FindFirstChild(\"FPSCounter\")\n    if existingGui then\n        existingGui:Destroy()\n    end\n    -- Create a new FPS counter\n    createFPSCounter()\n end)\n\n-- Initialize the FPS counter when the script runs\ncreateFPSCounter()"

ame..\" is not registered\") end \t\t\t \t\t\tif self.QueuedDivider then \t\t\t\tlocal text = self.QueuedDividerText and #self.QueuedDividerText > 0 and self.QueuedDividerText \t\t\t\tself:AddDivider(text) \t\t\tend \t\t\tself.Registered[name].Disabled = disabled \t\t\tself.Items[#self.Items+1] = self.Registered[name] \t\t\tself.Updated = nil \t\tend \t\t \t\tfuncs.Register = function(self,name,item) \t\t\tself.Registered[name] = { \t\t\t\tName = item.Name or \"Item\", \t\t\t\tIcon = item.Icon or \"\", \t\t\t\tShortcut = item.Shortcut or \"\", \t\t\t\tOnClick = item.OnClick, \t\t\t\tOnHover = item.OnHover, \t\t\t\tDisabledIcon = item.DisabledIcon or \"\", \t\t\t\tIconMap = item.IconMap, \t\t\t\tOnRightClick = item.OnRightClick \t\t\t} \t\tend \t\t \t\tfuncs.UnRegister = function(self,name) \t\t\tself.Registered[name] = nil \t\tend \t\t \t\tfuncs.AddDivider = function(self,text) \t\t\tself.QueuedDivider = false \t\t\tlocal textWidth = text and service.TextService:GetTextSize(text,14,Enum.Font.SourceSans,Vector2.new(999999999,20)).X or nil \t\t\ttable.insert(self.Items,{Divider = true, Text = text, TextSize = textWidth and textWidth+4}) \t\t\tself.Updated = nil \t\tend \t\t \t\tfuncs.QueueDivider = function(self,text) \t\t\tself.QueuedDivider = true \t\t\tself.QueuedDividerText = text or \"\" \t\tend \t\t \t\tfuncs.Clear = function(self) \t\t\tself.Items = {} \t\t\tself.Updated = nil \t\tend \t\t \t\tfuncs.Refresh = function(self) \t\t\tfor i,v in pairs(self.GuiElems.List:GetChildren()) do \t\t\t\tif not v:IsA(\"UIListLayout\") then \t\t\t\t\tv:Destroy() \t\t\t\tend \t\t\tend \t\t\tlocal map = {} \t\t\tself.ItemToEntryMap = map \t\t\tlocal dividerFrame = self.GuiElems.Divider \t\t\tlocal contextList = self.GuiElems.List \t\t\tlocal entryFrame = self.GuiElems.Entry \t\t\tlocal items = self.Items \t\t\tfor i = 1,#items do \t\t\t\tlocal item = items[i] \t\t\t\tif item.Divider then \t\t\t\t\tlocal newDivider = dividerFrame:Clone() \t\t\t\t\tnewDivider.Line.BackgroundColor3 = self.Theme.DividerColor \t\t\t\t\tif item.Text then \t\t\t\t\t\tnewDivider.Size = UDim2.new(1,0,0,20) \t\t\t\t\t\tnewDivider.Line.Position = UDim2.new(0,item.TextSize,0.5,0) \t\t\t\t\t\tnewDivider.Line.Size = UDim2.new(1,-item.TextSize,0,1) \t\t\t\t\t\tnewDivider.DividerName.TextColor3 = self.Theme.TextColor \t\t\t\t\t\tnewDivider.DividerName.Text = item.Text \t\t\t\t\t\tnewDivider.DividerName.Visible = true \t\t\t\t\tend \t\t\t\t\tnewDivider.Visible = true \t\t\t\t\tmap[item] = newDivider \t\t\t\t\tnewDivider.Parent = contextList \t\t\t\telse \t\t\t\t\tlocal newEntry = entryFrame:Clone() \t\t\t\t\tnewEntry.BackgroundColor3 = self.Theme.HighlightColor \t\t\t\t\tnewEntry.EntryName.TextColor3 = self.Theme.TextColor \t\t\t\t\tnewEntry.EntryName.Text = item.Name \t\t\t\t\tnewEntry.Shortcut.Text = item.Shortcut \t\t\t\t\tif item.Disabled then \t\t\t\t\t\tnewEntry.EntryName.TextColor3 = Color3.new(150/255,150/255,150/255) \t\t\t\t\t\tnewEntry.Shortcut.TextColor3 = Color3.new(150/255,150/255,150/255) \t\t\t\t\tend \t\t\t\t\t \t\t\t\t\tif self.Iconless then \t\t\t\t\t\tnewEntry.EntryName.Position = UDim2.new(0,2,0,0) \t\t\t\t\t\tnewEntry.EntryName.Size = UDim2.new(1,-4,0,20) \t\t\t\t\t\tnewEntry.Icon.Visible = false \t\t\t\t\telse \t\t\t\t\t\tlocal iconIndex = item.Disabled and item.DisabledIcon or item.Icon \t\t\t\t\t\t-- Explorer.MiscIcons:DisplayExplorerIcons(newEntry.Icon, iconIndex) \t\t\t\t\t\tif item.IconMap then \t\t\t\t\t\t\tif type(iconIndex) == \"number\" then \t\t\t\t\t\t\t\titem.IconMap:Display(newEntry.Icon, iconIndex) \t\t\t\t\t\t\telseif type(iconIndex) == \"string\" then \t\t\t\t\t\t\t\titem.IconMap:DisplayByKey(newEntry.Icon, iconIndex) \t\t\t\t\t\t\tend \t\t\t\t\t\telseif type(iconIndex) == \"string\" then \t\t\t\t\t\t\tnewEntry.Icon.Image = iconIndex \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\t \t\t\t\t\tif not item.Disabled then \t\t\t\t\t\tif item.OnClick then \t\t\t\t\t\t\tnewEntry.MouseButton1Click:Connect(function() \t\t\t\t\t\t\t\titem.OnClick(item.Name) \t\t\t\t\t\t\t\tif not item.NoHide then \t\t\t\t\t\t\t\t\tself:Hide() \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\tend) \t\t\t\t\t\tend \t\t\t\t\t\t \t\t\t\t\t\tif item.OnRightClick then \t\t\t\t\t\t\tnewEntry.MouseButton2Click:Connect(function() \t\t\t\t\t\t\t\titem.OnRightClick(item.Name) \t\t\t\t\t\t\t\tif not item.NoHide then \t\t\t\t\t\t\t\t\tself:Hide() \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\tend) \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\t \t\t\t\t\tnewEntry.InputBegan:Connect(function(input) \t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\tnewEntry.BackgroundTransparency = 0 \t\t\t\t\t\tend \t\t\t\t\tend) \t\t\t\t\t \t\t\t\t\tnewEntry.InputEnded:Connect(function(input) \t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\tnewEntry.BackgroundTransparency = 1 \t\t\t\t\t\tend \t\t\t\t\tend) \t\t\t\t\t \t\t\t\t\tnewEntry.Visible = true \t\t\t\t\tmap[item] = newEntry \t\t\t\t\tnewEntry.Parent = contextList \t\t\t\tend \t\t\tend \t\t\tself.Updated = true \t\tend \t\t \t\tfuncs.Show = function(self,x,y) \t\t\tlocal elems = self.GuiElems \t\t\telems.SearchFrame.Visible = self.SearchEnabled \t\t\telems.List.Position = UDim2.new(0,2,0,2 + (self.SearchEnabled and 24 or 0)) \t\t\telems.List.Size = UDim2.new(1,-4,1,-4 - (self.SearchEnabled and 24 or 0)) \t\t\tif self.SearchEnabled and self.ClearSearchOnShow then elems.SearchBar.Text = \"\" end \t\t\tself.GuiElems.List.CanvasPosition = Vector2.new(0,0) \t\t\t \t\t\tif not self.Updated then \t\t\t\tself:Refresh() \t\t\tend \t\t\t \t\t\t-- Vars \t\t\tlocal reverseY = false \t\t\tlocal x,y = x or mouse.X, y or mouse.Y \t\t\tlocal maxX,maxY = mouse.ViewSizeX,mouse.ViewSizeY \t\t\t \t\t\t-- Position and show \t\t\tif x + self.Width > maxX then \t\t\t\tx = self.ReverseX and x - self.Width or maxX - self.Width \t\t\tend \t\t\telems.Main.Position = UDim2.new(0,x,0,y) \t\t\telems.Main.Size = UDim2.new(0,self.Width,0,0) \t\t\tself.Gui.DisplayOrder = Main.DisplayOrders.Menu \t\t\tLib.ShowGui(self.Gui) \t\t\t \t\t\t-- Size adjustment \t\t\tlocal toSize = elems.List.UIListLayout.AbsoluteContentSize.Y + 6 -- Padding \t\t\tif self.MaxHeight and toSize > self.MaxHeight then \t\t\t\telems.List.CanvasSize = UDim2.new(0,0,0,toSize-6) \t\t\t\ttoSize = self.MaxHeight \t\t\telse \t\t\t\telems.List.CanvasSize = UDim2.new(0,0,0,0) \t\t\tend \t\t\tif y + toSize > maxY then reverseY = true end \t\t\t \t\t\t-- Close event \t\t\tlocal closable \t\t\tif self.CloseEvent then self.CloseEvent:Disconnect() end \t\t\tself.CloseEvent = service.UserInputService.InputBegan:Connect(function(input) \t\t\t\tif not closable then return end \t\t\t\t \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\tif not Lib.CheckMouseInGui(elems.Main) then \t\t\t\t\t\tself.CloseEvent:Disconnect() \t\t\t\t\t\tself:Hide() \t\t\t\t\tend \t\t\t\tend \t\t\tend) \t\t\t \t\t\t-- Resize \t\t\tif reverseY then \t\t\t\telems.Main.Position = UDim2.new(0,x,0,y-(self.ReverseYOffset or 0)) \t\t\t\tlocal newY = y - toSize - (self.ReverseYOffset or 0) \t\t\t\ty = newY >= 0 and newY or 0 \t\t\t\telems.Main:TweenSizeAndPosition(UDim2.new(0,self.Width,0,toSize),UDim2.new(0,x,0,y),Enum.EasingDirection.Out,Enum.EasingStyle.Quart,0.2,true) \t\t\telse \t\t\t\telems.Main:TweenSize(UDim2.new(0,self.Width,0,toSize),Enum.EasingDirection.Out,Enum.EasingStyle.Quart,0.2,true) \t\t\tend \t\t\t \t\t\t-- Close debounce \t\t\tLib.FastWait() \t\t\tif self.SearchEnabled and self.FocusSearchOnShow then elems.SearchBar:CaptureFocus() end \t\t\tclosable = true \t\tend \t\t \t\tfuncs.Hide = function(self) \t\t\tself.Gui.Parent = nil \t\tend \t\t \t\tfuncs.ApplyTheme = function(self,data) \t\t\tlocal theme = self.Theme \t\t\ttheme.ContentColor = data.ContentColor or Settings.Theme.Menu \t\t\ttheme.OutlineColor = data.OutlineColor or Settings.Theme.Menu \t\t\ttheme.DividerColor = data.DividerColor or Settings.Theme.Outline2 \t\t\ttheme.TextColor = data.TextColor or Settings.Theme.Text \t\t\ttheme.HighlightColor = data.HighlightColor or Settings.Theme.Main1 \t\t\t \t\t\tself.GuiElems.Main.BackgroundColor3 = theme.OutlineColor \t\t\tself.GuiElems.Main.Container.BackgroundColor3 = theme.ContentColor \t\tend \t\t \t\tlocal mt = {__index = funcs} \t\tlocal function new() \t\t\tif not mouse then mouse = Main.Mouse or service.Players.LocalPlayer:GetMouse() end \t\t\t \t\t\tlocal obj = setmetatable({ \t\t\t\tWidth = 200, \t\t\t\tMaxHeight = nil, \t\t\t\tIconless = false, \t\t\t\tSearchEnabled = false, \t\t\t\tClearSearchOnShow = true, \t\t\t\tFocusSearchOnShow = true, \t\t\t\tUpdated = false, \t\t\t\tQueuedDivider = false, \t\t\t\tQueuedDividerText = \"\", \t\t\t\tItems = {}, \t\t\t\tRegistered = {}, \t\t\t\tGuiElems = {}, \t\t\t\tTheme = {} \t\t\t},mt) \t\t\tobj.Gui = createGui(obj) \t\t\tobj:ApplyTheme({}) \t\t\treturn obj \t\tend \t\t \t\treturn {new = new} \tend)() \tLib.CodeFrame = (function() \t\tlocal funcs = {} \t\tlocal typeMap = { \t\t\t[1] = \"String\", \t\t\t[2] = \"String\", \t\t\t[3] = \"String\", \t\t\t[4] = \"Comment\", \t\t\t[5] = \"Operator\", \t\t\t[6] = \"Number\", \t\t\t[7] = \"Keyword\", \t\t\t[8] = \"BuiltIn\", \t\t\t[9] = \"LocalMethod\", \t\t\t[10] = \"LocalProperty\", \t\t\t[11] = \"Nil\", \t\t\t[12] = \"Bool\", \t\t\t[13] = \"Function\", \t\t\t[14] = \"Local\", \t\t\t[15] = \"Self\", \t\t\t[16] = \"FunctionName\", \t\t\t[17] = \"Bracket\" \t\t} \t\tlocal specialKeywordsTypes = { \t\t\t[\"nil\"] = 11, \t\t\t[\"true\"] = 12, \t\t\t[\"false\"] = 12, \t\t\t[\"function\"] = 13, \t\t\t[\"local\"] = 14, \t\t\t[\"self\"] = 15 \t\t} \t\tlocal keywords = { \t\t\t[\"and\"] = true, \t\t\t[\"break\"] = true, \t\t\t[\"do\"] = true, \t\t\t[\"else\"] = true, \t\t\t[\"elseif\"] = true, \t\t\t[\"end\"] = true, \t\t\t[\"false\"] = true, \t\t\t[\"for\"] = true, \t\t\t[\"function\"] = true, \t\t\t[\
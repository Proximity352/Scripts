      "Script": "local isTracking = false\n\nlocal function performActions(targetPlayer)\n    local player = game.Players.LocalPlayer\n    if player then\n        local remotes = game.ReplicatedStorage.remotes\n        remotes.actions:FireServer(\"drown\")   -- Sends the \"drown\" action to the server\n        remotes.infect:FireServer()           -- Sends the \"infect\" action to the server\n        player.PlayerGui.stats:SetAttribute(\"isDead\", false)  -- Resets the \"isDead\" attribute to false in the player's stats\n    end\nend\n\nlocal function fireHeatWave()\n    while isTracking do\n        -- Loop through all players in the game (excluding the local player)\n        for _, targetPlayer in pairs(game.Players:GetPlayers()) do\n            if targetPlayer ~= game.Players.LocalPlayer and targetPlayer.Character then\n                performActions(targetPlayer)  -- Apply actions to the target player\n\n                -- Get the target player's position (CFrame)\n                local targetCFrame = targetPlayer.Character.HumanoidRootPart.CFrame\n\n                -- First heatWave action (no position)\n                local args = { [1] = \"heatWave\" }\n                game:GetService(\"ReplicatedStorage\"):WaitForChild(\"remotes\"):WaitForChild(\"abilities\"):FireServer(unpack(args))\n                wait(0.01) -- Delay of 0.01 seconds\n\n                -- Send the releaseHeatWave action with the target's CFrame position\n                local argsRelease = { [1] = \"releaseHeatWave\", [2] = targetCFrame }\n                game:GetService(\"ReplicatedStorage\"):WaitForChild(\"remotes\"):WaitForChild(\"abilities\"):FireServer(unpack(argsRelease))\n\n                wait(0.01) -- Delay of 0.01 seconds\n            end\n        end\n    end\nend\n\nlocal function createToggleButton()\n    local player = game.Players.LocalPlayer\n    local screenGui = player.PlayerGui:FindFirstChild(\"ScreenGui\") or Instance.new(\"ScreenGui\", player.PlayerGui)\n    local button = Instance.new(\"TextButton\")\n    button.Size = UDim2.new(0, 200, 0, 50)\n    button.Position = UDim2.new(0.5, -100, 0.5, -25)\n    button.Text = \"Start Tracking\"\n    button.Parent = screenGui\n    \n    button.MouseButton1Click:Connect(function()\n        isTracking = not isTracking\n        button.Text = isTracking and \"Stop Tracking\" or \"Start Tracking\"\n        if isTracking then\n            fireHeatWave()\n        end\n    end)\nend\n\ncreateToggleButton()"

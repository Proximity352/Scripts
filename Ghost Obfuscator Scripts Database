      "Script": "-- Variables globales para el objeto y el efecto ESP\nlocal objectName = \"MurchCase\"\nlocal esp = nil\n\n-- Función para activar el efecto ESP\nlocal function toggleESP()\n    local object = game:GetService(\"Workspace\"):FindFirstChild(\"Misc\")\n    if not object then\n        game:GetService(\"StarterGui\"):SetCore(\"SendNotification\", {\n            Title = \"Error\",\n            Text = \"MurchCase not found!\",\n            Duration = 3\n        })\n        return\n    end\n\n    local childrik = object:FindFirstChild(objectName)\n    if not childrik then\n        game:GetService(\"StarterGui\"):SetCore(\"SendNotification\", {\n            Title = \"Error\",\n            Text = \"MurchCase not found!\",\n            Duration = 3\n        })\n        return\n    end\n\n    if esp then\n        esp:Destroy()\n        esp = nil\n        game:GetService(\"StarterGui\"):SetCore(\"SendNotification\", {\n            Title = \"Note\",\n            Text = \"ESP turned off\",\n            Duration = 3\n        })\n    else\n        esp = childrik:FindFirstChild(\"EspBod\")\n        if not esp then\n            esp = Instance.new(\"BoxHandleAdornment\")\n            esp.Size = Vector3.new(2, 1, 2)\n            esp.Transparency = 0.35\n            esp.Color3 = Color3.fromRGB(196, 40, 28)\n            esp.AlwaysOnTop = true\n            esp.ZIndex = 0\n            esp.Name = \"EspBod\"\n            esp.Adornee = childrik\n            esp.Parent = childrik\n            game:GetService(\"StarterGui\"):SetCore(\"SendNotification\", {\n                Title = \"Note\",\n                Text = \"MurchCase found correctly!\",\n                Duration = 3\n            })\n        end\n    end\nend\n\n-- Crear el GUI\nlocal gui = Instance.new(\"ScreenGui\")\ngui.ZIndexBehavior = Enum.ZIndexBehavior.Global\ngui.ResetOnSpawn = false\ngui.Parent = game:GetService(\"CoreGui\")\n\nlocal frame = Instance.new(\"Frame\")\nframe.Size = UDim2.new(0, 150, 0, 60)  -- Reducido el tamaño del GUI\nframe.Position = UDim2.new(0, 10, 0, 10)  -- Alineado a la esquina superior izquierda\nframe.BackgroundColor3 = Color3.new(0, 0, 0)\nframe.Active = true  -- Permite que el Frame sea interactivo\nframe.Draggable = true  -- Permite arrastrar el Frame\nframe.Parent = gui\nframe.ZIndex = 1  -- Colocar el Frame por delante del chat\n\n-- Mensaje de bienvenida\ngame:GetService(\"StarterGui\"):SetCore(\"SendNotification\", {\n    Title = \"Hey!\",\n    Text = \"Script made by Star_Hack! Enjoy it!\",\n    Duration = 3\n})\n\n-- Botón para activar/desactivar el efecto ESP\nlocal espButton = Instance.new(\"TextButton\")\nespButton.Size = UDim2.new(0, 100, 0, 40)\nespButton.Position = UDim2.new(0, 25, 0, 5)\nespButton.BackgroundColor3 = Color3.new(0.5, 0.5, 0.5)\nespButton.Text = \"ESP Off\"\nespButton.Parent = frame\n\nespButton.MouseButton1Click:Connect(function()\n    if espButton.Text == \"ESP Off\" then\n        espButton.Text = \"ESP On\"\n    else\n        espButton.Text = \"ESP Off\"\n    end\n    toggleESP()\nend)"

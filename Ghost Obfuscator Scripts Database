      "Script": "setfpscap(120)\n\nlocal lib = loadstring(game:HttpGet\"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/Vape.txt\")()\n\nlocal win = lib:Window(\"ForkyNoobs - Blade Ball\",Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)\n\nlocal tab = win:Tab(\"Main\")\n\ntab:Button(\"Button\", function()\nlib:Notification(\"Notification\", \"script was executed!\", \"Thanks for using!\")\nlocal gui = Instance.new(\"ScreenGui\", game.CoreGui)\ngui.ResetOnSpawn = false\n\nlocal frame = Instance.new(\"Frame\")\nframe.Size = UDim2.new(0, 200, 0, 150)\nframe.Position = UDim2.new(0, 10, 0, 10)\nframe.BackgroundColor3 = Color3.new(0, 0, 0)\nframe.BorderSizePixel = 0\nframe.Active = true\nframe.Draggable = true\nframe.Parent = gui\n\n-- Manual Spam Button\nlocal button1 = Instance.new(\"TextButton\")\nbutton1.Text = \"Manual Spam (Off)\"\nbutton1.Size = UDim2.new(1, -20, 0.4, -10)\nbutton1.Position = UDim2.new(0, 10, 0, 10)\nbutton1.BackgroundColor3 = Color3.new(1, 0, 0) -- Red when off\nbutton1.BorderColor3 = Color3.new(1, 1, 1)\nbutton1.BorderSizePixel = 2\nbutton1.Font = Enum.Font.SourceSans\nbutton1.TextColor3 = Color3.new(1, 1, 1)\nbutton1.TextSize = 16\nbutton1.Parent = frame\n\n-- Ball Detection Button\nlocal button2 = Instance.new(\"TextButton\")\nbutton2.Text = \"Auto Ball Detection (Off)\"\nbutton2.Size = UDim2.new(1, -20, 0.4, -10)\nbutton2.Position = UDim2.new(0, 10, 0.5, 10)\nbutton2.BackgroundColor3 = Color3.new(1, 0, 0) -- Red when off\nbutton2.BorderColor3 = Color3.new(1, 1, 1)\nbutton2.BorderSizePixel = 2\nbutton2.Font = Enum.Font.SourceSans\nbutton2.TextColor3 = Color3.new(1, 1, 1)\nbutton2.TextSize = 16\nbutton2.Parent = frame\n\n-- Variables\nlocal runService = game:GetService(\"RunService\")\nlocal players = game:GetService(\"Players\")\nlocal workspace = game:GetService(\"Workspace\")\nlocal vim = game:GetService(\"VirtualInputManager\")\nlocal ballFolder = workspace.Balls\n\nlocal manualSpamActive = false\nlocal autoBallDetectionActive = false\nlocal lastBallPressed, isKeyPressed = nil, false\nlocal ballDetectionConnection\n\n-- Toggle Manual Spam\nlocal function toggleManualSpam()\n    manualSpamActive = not manualSpamActive\n    button1.Text = manualSpamActive and \"Manual Spam (On)\" or \"Manual Spam (Off)\"\n    button1.BackgroundColor3 = manualSpamActive and Color3.new(0, 1, 0) or Color3.new(1, 0, 0) -- Green when on, Red when off\n\n    if manualSpamActive then\n        while manualSpamActive do\n            local args = {1.5, CFrame.new(-254.29, 112.14, -119.27) * CFrame.Angles(-2.03, 0.57, 2.31), {[\"2617721424\"] = Vector3.new(-273.40, -724.80, -20.92)}, {910, 154}}\n            game:GetService(\"ReplicatedStorage\").Remotes.ParryAttempt:FireServer(unpack(args))\n\n            button1.BorderColor3 = Color3.new(math.random(), math.random(), math.random())\n            runService.Heartbeat:Wait()\n        end\n    else\n        button1.BorderColor3 = Color3.new(1, 1, 1) -- Reset border color when off\n    end\nend\n\n-- Ball Prediction Function\nlocal function calculatePredictionTime(ball, player)\n    local rootPart = player.Character and player.Character:FindFirstChild(\"HumanoidRootPart\")\n    if rootPart then\n        local relativePosition = ball.Position - rootPart.Position\n        local velocity = ball.Velocity + rootPart.Velocity\n        local a = ball.Size.magnitude / 2\n        local b = relativePosition.magnitude\n        local c = math.sqrt(a * a + b * b)\n        return (c - a) / velocity.magnitude\n    end\n    return math.huge\nend\n\nlocal function checkProximityToPlayer(ball, player)\n    local predictionTime = calculatePredictionTime(ball, player)\n    local realBallAttribute = ball:GetAttribute(\"realBall\")\n    local target = ball:GetAttribute(\"target\")\n\n    local ballSpeedThreshold = math.max(0.4, 0.6 - ball.Velocity.magnitude * 0.01)\n    if predictionTime <= ballSpeedThreshold and realBallAttribute and target == player.Name and not isKeyPressed then\n        vim:SendKeyEvent(true, Enum.KeyCode.F, false, nil)\n        wait(0.005)\n        vim:SendKeyEvent(false, Enum.KeyCode.F, false, nil)\n        lastBallPressed = ball\n        isKeyPressed = true\n    elseif lastBallPressed == ball and (predictionTime > ballSpeedThreshold or not realBallAttribute or target ~= player.Name) then\n        isKeyPressed = false\n    end\nend\n\nlocal function checkBallsProximity()\n    local player = players.LocalPlayer\n    if player then\n        for _, ball in pairs(ballFolder:GetChildren()) do\n            checkProximityToPlayer(ball, player)\n        end\n    end\nend\n\n-- Toggle Auto Ball Detection\nlocal function toggleAutoBallDetection()\n    autoBallDetectionActive = not autoBallDetectionActive\n    button2.Text = autoBallDetectionActive and \"Auto Ball Detection (On)\" or \"Auto Ball Detection (Off)\"\n    button2.BackgroundColor3 = autoBallDetectionActive and Color3.new(0, 1, 0) or Color3.new(1, 0, 0) -- Green when on, Red when off\n\n    if autoBallDetectionActive then\n        if not ballDetectionConnection then\n            ballDetectionConnection = runService.Heartbeat:Connect(checkBallsProximity)\n        end\n    else\n        if ballDetectionConnection then\n            ballDetectionConnection:Disconnect()\n            ballDetectionConnection = nil\n        end\n        lastBallPressed = nil\n        isKeyPressed = false\n    end\nend\n\n-- Button Click Events\nbutton1.MouseButton1Click:Connect(toggleManualSpam)\nbutton2.MouseButton1Click:Connect(toggleAutoBallDetection)\nend)"

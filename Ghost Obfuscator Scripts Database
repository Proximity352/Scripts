lse \t\t\t\tlocal c = 0 \t\t\t\tfor _,node in next,nodes do \t\t\t\t\tif node.HasDel then \t\t\t\t\t\tlocal delInd \t\t\t\t\t\tfor i = 1,#node do \t\t\t\t\t\t\tif node[i].Del then \t\t\t\t\t\t\t\tdelInd = i \t\t\t\t\t\t\t\tbreak \t\t\t\t\t\t\tend \t\t\t\t\t\tend \t\t\t\t\t\tif delInd then \t\t\t\t\t\t\tfor i = delInd+1,#node do \t\t\t\t\t\t\t\tlocal cn = node[i] \t\t\t\t\t\t\t\tif not cn.Del then \t\t\t\t\t\t\t\t\tnode[delInd] = cn \t\t\t\t\t\t\t\t\tdelInd = delInd+1 \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\tend \t\t\t\t\t\t\tfor i = delInd,#node do \t\t\t\t\t\t\t\tnode[i] = nil \t\t\t\t\t\t\tend \t\t\t\t\t\tend \t\t\t\t\t\tnode.HasDel = false \t\t\t\t\t\tprocessed = true \t\t\t\t\t\tfw() \t\t\t\t\tend \t\t\t\t\tc = c + 1 \t\t\t\t\tif c > 10000 then \t\t\t\t\t\tc = 0 \t\t\t\t\t\tfw() \t\t\t\t\tend \t\t\t\tend \t\t\t\tif processed and not refreshDebounce then Explorer.PerformRefresh() end \t\t\t\tfw(0.5) \t\t\tend \t\tend)() \tend \tExplorer.UpdateSelectionVisuals = function() \t\tlocal holder = Explorer.SelectionVisualsHolder \t\tlocal isa = game.IsA \t\tlocal clone = game.Clone \t\tif not holder then \t\t\tholder = Instance.new(\"ScreenGui\") \t\t\tholder.Name = \"ExplorerSelections\" \t\t\tholder.DisplayOrder = Main.DisplayOrders.Core \t\t\tLib.ShowGui(holder) \t\t\tExplorer.SelectionVisualsHolder = holder \t\t\tExplorer.SelectionVisualCons = {} \t\t\tlocal guiTemplate = create({ \t\t\t\t{1,\"Frame\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,Size=UDim2.new(0,100,0,100),}}, \t\t\t\t{2,\"Frame\",{BackgroundColor3=Color3.new(0.04313725605607,0.35294118523598,0.68627452850342),BorderSizePixel=0,Parent={1},Position=UDim2.new(0,-1,0,-1),Size=UDim2.new(1,2,0,1),}}, \t\t\t\t{3,\"Frame\",{BackgroundColor3=Color3.new(0.04313725605607,0.35294118523598,0.68627452850342),BorderSizePixel=0,Parent={1},Position=UDim2.new(0,-1,1,0),Size=UDim2.new(1,2,0,1),}}, \t\t\t\t{4,\"Frame\",{BackgroundColor3=Color3.new(0.04313725605607,0.35294118523598,0.68627452850342),BorderSizePixel=0,Parent={1},Position=UDim2.new(0,-1,0,0),Size=UDim2.new(0,1,1,0),}}, \t\t\t\t{5,\"Frame\",{BackgroundColor3=Color3.new(0.04313725605607,0.35294118523598,0.68627452850342),BorderSizePixel=0,Parent={1},Position=UDim2.new(1,0,0,0),Size=UDim2.new(0,1,1,0),}}, \t\t\t}) \t\t\tExplorer.SelectionVisualGui = guiTemplate \t\t\tlocal boxTemplate = Instance.new(\"SelectionBox\") \t\t\tboxTemplate.LineThickness = 0.03 \t\t\tboxTemplate.Color3 = Color3.fromRGB(0, 170, 255) \t\t\tExplorer.SelectionVisualBox = boxTemplate \t\tend \t\tholder:ClearAllChildren() \t\t \t\t-- Updates theme \t\tfor i,v in pairs(Explorer.SelectionVisualGui:GetChildren()) do \t\t\tv.BackgroundColor3 = Color3.fromRGB(0, 170, 255) \t\tend \t\t \t\tlocal attachCons = Explorer.SelectionVisualCons \t\tfor i = 1,#attachCons do \t\t\tattachCons[i].Destroy() \t\tend \t\ttable.clear(attachCons) \t\tlocal partEnabled = Settings.Explorer.PartSelectionBox \t\tlocal guiEnabled = Settings.Explorer.GuiSelectionBox \t\tif not partEnabled and not guiEnabled then return end \t\tlocal svg = Explorer.SelectionVisualGui \t\tlocal svb = Explorer.SelectionVisualBox \t\tlocal attachTo = Lib.AttachTo \t\tlocal sList = selection.List \t\tlocal count = 1 \t\tlocal boxCount = 0 \t\tlocal workspaceNode = nodes[workspace] \t\tfor i = 1,#sList do \t\t\tif boxCount > 1000 then break end \t\t\tlocal node = sList[i] \t\t\tlocal obj = node.Obj \t\t\tif node ~= workspaceNode then \t\t\t\tif isa(obj,\"GuiObject\") and guiEnabled then \t\t\t\t\tlocal newVisual = clone(svg) \t\t\t\t\tattachCons[count] = attachTo(newVisual,{Target = obj, Resize = true}) \t\t\t\t\tcount = count + 1 \t\t\t\t\tnewVisual.Parent = holder \t\t\t\t\tboxCount = boxCount + 1 \t\t\t\telseif isa(obj,\"PVInstance\") and partEnabled then \t\t\t\t\tlocal newBox = clone(svb) \t\t\t\t\tnewBox.Adornee = obj \t\t\t\t\tnewBox.Parent = holder \t\t\t\t\tboxCount = boxCount + 1 \t\t\t\tend \t\t\tend \t\tend \tend \tExplorer.Init = function() \t\tExplorer.ClassIcons = Lib.IconMap.newLinear(\"rbxasset://textures/ClassImages.PNG\", 16, 16) \t\tExplorer.MiscIcons = Main.MiscIcons \t\t \t\tclipboard = {} \t\t \t\tselection = Lib.Set.new() \t\tselection.ShiftSet = {} \t\tselection.Changed:Connect(Properties.ShowExplorerProps) \t\tExplorer.Selection = selection \t\t \t\tExplorer.InitRightClick() \t\tExplorer.InitInsertObject() \t\tExplorer.SetSortingEnabled(Settings.Explorer.Sorting) \t\tExplorer.Expanded = setmetatable({},{__mode = \"k\"}) \t\tExplorer.SearchExpanded = setmetatable({},{__mode = \"k\"}) \t\texpanded = Explorer.Expanded \t\tnilNode.Obj.Name = \"Nil Instances\" \t\tnilNode.Locked = true \t\tlocal explorerItems = create({ \t\t\t{1,\"Folder\",{Name=\"ExplorerItems\",}}, \t\t\t{2,\"Frame\",{BackgroundColor3=Color3.new(0.20392157137394,0.20392157137394,0.20392157137394),BorderSizePixel=0,Name=\"ToolBar\",Parent={1},Size=UDim2.new(1,0,0,22),}}, \t\t\t{3,\"Frame\",{BackgroundColor3=Color3.new(0.14901961386204,0.14901961386204,0.14901961386204),BorderColor3=Color3.new(0.1176470592618,0.1176470592618,0.1176470592618),BorderSizePixel=0,Name=\"SearchFrame\",Parent={2},Position=UDim2.new(0,3,0,1),Size=UDim2.new(1,-6,0,18),}}, \t\t\t{4,\"TextBox\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,ClearTextOnFocus=false,Font=3,Name=\"SearchBox\",Parent={3},PlaceholderColor3=Color3.new(0.39215689897537,0.39215689897537,0.39215689897537),PlaceholderText=\"Search workspace\",Position=UDim2.new(0,4,0,0),Size=UDim2.new(1,-24,0,18),Text=\"\",TextColor3=Color3.new(1,1,1),TextSize=14,TextXAlignment=0,}}, \t\t\t{5,\"UICorner\",{CornerRadius=UDim.new(0,2),Parent={3},}}, \t\t\t{6,\"UIStroke\",{Thickness=1.4,Parent={3},Color=Color3.fromRGB(42,42,42)}}, \t\t\t{7,\"TextButton\",{AutoButtonColor=false,BackgroundColor3=Color3.new(0.12549020349979,0.12549020349979,0.12549020349979),BackgroundTransparency=1,BorderSizePixel=0,Font=3,Name=\"Reset\",Parent={3},Position=UDim2.new(1,-17,0,1),Size=UDim2.new(0,16,0,16),Text=\"\",TextColor3=Color3.new(1,1,1),TextSize=14,}}, \t\t\t{8,\"ImageLabel\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,Image=\"rbxassetid://5034718129\",ImageColor3=Color3.new(0.39215686917305,0.39215686917305,0.39215686917305),Parent={7},Size=UDim2.new(0,16,0,16),}}, \t\t\t{9,\"TextButton\",{AutoButtonColor=false,BackgroundColor3=Color3.new(0.12549020349979,0.12549020349979,0.12549020349979),BackgroundTransparency=1,BorderSizePixel=0,Font=3,Name=\"Refresh\",Parent={2},Position=UDim2.new(1,-20,0,1),Size=UDim2.new(0,18,0,18),Text=\"\",TextColor3=Color3.new(1,1,1),TextSize=14,Visible=false,}}, \t\t\t{10,\"ImageLabel\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,Image=\"rbxassetid://5642310344\",Parent={9},Position=UDim2.new(0,3,0,3),Size=UDim2.new(0,12,0,12),}}, \t\t\t{11,\"Frame\",{BackgroundColor3=Color3.new(0.15686275064945,0.15686275064945,0.15686275064945),BorderSizePixel=0,Name=\"ScrollCorner\",Parent={1},Position=UDim2.new(1,-16,1,-16),Size=UDim2.new(0,16,0,16),Visible=false,}}, \t\t\t{12,\"Frame\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,ClipsDescendants=true,Name=\"List\",Parent={1},Position=UDim2.new(0,0,0,23),Size=UDim2.new(1,0,1,-23),}} \t\t}) \t\t \t\ttoolBar = explorerItems.ToolBar \t\ttreeFrame = explorerItems.List \t\tExplorer.GuiElems.ToolBar = toolBar \t\tExplorer.GuiElems.TreeFrame = treeFrame \t\tscrollV = Lib.ScrollBar.new()\t\t \t\tscrollV.WheelIncrement = 3 \t\tscrollV.Gui.Position = UDim2.new(1,-16,0,23) \t\tscrollV:SetScrollFrame(treeFrame) \t\tscrollV.Scrolled:Connect(function() \t\t\tExplorer.Index = scrollV.Index \t\t\tExplorer.Refresh() \t\tend) \t\tscrollH = Lib.ScrollBar.new(true) \t\tscrollH.Increment = 5 \t\tscrollH.WheelIncrement = Explorer.EntryIndent \t\tscrollH.Gui.Position = UDim2.new(0,0,1,-16) \t\tscrollH.Scrolled:Connect(function() \t\t\tExplorer.Refresh() \t\tend) \t\tlocal window = Lib.Window.new() \t\tExplorer.Window = window \t\twindow:SetTitle(\"Explorer\") \t\twindow.GuiElems.Line.Position = UDim2.new(0,0,0,22) \t\tExplorer.InitEntryTemplate() \t\ttoolBar.Parent = window.GuiElems.Content \t\ttreeFrame.Parent = window.GuiElems.Content \t\texplorerItems.ScrollCorner.Parent = window.GuiElems.Content \t\tscrollV.Gui.Parent = window.GuiElems.Content \t\tscrollH.Gui.Parent = window.GuiElems.Content \t\t-- Init stuff that requires the window \t\tExplorer.InitRenameBox() \t\tExplorer.InitSearch() \t\tExplorer.InitDelCleaner() \t\tselection.Changed:Connect(Explorer.UpdateSelectionVisuals) \t\t-- Window events \t\twindow.GuiElems.Main:GetPropertyChangedSignal(\"AbsoluteSize\"):Connect(function() \t\t\tif Explorer.Active then \t\t\t\tExplorer.UpdateView() \t\t\t\tExplorer.Refresh() \t\t\tend \t\tend) \t\twindow.OnActivate:Connect(function() \t\t\tExplorer.Active = true \t\t\tExplorer.UpdateView() \t\t\tExplorer.Update() \t\t\tExplorer.Refresh() \t\tend) \t\twindow.OnRestore:Connect(function() \t\t\tExplorer.Active = true \t\t\tExplorer.UpdateView() \t\t\tExplorer.Update() \t\t\tExplorer.Refresh() \t\tend) \t\twindow.OnDeactivate:Connect(function() Explorer.Active = false end) \t\twindow.OnMinimize:Connect(function() Explorer.Active = false end) \t\t \t\t-- Settings \t\tautoUpdateSearch = Settings.Explorer.AutoUpdateSearch \t\t \t\t-- Fill in nodes \t\tnodes[game] = {Obj = game} \t\texpanded[nodes[game]] = true \t\t \t\t-- Nil Instances \t\tif env.getnilinstances then \t\t\tnodes[nilNode.Obj] = nilNode \t\tend \t\t \t\tExplorer.SetupConnections() \t\tlocal insts = getDescendants(game) \t\tif Main.Elevated then \t\t\tfor i = 1,#insts do \t\t\t\tlocal obj = insts[i] \t\t\t\tlocal par = nodes[ffa(obj,\"Instance\")] \t\t\t\tif not par then continue end \t\t\t\tlocal newNode = { \t\t\t\t\tObj = obj, \t\t\t\t\tParent = par, \t\t\t\t} \t\t\t\tnodes[obj] = newNode \t\t\t\tpar[#par+1] = newNode \t\t\tend \t\telse \t\t\tfor i = 1,#insts do \t\t\t\tlocal obj = insts[i] \t\t\t\tlocal s,parObj = pcall(ffa,obj,\"Instance\") \t\t\t\tlocal par = nodes[parObj] \t\t\t\tif not par then continue end \t\t\t\tlocal newNode = { \t\t\t\t\tObj = obj, \t\t\t\t\tParent = par, \t\t\t\t} \t\t\t\tnodes[obj] = newNode \t\t\t\tpar[#par+1] = newNode \t\t\tend \t\tend \tend \t \treturn Explorer end return {InitDeps = initDeps, InitAfterMain = initAfterMain, Main = main} end, Properties = function() --[[ \tProperties App Module \t \tThe main properties interface ]] -- Common Locals local Main,Lib,Apps,Settings -- Main Containers local Explorer, Properties, ScriptViewer, Notebook -- Major Apps local API,RMD,env,service,plr,create,createSimple -- Main Locals local function initDeps(data) \tMain = data.Main \tLib = data.Lib \tApps = data.Apps \tSettings = data.Settings \tAPI = data.API \tRMD = data.RMD \tenv = data.env \tservice = data.service \tplr = data.plr \tcreate = data.create \tcreateSimple = data.createSimple end local function initAfterMain() \tExplorer = Apps.Explorer \tProperties = Apps.Properties \tScriptViewer = Apps.ScriptViewer \tNotebook = Apps.Notebook end local function main() \tlocal Properties = {} \tlocal window, toolBar, propsFrame \tlocal scrollV, scrollH \tlocal categoryOrder \tlocal props,viewList,expanded,indexableProps,propEntries,autoUpdateObjs = {},{},{},{},{},{} \tlocal inputBox,inputTextBox,inputProp \tlocal checkboxes,propCons = {},{} \tlocal table,string = table,string \tlocal getPropChangedSignal = game.GetPropertyChangedSignal \tlocal getAttributeChangedSignal = game.GetAttributeChangedSignal \tlocal isa = game.IsA \tlocal getAttribute = game.GetAttribute \tlocal setAttribute = game.SetAttribute \tProperties.GuiElems = {} \tProperties.Index = 0 \tProperties.ViewWidth = 0 \tProperties.MinInputWidth = 100 \tProperties.EntryIndent = 16 \tProperties.EntryOffset = 4 \tProperties.NameWidthCache = {} \tProperties.SubPropCache = {} \tProperties.ClassLists = {} \tProperties.SearchText = \"\" \tProperties.AddAttributeProp = {Category = \"Attributes\", Class = \"\", Name = \"\", SpecialRow = \"AddAttribute\", Tags = {}} \tProperties.SoundPreviewProp = {Category = \"Data\", ValueType = {Name = \"SoundPlayer\"}, Class = \"Sound\", Name = \"Preview\", Tags = {}} \tProperties.IgnoreProps = { \t\t[\"DataModel\"] = { \t\t\t[\"PrivateServerId\"] = true, \t\t\t[\"PrivateServerOwnerId\"] = true, \t\t\t[\"VIPServerId\"] = true, \t\t\t[\"VIPServerOwnerId\"] = true \t\t} \t} \tProperties.ExpandableTypes = { \t\t[\"Vector2\"] = true, \t\t[\"Vector3\"] = true, \t\t[\"UDim\"] = true, \t\t[\"UDim2\"] = true, \t\t[\"CFrame\"] = true, \t\t[\"Rect\"] = true, \t\t[\"PhysicalProperties\"] = true, \t\t[\"Ray\"] = true, \t\t[\"NumberRange\"] = true, \t\t[\"Faces\"] = true, \t\t[\"Axes\"] = true \t} \t \tProperties.ExpandableProps = { \t\t[\"Sound.SoundId\"] = true \t} \tProperties.CollapsedCategories = { \t\t[\"Surface Inputs\"] = true, \t\t[\"Surface\"] = true \t} \tProperties.ConflictSubProps = { \t\t[\"Vector2\"] = {\"X\",\"Y\"}, \t\t[\"Vector3\"] = {\"X\",\"Y\",\"Z\"}, \t\t[\"UDim\"] = {\"Scale\",\"Offset\"}, \t\t[\"UDim2\"] = {\"X\",\"X.Scale\",\"X.Offset\",\"Y\",\"Y.Scale\",\"Y.Offset\"}, \t\t[\"CFrame\"] = {\"Position\",\"Position.X\",\"Position.Y\",\"Position.Z\", \t\t\t\"RightVector\",\"RightVector.X\",\"RightVector.Y\",\"RightVector.Z\", \t\t\t\"UpVector\",\"UpVector.X\",\"UpVector.Y\",\"UpVector.Z\", \t\t\t\"LookVector\",\"LookVector.X\",\"LookVector.Y\",\"LookVector.Z\"}, \t\t[\"Rect\"] = {\"Min.X\",\"Min.Y\",\"Max.X\",\"Max.Y\"}, \t\t[\"PhysicalProperties\"] = {\"Density\",\"Elasticity\",\"ElasticityWeight\",\"Friction\",\"FrictionWeight\"}, \t\t[\"Ray\"] = {\"Origin\",\"Origin.X\",\"Origin.Y\",\"Origin.Z\",\"Direction\",\"Direction.X\",\"Direction.Y\",\"Direction.Z\"}, \t\t[\"NumberRange\"] = {\"Min\",\"Max\"}, \t\t[\"Faces\"] = {\"Back\",\"Bottom\",\"Front\",\"Left\",\"Right\",\"Top\"}, \t\t[\"Axes\"] = {\"X\",\"Y\",\"Z\"} \t} \tProperties.ConflictIgnore = { \t\t[\"BasePart\"] = { \t\t\t[\"ResizableFaces\"] = true \t\t} \t} \tProperties.RoundableTypes = { \t\t[\"float\"] = true, \t\t[\"double\"] = true, \t\t[\"Color3\"] = true, \t\t[\"UDim\"] = true, \t\t[\"UDim2\"] = true, \t\t[\"Vector2\"] = true, \t\t[\"Vector3\"] = true, \t\t[\"NumberRange\"] = true, \t\t[\"Rect\"] = true, \t\t[\"NumberSequence\"] = true, \t\t[\"ColorSequence\"] = true, \t\t[\"Ray\"] = true, \t\t[\"CFrame\"] = true \t} \tProperties.TypeNameConvert = { \t\t[\"number\"] = \"double\", \t\t[\"boolean\"] = \"bool\" \t} \tProperties.ToNumberTypes = { \t\t[\"int\"] = true, \t\t[\"int64\"] = true, \t\t[\"float\"] = true, \t\t[\"double\"] = true \t} \tProperties.DefaultPropValue = { \t\tstring = \"\", \t\tbool = false, \t\tdouble = 0, \t\tUDim = UDim.new(0,0), \t\tUDim2 = UDim2.new(0,0,0,0), \t\tBrickColor = BrickColor.new(\"Medium stone grey\"), \t\tColor3 = Color3.new(1,1,1), \t\tVector2 = Vector2.new(0,0), \t\tVector3 = Vector3.new(0,0,0), \t\tNumberSequence = NumberSequence.new(1), \t\tColorSequence = ColorSequence.new(Color3.new(1,1,1)), \t\tNumberRange = NumberRange.new(0), \t\tRect = Rect.new(0,0,0,0) \t} \tProperties.AllowedAttributeTypes = {\"string\",\"boolean\",\"number\",\"UDim\",\"UDim2\",\"BrickColor\",\"Color3\",\"Vector2\",\"Vector3\",\"NumberSequence\",\"ColorSequence\",\"NumberRange\",\"Rect\"} \tProperties.StringToValue = function(prop,str) \t\tlocal typeData = prop.ValueType \t\tlocal typeName = typeData.Name \t\tif typeName == \"string\" or typeName == \"Content\" then \t\t\treturn str \t\telseif Properties.ToNumberTypes[typeName] then \t\t\treturn tonumber(str) \t\telseif typeName == \"Vector2\" then \t\t\tlocal vals = str:split(\",\") \t\t\tlocal x,y = tonumber(vals[1]),tonumber(vals[2]) \t\t\tif x and y and #vals >= 2 then return Vector2.new(x,y) end \t\telseif typeName == \"Vector3\" then \t\t\tlocal vals = str:split(\",\") \t\t\tlocal x,y,z = tonumber(vals[1]),tonumber(vals[2]),tonumber(vals[3]) \t\t\tif x and y and z and #vals >= 3 then return Vector3.new(x,y,z) end \t\telseif typeName == \"UDim\" then \t\t\tlocal vals = str:split(\",\") \t\t\tlocal scale,offset = tonumber(vals[1]),tonumber(vals[2]) \t\t\tif scale and offset and #vals >= 2 then return UDim.new(scale,offset) end \t\telseif typeName == \"UDim2\" then \t\t\tlocal vals = str:gsub(\"[{}]\",\"\"):split(\",\") \t\t\tlocal xScale,xOffset,yScale,yOffset = tonumber(vals[1]),tonumber(vals[2]),tonumber(vals[3]),tonumber(vals[4]) \t\t\tif xScale and xOffset and yScale and yOffset and #vals >= 4 then return UDim2.new(xScale,xOffset,yScale,yOffset) end \t\telseif typeName == \"CFrame\" then \t\t\tlocal vals = str:split(\",\") \t\t\tlocal s,result = pcall(CFrame.new, unpack(vals)) \t\t\tif s and #vals >= 12 then return result end \t\telseif typeName == \"Rect\" then \t\t\tlocal vals = str:split(\",\") \t\t\tlocal s,result = pcall(Rect.new,unpack(vals)) \t\t\tif s and #vals >= 4 then return result end \t\telseif typeName == \"Ray\" then \t\t\tlocal vals = str:gsub(\"[{}]\",\"\"):split(\",\") \t\t\tlocal s,origin = pcall(Vector3.new,unpack(vals,1,3)) \t\t\tlocal s2,direction = pcall(Vector3.new,unpack(vals,4,6)) \t\t\tif s and s2 and #vals >= 6 then return Ray.new(origin,direction) end \t\telseif typeName == \"NumberRange\" then \t\t\tlocal vals = str:split(\",\") \t\t\tlocal s,result = pcall(NumberRange.new,unpack(vals)) \t\t\tif s and #vals >= 1 then return result end \t\telseif typeName == \"Color3\" then \t\t\tlocal vals = str:gsub(\"[{}]\",\"\"):split(\",\") \t\t\tlocal s,result = pcall(Color3.fromRGB,unpack(vals)) \t\t\tif s and #vals >= 3 then return result end \t\tend \t\treturn nil \tend \tProperties.ValueToString = function(prop,val) \t\tlocal typeData = prop.ValueType \t\tlocal typeName = typeData.Name \t\tif typeName == \"Color3\" then \t\t\treturn Lib.ColorToBytes(val) \t\telseif typeName == \"NumberRange\" then \t\t\treturn val.Min..\", \"..val.Max \t\tend \t\t \t\treturn tostring(val) \tend \tProperties.GetIndexableProps = function(obj,classData) \t\tif not Main.Elevated then \t\t\tif not pcall(function() return obj.ClassName end) then return nil end \t\tend \t\tlocal ignoreProps = Properties.IgnoreProps[classData.Name] or {} \t\tlocal result = {} \t\tlocal count = 1 \t\tlocal props = classData.Properties \t\tfor i = 1,#props do \t\t\tlocal prop = props[i] \t\t\tif not ignoreProps[prop.Name] then \t\t\t\tlocal s = pcall(function() return obj[prop.Name] end) \t\t\t\tif s then \t\t\t\t\tresult[count] = prop \t\t\t\t\tcount = count + 1 \t\t\t\tend \t\t\tend \t\tend \t\treturn result \tend \tProperties.FindFirstObjWhichIsA = function(class) \t\tlocal classList = Properties.ClassLists[class] or {} \t\tif classList and #classList > 0 then \t\t\treturn classList[1] \t\tend \t\treturn nil \tend \tProperties.ComputeConflicts = function(p) \t\tlocal maxConflictCheck = Settings.Properties.MaxConflictCheck \t\tlocal sList = Explorer.Selection.List \t\tlocal classLists = Properties.ClassLists \t\tlocal stringSplit = string.split \t\tlocal t_clear = table.clear \t\tlocal conflictIgnore = Properties.ConflictIgnore \t\tlocal conflictMap = {} \t\tlocal propList = p and {p} or props \t\tif p then \t\t\tlocal gName = p.Class..\".\"..p.Name \t\t\tautoUpdateObjs[gName] = nil \t\t\tlocal subProps = Properties.ConflictSubProps[p.ValueType.Name] or {} \t\t\tfor i = 1,#subProps do \t\t\t\tautoUpdateObjs[gName..\".\"..subProps[i]] = nil \t\t\tend \t\telse \t\t\ttable.clear(autoUpdateObjs) \t\tend \t\tif #sList > 0 then \t\t\tfor i = 1,#propList do \t\t\t\tlocal prop = propList[i] \t\t\t\tlocal propName,propClass = prop.Name,prop.Class \t\t\t\tlocal typeData = prop.RootType or prop.ValueType \t\t\t\tlocal typeName = typeData.Name \t\t\t\tlocal attributeName = prop.AttributeName \t\t\t\tlocal gName = propClass..\".\"..propName \t\t\t\tlocal checked = 0 \t\t\t\tlocal subProps = Properties.ConflictSubProps[typeName] or {} \t\t\t\tlocal subPropCount = #subProps \t\t\t\tlocal toCheck = subPropCount + 1 \t\t\t\tlocal conflictsFound = 0 \t\t\t\tlocal indexNames = {} \t\t\t\tlocal ignored = conflictIgnore[propClass] and conflictIgnore[propClass][propName] \t\t\t\tlocal truthyCheck = (typeName == \"PhysicalProperties\") \t\t\t\tlocal isAttribute = prop.IsAttribute \t\t\t\tlocal isMultiType = prop.MultiType \t\t\t\tt_clear(conflictMap) \t\t\t\tif not isMultiType then \t\t\t\t\tlocal firstVal,firstObj,firstSet \t\t\t\t\tlocal classList = classLists[prop.Class] or {} \t\t\t\t\tfor c = 1,#classList do \t\t\t\t\t\tlocal obj = classList[c] \t\t\t\t\t\tif not firstSet then \t\t\t\t\t\t\tif isAttribute then \t\t\t\t\t\t\t\tfirstVal = getAttribute(obj,attributeName) \t\t\t\t\t\t\t\tif firstVal ~= nil then \t\t\t\t\t\t\t\t\tfirstObj = obj \t\t\t\t\t\t\t\t\tfirstSet = true \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\telse \t\t\t\t\t\t\t\tfirstVal = obj[propName] \t\t\t\t\t\t\t\tfirstObj = obj \t\t\t\t\t\t\t\tfirstSet = true \t\t\t\t\t\t\tend \t\t\t\t\t\t\tif ignored then break end \t\t\t\t\t\telse \t\t\t\t\t\t\tlocal propVal,skip \t\t\t\t\t\t\tif isAttribute then \t\t\t\t\t\t\t\tpropVal = getAttribute(obj,attributeName) \t\t\t\t\t\t\t\tif propVal == nil then skip = true end \t\t\t\t\t\t\telse \t\t\t\t\t\t\t\tpropVal = obj[propName] \t\t\t\t\t\t\tend \t\t\t\t\t\t\tif not skip then \t\t\t\t\t\t\t\tif not conflictMap[1] then \t\t\t\t\t\t\t\t\tif truthyCheck then \t\t\t\t\t\t\t\t\t\tif (firstVal and true or false) ~= (propVal and true or false) then \t\t\t\t\t\t\t\t\t\t\tconflictMap[1] = true \t\t\t\t\t\t\t\t\t\t\tconflictsFound = conflictsFound + 1 \t\t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\telseif firstVal ~= propVal then \t\t\t\t\t\t\t\t\t\tconflictMap[1] = true \t\t\t\t\t\t\t\t\t\tconflictsFound = conflictsFound + 1 \t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tif subPropCount > 0 then \t\t\t\t\t\t\t\t\tfor sPropInd = 1,subPropCount do \t\t\t\t\t\t\t\t\t\tlocal indexes = indexNames[sPropInd] \t\t\t\t\t\t\t\t\t\tif not indexes then indexes = stringSplit(subProps[sPropInd],\".\") indexNames[sPropInd] = indexes end \t\t\t\t\t\t\t\t\t\tlocal firstValSub = firstVal \t\t\t\t\t\t\t\t\t\tlocal propValSub = propVal \t\t\t\t\t\t\t\t\t\tfor j = 1,#indexes do \t\t\t\t\t\t\t\t\t\t\tif not firstValSub or not propValSub then break end -- PhysicalProperties \t\t\t\t\t\t\t\t\t\t\tlocal indexName = indexes[j] \t\t\t\t\t\t\t\t\t\t\tfirstValSub = firstValSub[indexName] \t\t\t\t\t\t\t\t\t\t\tpropValSub = propValSub[indexName] \t\t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\t\tlocal mapInd = sPropInd + 1 \t\t\t\t\t\t\t\t\t\tif not conflictMap[mapInd] and firstValSub ~= propValSub then \t\t\t\t\t\t\t\t\t\t\tconflictMap[mapInd] = true \t\t\t\t\t\t\t\t\t\t\tconflictsFound = conflictsFound + 1 \t\t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tif conflictsFound == toCheck then break end \t\t\t\t\t\t\tend \t\t\t\t\t\tend \t\t\t\t\t\tchecked = checked + 1 \t\t\t\t\t\tif checked == maxConflictCheck then break end \t\t\t\t\tend \t\t\t\t\tif not conflictMap[1] then autoUpdateObjs[gName] = firstObj end \t\t\t\t\tfor sPropInd = 1,subPropCount do \t\t\t\t\t\tif not conflictMap[sPropInd+1] then \t\t\t\t\t\t\tautoUpdateObjs[gName..\".\"..subProps[sPropInd]] = firstObj \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\tend \t\t\tend \t\tend \t\tif p then \t\t\tProperties.Refresh() \t\tend \tend \t-- Fetches the properties to be displayed based on the explorer selection \tSettings.Properties.ShowAttributes = true -- im making it true anyway since its useful by default and people complain \tProperties.ShowExplorerProps = function() \t\tlocal maxConflictCheck = Settings.Properties.MaxConflictCheck \t\tlocal sList = Explorer.Selection.List \t\tlocal foundClasses = {} \t\tlocal propCount = 1 \t\tlocal elevated = Main.Elevated \t\tlocal showDeprecated,showHidden = Settings.Properties.ShowDeprecated,Settings.Properties.ShowHidden \t\tlocal Classes = API.Classes \t\tlocal classLists = {} \t\tlocal lower = string.lower \t\tlocal RMDCustomOrders = RMD.PropertyOrders \t\tlocal getAttributes = game.GetAttributes \t\tlocal maxAttrs = Settings.Properties.MaxAttributes \t\tlocal showingAttrs = Settings.Properties.ShowAttributes \t\tlocal foundAttrs = {} \t\tlocal attrCount = 0 \t\tlocal typeof = typeof \t\tlocal typeNameConvert = Properties.TypeNameConvert \t\ttable.clear(props) \t\tfor i = 1,#sList do \t\t\tlocal node = sList[i] \t\t\tlocal obj = node.Obj \t\t\tlocal class = node.Class \t\t\tif not class then class = obj.ClassName node.Class = class end \t\t\tlocal apiClass = Classes[class] \t\t\twhile apiClass do \t\t\t\tlocal APIClassName = apiClass.Name \t\t\t\tif not foundClasses[APIClassName] then \t\t\t\t\tlocal apiProps = indexableProps[APIClassName] \t\t\t\t\tif not apiProps then apiProps = Properties.GetIndexableProps(obj,apiClass) indexableProps[APIClassName] = apiProps end \t\t\t\t\tfor i = 1,#apiProps do \t\t\t\t\t\tlocal prop = apiProps[i] \t\t\t\t\t\tlocal tags = prop.Tags \t\t\t\t\t\tif (not tags.Deprecated or showDeprecated) and (not tags.Hidden or showHidden) then \t\t\t\t\t\t\tprops[propCount] = prop \t\t\t\t\t\t\tpropCount = propCount + 1 \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\tfoundClasses[APIClassName] = true \t\t\t\tend \t\t\t\tlocal classList = classLists[APIClassName] \t\t\t\tif not classList then classList = {} classLists[APIClassName] = classList end \t\t\t\tclassList[#classList+1] = obj \t\t\t\tapiClass = apiClass.Superclass \t\t\tend \t\t\tif showingAttrs and attrCount < maxAttrs then \t\t\t\tlocal attrs = getAttributes(obj) \t\t\t\tfor name,val in pairs(attrs) do \t\t\t\t\tlocal typ = typeof(val) \t\t\t\t\tif not foundAttrs[name] then \t\t\t\t\t\tlocal category = (typ == \"Instance\" and \"Class\") or (typ == \"EnumItem\" and \"Enum\") or \"Other\" \t\t\t\t\t\tlocal valType = {Name = typeNameConvert[typ] or typ, Category = category} \t\t\t\t\t\tlocal attrProp = {IsAttribute = true, Name = \"ATTR_\"..name, AttributeName = name, DisplayName = name, Class = \"Instance\", ValueType = valType, Category = \"Attributes\", Tags = {}} \t\t\t\t\t\tprops[propCount] = attrProp \t\t\t\t\t\tpropCount = propCount + 1 \t\t\t\t\t\tattrCount = attrCount + 1 \t\t\t\t\t\tfoundAttrs[name] = {typ,attrProp} \t\t\t\t\t\tif attrCount == maxAttrs then break end \t\t\t\t\telseif foundAttrs[name][1] ~= typ then \t\t\t\t\t\tfoundAttrs[name][2].MultiType = true \t\t\t\t\t\tfoundAttrs[name][2].Tags.ReadOnly = true \t\t\t\t\t\tfoundAttrs[name][2].ValueType = {Name = \"string\"} \t\t\t\t\tend \t\t\t\tend \t\t\tend \t\tend \t\ttable.sort(props,function(a,b) \t\t\tif a.Category ~= b.Category then \t\t\t\treturn (categoryOrder[a.Category] or 9999) < (categoryOrder[b.Category] or 9999) \t\t\telse \t\t\t\tlocal aOrder = (RMDCustomOrders[a.Class] and RMDCustomOrders[a.Class][a.Name]) or 9999999 \t\t\t\tlocal bOrder = (RMDCustomOrders[b.Class] and RMDCustomOrders[b.Class][b.Name]) or 9999999 \t\t\t\tif aOrder ~= bOrder then \t\t\t\t\treturn aOrder < bOrder \t\t\t\telse \t\t\t\t\treturn lower(a.Name) < lower(b.Name) \t\t\t\tend \t\t\tend \t\tend) \t\t-- Find conflicts and get auto-update instances \t\tProperties.ClassLists = classLists \t\tProperties.ComputeConflicts() \t\t--warn(\"CONFLICT\",tick()-start) \t\tif #props > 0 then \t\t\tprops[#props+1] = Properties.AddAttributeProp \t\tend \t\tProperties.Update() \t\tProperties.Refresh() \tend \tProperties.UpdateView = function() \t\tlocal maxEntries = math.ceil(propsFrame.AbsoluteSize.Y / 23) \t\tlocal maxX = propsFrame.AbsoluteSize.X \t\tlocal totalWidth = Properties.ViewWidth + Properties.MinInputWidth \t\tscrollV.VisibleSpace = maxEntries \t\tscrollV.TotalSpace = #viewList + 1 \t\tscrollH.VisibleSpace = maxX \t\tscrollH.TotalSpace = totalWidth \t\tscrollV.Gui.Visible = #viewList + 1 > maxEntries \t\tscrollH.Gui.Visible = Settings.Properties.ScaleType == 0 and totalWidth > maxX \t\tlocal oldSize = propsFrame.Size \t\tpropsFrame.Size = UDim2.new(1,(scrollV.Gui.Visible and -16 or 0),1,(scrollH.Gui.Visible and -39 or -23)) \t\tif oldSize ~= propsFrame.Size then \t\t\tProperties.UpdateView() \t\telse \t\t\tscrollV:Update() \t\t\tscrollH:Update() \t\t\tif scrollV.Gui.Visible and scrollH.Gui.Visible then \t\t\t\tscrollV.Gui.Size = UDim2.new(0,16,1,-39) \t\t\t\tscrollH.Gui.Size = UDim2.new(1,-16,0,16) \t\t\t\tProperties.Window.GuiElems.Content.ScrollCorner.Visible = true \t\t\telse \t\t\t\tscrollV.Gui.Size = UDim2.new(0,16,1,-23) \t\t\t\tscrollH.Gui.Size = UDim2.new(1,0,0,16) \t\t\t\tProperties.Window.GuiElems.Content.ScrollCorner.Visible = false \t\t\tend \t\t\tProperties.Index = scrollV.Index \t\tend \tend \tProperties.MakeSubProp = function(prop,subName,valueType,displayName) \t\tlocal subProp = {} \t\tfor i,v in pairs(prop) do \t\t\tsubProp[i] = v \t\tend \t\tsubProp.RootType = subProp.RootType or subProp.ValueType \t\tsubProp.ValueType = valueType \t\tsubProp.SubName = subProp.SubName and (subProp.SubName..subName) or subName \t\tsubProp.DisplayName = displayName \t\treturn subProp \tend \tProperties.GetExpandedProps = function(prop) -- TODO: Optimize using table \t\tlocal result = {} \t\tlocal typeData = prop.ValueType \t\tlocal typeName = typeData.Name \t\tlocal makeSubProp = Properties.MakeSubProp \t\tif typeName == \"Vector2\" then \t\t\tresult[1] = makeSubProp(prop,\".X\",{Name = \"float\"}) \t\t\tresult[2] = makeSubProp(prop,\".Y\",{Name = \"float\"}) \t\telseif typeName == \"Vector3\" then \t\t\tresult[1] = makeSubProp(prop,\".X\",{Name = \"float\"}) \t\t\tresult[2] = makeSubProp(prop,\".Y\",{Name = \"float\"}) \t\t\tresult[3] = makeSubProp(prop,\".Z\",{Name = \"float\"}) \t\telseif typeName == \"CFrame\" then \t\t\tresult[1] = makeSubProp(prop,\".Position\",{Name = \"Vector3\"}) \t\t\tresult[2] = makeSubProp(prop,\".RightVector\",{Name = \"Vector3\"}) \t\t\tresult[3] = makeSubProp(prop,\".UpVector\",{Name = \"Vector3\"}) \t\t\tresult[4] = makeSubProp(prop,\".LookVector\",{Name = \"Vector3\"}) \t\telseif typeName == \"UDim\" then \t\t\tresult[1] = makeSubProp(prop,\".Scale\",{Name = \"float\"}) \t\t\tresult[2] = makeSubProp(prop,\".Offset\",{Name = \"int\"}) \t\telseif typeName == \"UDim2\" then \t\t\tresult[1] = makeSubProp(prop,\".X\",{Name = \"UDim\"}) \t\t\tresult[2] = makeSubProp(prop,\".Y\",{Name = \"UDim\"}) \t\telseif typeName == \"Rect\" then \t\t\tresult[1] = makeSubProp(prop,\".Min.X\",{Name = \"float\"},\"X0\") \t\t\tresult[2] = makeSubProp(prop,\".Min.Y\",{Name = \"float\"},\"Y0\") \t\t\tresult[3] = makeSubProp(prop,\".Max.X\",{Name = \"float\"},\"X1\") \t\t\tresult[4] = makeSubProp(prop,\".Max.Y\",{Name = \"float\"},\"Y1\") \t\telseif typeName == \"PhysicalProperties\" then \t\t\tresult[1] = makeSubProp(prop,\".Density\",{Name = \"float\"}) \t\t\tresult[2] = makeSubProp(prop,\".Elasticity\",{Name = \"float\"}) \t\t\tresult[3] = makeSubProp(prop,\".ElasticityWeight\",{Name = \"float\"}) \t\t\tresult[4] = makeSubProp(prop,\".Friction\",{Name = \"float\"}) \t\t\tresult[5] = makeSubProp(prop,\".FrictionWeight\",{Name = \"float\"}) \t\telseif typeName == \"Ray\" then \t\t\tresult[1] = makeSubProp(prop,\".Origin\",{Name = \"Vector3\"}) \t\t\tresult[2] = makeSubProp(prop,\".Direction\",{Name = \"Vector3\"}) \t\telseif typeName == \"NumberRange\" then \t\t\tresult[1] = makeSubProp(prop,\".Min\",{Name = \"float\"}) \t\t\tresult[2] = makeSubProp(prop,\".Max\",{Name = \"float\"}) \t\telseif typeName == \"Faces\" then \t\t\tresult[1] = makeSubProp(prop,\".Back\",{Name = \"bool\"}) \t\t\tresult[2] = makeSubProp(prop,\".Bottom\",{Name = \"bool\"}) \t\t\tresult[3] = makeSubProp(prop,\".Front\",{Name = \"bool\"}) \t\t\tresult[4] = makeSubProp(prop,\".Left\",{Name = \"bool\"}) \t\t\tresult[5] = makeSubProp(prop,\".Right\",{Name = \"bool\"}) \t\t\tresult[6] = makeSubProp(prop,\".Top\",{Name = \"bool\"}) \t\telseif typeName == \"Axes\" then \t\t\tresult[1] = makeSubProp(prop,\".X\",{Name = \"bool\"}) \t\t\tresult[2] = makeSubProp(prop,\".Y\",{Name = \"bool\"}) \t\t\tresult[3] = makeSubProp(prop,\".Z\",{Name = \"bool\"}) \t\tend \t\t \t\tif prop.Name == \"SoundId\" and prop.Class == \"Sound\" then \t\t\tresult[1] = Properties.SoundPreviewProp \t\tend \t\t \t\treturn result \tend \tProperties.Update = function() \t\ttable.clear(viewList) \t\tlocal nameWidthCache = Properties.NameWidthCache \t\tlocal lastCategory \t\tlocal count = 1 \t\tlocal maxWidth,maxDepth = 0,1 \t\tlocal textServ = service.TextService \t\tlocal getTextSize = textServ.GetTextSize \t\tlocal font = Enum.Font.SourceSans \t\tlocal size = Vector2.new(math.huge,20) \t\tlocal stringSplit = string.split \t\tlocal entryIndent = Properties.EntryIndent \t\tlocal isFirstScaleType = Settings.Properties.ScaleType == 0 \t\tlocal find,lower = string.find,string.lower \t\tlocal searchText = (#Properties.SearchText > 0 and lower(Properties.SearchText)) \t\tlocal function recur(props,depth) \t\t\tfor i = 1,#props do \t\t\t\tlocal prop = props[i] \t\t\t\tlocal propName = prop.Name \t\t\t\tlocal subName = prop.SubName \t\t\t\tlocal category = prop.Category \t\t\t\t \t\t\t\tlocal visible \t\t\t\tif searchText and depth == 1 then \t\t\t\t\tif find(lower(propName),searchText,1,true) then \t\t\t\t\t\tvisible = true \t\t\t\t\tend \t\t\t\telse \t\t\t\t\tvisible = true \t\t\t\tend \t\t\t\t \t\t\t\tif visible and lastCategory ~= category then \t\t\t\t\tviewList[count] = {CategoryName = category} \t\t\t\t\tcount = count + 1 \t\t\t\t\tlastCategory = category \t\t\t\tend \t\t\t\t \t\t\t\tif (expanded[\"CAT_\"..category] and visible) or prop.SpecialRow then \t\t\t\t\tif depth > 1 then prop.Depth = depth if depth > maxDepth then maxDepth = depth end end \t\t\t\t\t \t\t\t\t\tif isFirstScaleType then \t\t\t\t\t\tlocal nameArr = subName and stringSplit(subName,\".\") \t\t\t\t\t\tlocal displayName = prop.DisplayName or (nameArr and nameArr[#nameArr]) or propName \t\t\t\t\t\t \t\t\t\t\t\tlocal nameWidth = nameWidthCache[displayName] \t\t\t\t\t\tif not nameWidth then nameWidth = getTextSize(textServ,displayName,14,font,size).X nameWidthCache[displayName] = nameWidth end \t\t\t\t\t\t \t\t\t\t\t\tlocal totalWidth = nameWidth + entryIndent*depth \t\t\t\t\t\tif totalWidth > maxWidth then \t\t\t\t\t\t\tmaxWidth = totalWidth \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\tviewList[count] = prop \t\t\t\t\tcount = count + 1 \t\t\t\t\tlocal fullName = prop.Class..\".\"..prop.Name..(prop.SubName or \"\") \t\t\t\t\tif expanded[fullName] then \t\t\t\t\t\tlocal nextDepth = depth+1 \t\t\t\t\t\tlocal expandedProps = Properties.GetExpandedProps(prop) \t\t\t\t\t\tif #expandedProps > 0 then \t\t\t\t\t\t\trecur(expandedProps,nextDepth) \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\tend \t\t\tend \t\tend \t\trecur(props,1) \t\tinputProp = nil \t\tProperties.ViewWidth = maxWidth + 9 + Properties.EntryOffset \t\tProperties.UpdateView() \tend \tProperties.NewPropEntry = function(index) \t\tlocal newEntry = Properties.EntryTemplate:Clone() \t\tlocal nameFrame = newEntry.NameFrame \t\tlocal valueFrame = newEntry.ValueFrame \t\tlocal newCheckbox = Lib.Checkbox.new(1) \t\tnewCheckbox.Gui.Position = UDim2.new(0,3,0,3) \t\tnewCheckbox.Gui.Parent = valueFrame \t\tnewCheckbox.OnInput:Connect(function() \t\t\tlocal prop = viewList[index + Properties.Index] \t\t\tif not prop then return end \t\t\tif prop.ValueType.Name == \"PhysicalProperties\" then \t\t\t\tProperties.SetProp(prop,newCheckbox.Toggled and true or nil) \t\t\telse \t\t\t\tProperties.SetProp(prop,newCheckbox.Toggled) \t\t\tend \t\tend) \t\tcheckboxes[index] = newCheckbox \t\tlocal iconFrame = Main.MiscIcons:GetLabel() \t\ticonFrame.Position = UDim2.new(0,2,0,3) \t\ticonFrame.Parent = newEntry.ValueFrame.RightButton \t\tnewEntry.Position = UDim2.new(0,0,0,23*(index-1)) \t\tnameFrame.Expand.InputBegan:Connect(function(input) \t\t\tlocal prop = viewList[index + Properties.Index] \t\t\tif not prop or (input.UserInputType ~= Enum.UserInputType.MouseMovement and input.UserInputType ~= Enum.UserInputType.Touch) then return end \t\t \t\t\tlocal fullName = (prop.CategoryName and \"CAT_\"..prop.CategoryName) or prop.Class..\".\"..prop.Name..(prop.SubName or \"\") \t\t \t\t\tMain.MiscIcons:DisplayByKey(newEntry.NameFrame.Expand.Icon, expanded[fullName] and \"Collapse_Over\" or \"Expand_Over\") \t\tend) \t\t \t\tnameFrame.Expand.InputEnded:Connect(function(input) \t\t\tlocal prop = viewList[index + Properties.Index] \t\t\tif not prop or (input.UserInputType ~= Enum.UserInputType.MouseMovement and input.UserInputType ~= Enum.UserInputType.Touch) then return end \t\t \t\t\tlocal fullName = (prop.CategoryName and \"CAT_\"..prop.CategoryName) or prop.Class..\".\"..prop.Name..(prop.SubName or \"\") \t\t \t\t\tMain.MiscIcons:DisplayByKey(newEntry.NameFrame.Expand.Icon, expanded[fullName] and \"Collapse\" or \"Expand\") \t\tend) \t\tnameFrame.Expand.MouseButton1Down:Connect(function() \t\t\tlocal prop = viewList[index + Properties.Index] \t\t\tif not prop then return end \t\t\tlocal fullName = (prop.CategoryName and \"CAT_\"..prop.CategoryName) or prop.Class..\".\"..prop.Name..(prop.SubName or \"\") \t\t\tif not prop.CategoryName and not Properties.ExpandableTypes[prop.ValueType and prop.ValueType.Name] and not Properties.ExpandableProps[fullName] then return end \t\t\texpanded[fullName] = not expanded[fullName] \t\t\tProperties.Update() \t\t\tProperties.Refresh() \t\tend) \t\tnameFrame.PropName.InputBegan:Connect(function(input) \t\t\tlocal prop = viewList[index + Properties.Index] \t\t\tif not prop then return end \t\t\tif (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and not nameFrame.PropName.TextFits then \t\t\t\tlocal fullNameFrame = Properties.FullNameFrame\t \t\t\t\tlocal nameArr = string.split(prop.Class..\".\"..prop.Name..(prop.SubName or \"\"), \".\") \t\t\t\tlocal dispName = prop.DisplayName or nameArr[#nameArr] \t\t\t\tlocal sizeX = service.TextService:GetTextSize(dispName, 14, Enum.Font.SourceSans, Vector2.new(math.huge, 20)).X \t\t \t\t\t\tfullNameFrame.TextLabel.Text = dispName \t\t\t\tfullNameFrame.Size = UDim2.new(0, sizeX + 4, 0, 22) \t\t\t\tfullNameFrame.Visible = true \t\t\t\tProperties.FullNameFrameIndex = index \t\t\t\tProperties.FullNameFrameAttach.SetData(fullNameFrame, {Target = nameFrame}) \t\t\t\tProperties.FullNameFrameAttach.Enable() \t\t\tend \t\tend) \t\t \t\tnameFrame.PropName.InputEnded:Connect(function(input) \t\t\tif (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and Properties.FullNameFrameIndex == index then \t\t\t\tProperties.FullNameFrame.Visible = false \t\t\t\tProperties.FullNameFrameAttach.Disable() \t\t\tend \t\tend) \t\t \t\tvalueFrame.ValueBox.MouseButton1Down:Connect(function() \t\t\tlocal prop = viewList[index + Properties.Index] \t\t\tif not prop then return end \t\t\t \t\t\tProperties.SetInputProp(prop,index) \t\tend) \t\tvalueFrame.ColorButton.MouseButton1Down:Connect(function() \t\t\tlocal prop = viewList[index + Properties.Index] \t\t\tif not prop then return end \t\t\tProperties.SetInputProp(prop,index,\"color\") \t\tend) \t\tvalueFrame.RightButton.MouseButton1Click:Connect(function() \t\t\tlocal prop = viewList[index + Properties.Index] \t\t\tif not prop then return end \t\t\tlocal fullName = prop.Class..\".\"..prop.Name..(prop.SubName or \"\") \t\t\tlocal inputFullName = inputProp and (inputProp.Class..\".\"..inputProp.Name..(inputProp.SubName or \"\")) \t\t\tif fullName == inputFullName and inputProp.ValueType.Category == \"Class\" then \t\t\t\tinputProp = nil \t\t\t\tProperties.SetProp(prop,nil) \t\t\telse \t\t\t\tProperties.SetInputProp(prop,index,\"right\") \t\t\tend \t\tend) \t\tnameFrame.ToggleAttributes.MouseButton1Click:Connect(function() \t\t\tSettings.Properties.ShowAttributes = not Settings.Properties.ShowAttributes \t\t\tProperties.ShowExplorerProps() \t\tend) \t\tnewEntry.RowButton.MouseButton1Click:Connect(function() \t\t\tProperties.DisplayAddAttributeWindow() \t\tend) \t\tnewEntry.EditAttributeButton.MouseButton1Down:Connect(function() \t\t\tlocal prop = viewList[index + Properties.Index] \t\t\tif not prop then return end \t\t\tProperties.DisplayAttributeContext(prop) \t\tend) \t\tvalueFrame.SoundPreview.ControlButton.MouseButton1Click:Connect(function() \t\t\tif Properties.PreviewSound and Properties.PreviewSound.Playing then \t\t\t\tProperties.SetSoundPreview(false) \t\t\telse \t\t\t\tlocal soundObj = Properties.FindFirstObjWhichIsA(\"Sound\") \t\t\t\tif soundObj then Properties.SetSoundPreview(soundObj) end \t\t\tend \t\tend) \t\tvalueFrame.SoundPreview.InputBegan:Connect(function(input) \t\t\tif input.UserInputType ~= Enum.UserInputType.MouseButton1 and input.UserInputType ~= Enum.UserInputType.Touch then return end \t\t \t\t\tlocal releaseEvent, inputEvent \t\t\treleaseEvent = service.UserInputService.InputEnded:Connect(function(input) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\tif inputEvent then \t\t\t\t\t\tinputEvent:Disconnect() \t\t\t\t\tend \t\t\t\tend \t\t\tend) \t\t \t\t\tlocal timeLine = newEntry.ValueFrame.SoundPreview.TimeLine \t\t\tlocal soundObj = Properties.FindFirstObjWhichIsA(\"Sound\") \t\t\tif soundObj then Properties.SetSoundPreview(soundObj, true) end \t\t \t\t\tlocal function update(input) \t\t\t\tlocal sound = Properties.PreviewSound \t\t\t\tif not sound or sound.TimeLength == 0 then return end \t\t \t\t\t\tlocal inputX = (input.UserInputType == Enum.UserInputType.Touch) and input.Position.X or input.Position.X \t\t\t\tlocal timeLineSize = timeLine.AbsoluteSize \t\t\t\tlocal relaX = inputX - timeLine.AbsolutePosition.X \t\t \t\t\t\tif timeLineSize.X <= 1 then return end \t\t\t\tif relaX < 0 then relaX = 0 elseif relaX >= timeLineSize.X then relaX = timeLineSize.X - 1 end \t\t \t\t\t\tlocal perc = (relaX / (timeLineSize.X - 1)) \t\t\t\tsound.TimePosition = perc * sound.TimeLength \t\t\t\ttimeLine.Slider.Position = UDim2.new(perc, -4, 0, -8) \t\t\tend \t\t \t\t\tupdate(input) \t\t \t\t\tinputEvent = service.UserInputService.InputChanged:Connect(function(input) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\tupdate(input) \t\t\t\tend \t\t\tend) \t\tend) \t\t \t\t \t\tnewEntry.Parent = propsFrame \t\t \t\treturn { \t\t\tGui = newEntry, \t\t\tGuiElems = { \t\t\t\tNameFrame = nameFrame, \t\t\t\tValueFrame = valueFrame, \t\t\t\tPropName = nameFrame.PropName, \t\t\t\tValueBox = valueFrame.ValueBox, \t\t\t\tExpand = nameFrame.Expand, \t\t\t\tColorButton = valueFrame.ColorButton, \t\t\t\tColorPreview = valueFrame.ColorButton.ColorPreview, \t\t\t\tGradient = valueFrame.ColorButton.ColorPreview.UIGradient, \t\t\t\tEnumArrow = valueFrame.EnumArrow, \t\t\t\tCheckbox = valueFrame.Checkbox, \t\t\t\tRightButton = valueFrame.RightButton, \t\t\t\tRightButtonIcon = iconFrame, \t\t\t\tRowButton = newEntry.RowButton, \t\t\t\tEditAttributeButton = newEntry.EditAttributeButton, \t\t\t\tToggleAttributes = nameFrame.ToggleAttributes, \t\t\t\tSoundPreview = valueFrame.SoundPreview, \t\t\t\tSoundPreviewSlider = valueFrame.SoundPreview.TimeLine.Slider \t\t\t} \t\t} \tend \tProperties.GetSoundPreviewEntry = function() \t\tfor i = 1,#viewList do \t\t\tif viewList[i] == Properties.SoundPreviewProp then \t\t\t\treturn propEntries[i - Properties.Index] \t\t\tend \t\tend \tend \tProperties.SetSoundPreview = function(soundObj,noplay) \t\tlocal sound = Properties.PreviewSound \t\tif not sound then \t\t\tsound = Instance.new(\"Sound\") \t\t\tsound.Name = \"Preview\" \t\t\tsound.Paused:Connect(function() \t\t\t\tlocal entry = Properties.GetSoundPreviewEntry() \t\t\t\tif entry then Main.MiscIcons:DisplayByKey(entry.GuiElems.SoundPreview.ControlButton.Icon, \"Play\") end \t\t\tend) \t\t\tsound.Resumed:Connect(function() Properties.Refresh() end) \t\t\tsound.Ended:Connect(function() \t\t\t\tlocal entry = Properties.GetSoundPreviewEntry() \t\t\t\tif entry then entry.GuiElems.SoundPreviewSlider.Position = UDim2.new(0,-4,0,-8) end \t\t\t\tProperties.Refresh() \t\t\tend) \t\t\tsound.Parent = window.Gui \t\t\tProperties.PreviewSound = sound \t\tend \t\tif not soundObj then \t\t\tsound:Pause() \t\telse \t\t\tlocal newId = sound.SoundId ~= soundObj.SoundId \t\t\tsound.SoundId = soundObj.SoundId \t\t\tsound.PlaybackSpeed = soundObj.PlaybackSpeed \t\t\tsound.Volume = soundObj.Volume \t\t\tif newId then sound.TimePosition = 0 end \t\t\tif not noplay then sound:Resume() end \t\t\tcoroutine.wrap(function() \t\t\t\tlocal previewTime = tick() \t\t\t\tProperties.SoundPreviewTime = previewTime \t\t\t\twhile previewTime == Properties.SoundPreviewTime and sound.Playing do \t\t\t\t\tlocal entry = Properties.GetSoundPreviewEntry() \t\t\t\t\tif entry then \t\t\t\t\t\tlocal tl = sound.TimeLength \t\t\t\t\t\tlocal perc = sound.TimePosition/(tl == 0 and 1 or tl) \t\t\t\t\t\tentry.GuiElems.SoundPreviewSlider.Position = UDim2.new(perc,-4,0,-8) \t\t\t\t\tend \t\t\t\t\tLib.FastWait() \t\t\t\tend \t\t\tend)() \t\t\tProperties.Refresh() \t\tend \tend \tProperties.DisplayAttributeContext = function(prop) \t\tlocal context = Properties.AttributeContext \t\tif not context then \t\t\tcontext = Lib.ContextMenu.new() \t\t\tcontext.Iconless = true \t\t\tcontext.Width = 80 \t\tend \t\tcontext:Clear() \t\tcontext:Add({Name = \"Edit\", OnClick = function() \t\t\tProperties.DisplayAddAttributeWindow(prop) \t\tend}) \t\tcontext:Add({Name = \"Delete\", OnClick = function() \t\t\tProperties.SetProp(prop,nil,true) \t\t\tProperties.ShowExplorerProps() \t\tend}) \t\tcontext:Show() \tend \tProperties.DisplayAddAttributeWindow = function(editAttr) \t\tlocal win = Properties.AddAttributeWindow \t\tif not win then \t\t\twin = Lib.Window.new() \t\t\twin.Alignable = false \t\t\twin.Resizable = false \t\t\twin:SetTitle(\"Add Attribute\") \t\t\twin:SetSize(200,130) \t\t\tlocal saveButton = Lib.Button.new() \t\t\tlocal nameLabel = Lib.Label.new() \t\t\tnameLabel.Text = \"Name\" \t\t\tnameLabel.Position = UDim2.new(0,30,0,10) \t\t\tnameLabel.Size = UDim2.new(0,40,0,20) \t\t\twin:Add(nameLabel) \t\t\tlocal nameBox = Lib.ViewportTextBox.new() \t\t\tnameBox.Position = UDim2.new(0,75,0,10) \t\t\tnameBox.Size = UDim2.new(0,120,0,20) \t\t\twin:Add(nameBox,\"NameBox\") \t\t\tnameBox.TextBox:GetPropertyChangedSignal(\"Text\"):Connect(function() \t\t\t\tsaveButton:SetDisabled(#nameBox:GetText() == 0) \t\t\tend) \t\t\tlocal typeLabel = Lib.Label.new() \t\t\ttypeLabel.Text = \"Type\" \t\t\ttypeLabel.Position = UDim2.new(0,30,0,40) \t\t\ttypeLabel.Size = UDim2.new(0,40,0,20) \t\t\twin:Add(typeLabel) \t\t\tlocal typeChooser = Lib.DropDown.new() \t\t\ttypeChooser.CanBeEmpty = false \t\t\ttypeChooser.Position = UDim2.new(0,75,0,40) \t\t\ttypeChooser.Size = UDim2.new(0,120,0,20) \t\t\ttypeChooser:SetOptions(Properties.AllowedAttributeTypes) \t\t\twin:Add(typeChooser,\"TypeChooser\") \t\t\tlocal errorLabel = Lib.Label.new() \t\t\terrorLabel.Text = \"\" \t\t\terrorLabel.Position = UDim2.new(0,5,1,-45) \t\t\terrorLabel.Size = UDim2.new(1,-10,0,20) \t\t\terrorLabel.TextColor3 = Settings.Theme.Important \t\t\twin.ErrorLabel = errorLabel \t\t\twin:Add(errorLabel,\"Error\") \t\t\tlocal cancelButton = Lib.Button.new() \t\t\tcancelButton.Text = \"Cancel\" \t\t\tcancelButton.Position = UDim2.new(1,-97,1,-25) \t\t\tcancelButton.Size = UDim2.new(0,92,0,20) \t\t\tcancelButton.OnClick:Connect(function() \t\t\t\twin:Close() \t\t\tend) \t\t\twin:Add(cancelButton) \t\t\tsaveButton.Text = \"Save\" \t\t\tsaveButton.Position = UDim2.new(0,5,1,-25) \t\t\tsaveButton.Size = UDim2.new(0,92,0,20) \t\t\tsaveButton.OnClick:Connect(function() \t\t\t\tlocal name = nameBox:GetText() \t\t\t\tif #name > 100 then \t\t\t\t\terrorLabel.Text = \"Error: Name over 100 chars\" \t\t\t\t\treturn \t\t\t\telseif name:sub(1,3) == \"RBX\" then \t\t\t\t\terrorLabel.Text = \"Error: Name begins with 'RBX'\" \t\t\t\t\treturn \t\t\t\tend \t\t\t\tlocal typ = typeChooser.Selected \t\t\t\tlocal valType = {Name = Properties.TypeNameConvert[typ] or typ, Category = \"DataType\"} \t\t\t\tlocal attrProp = {IsAttribute = true, Name = \"ATTR_\"..name, AttributeName = name, DisplayName = name, Class = \"Instance\", ValueType = valType, Category = \"Attributes\", Tags = {}} \t\t\t\tSettings.Properties.ShowAttributes = true \t\t\t\tProperties.SetProp(attrProp,Properties.DefaultPropValue[valType.Name],true,Properties.EditingAttribute) \t\t\t\tProperties.ShowExplorerProps() \t\t\t\twin:Close() \t\t\tend) \t\t\twin:Add(saveButton,\"SaveButton\") \t\t\tProperties.AddAttributeWindow = win \t\tend \t\tProperties.EditingAttribute = editAttr \t\twin:SetTitle(editAttr and \"Edit Attribute \"..editAttr.AttributeName or \"Add Attribute\") \t\twin.Elements.Error.Text = \"\" \t\twin.Elements.NameBox:SetText(\"\") \t\twin.Elements.SaveButton:SetDisabled(true) \t\twin.Elements.TypeChooser:SetSelected(1) \t\twin:Show() \tend \tProperties.IsTextEditable = function(prop) \t\tlocal typeData = prop.ValueType \t\tlocal typeName = typeData.Name \t\treturn typeName ~= \"bool\" and typeData.Category ~= \"Enum\" and typeData.Category ~= \"Class\" and typeName ~= \"BrickColor\" \tend \t \tProperties.DisplayEnumDropdown = function(entryIndex) \t\tlocal context = Properties.EnumContext \t\tif not context then \t\t\tcontext = Lib.ContextMenu.new() \t\t\tcontext.Iconless = true \t\t\tcontext.MaxHeight = 200 \t\t\tcontext.ReverseYOffset = 22 \t\t\tProperties.EnumDropdown = context \t\tend \t\tif not inputProp or inputProp.ValueType.Category ~= \"Enum\" then return end \t\tlocal prop = inputProp \t\tlocal entry = propEntries[entryIndex] \t\tlocal valueFrame = entry.GuiElems.ValueFrame \t\tlocal enum = Enum[prop.ValueType.Name] \t\tif not enum then return end \t\tlocal sorted = {} \t\tfor name,enum in next,enum:GetEnumItems() do \t\t\tsorted[#sorted+1] = enum \t\tend \t\ttable.sort(sorted,function(a,b) return a.Name < b.Name end) \t\t \t\tcontext:Clear() \t\tlocal function onClick(name) \t\t\tif prop ~= inputProp then return end \t\t\t \t\t\tlocal enumItem = enum[name] \t\t\tinputProp = nil \t\t\tProperties.SetProp(prop,enumItem) \t\tend \t\tfor i = 1,#sorted do \t\t\tlocal enumItem = sorted[i] \t\t\tcontext:Add({Name = enumItem.Name, OnClick = onClick}) \t\tend \t\t \t\tcontext.Width = valueFrame.AbsoluteSize.X \t\tcontext:Show(valueFrame.AbsolutePosition.X, valueFrame.AbsolutePosition.Y + 22) \tend \tProperties.DisplayBrickColorEditor = function(prop,entryIndex,col) \t\tlocal editor = Properties.BrickColorEditor \t\tif not editor then \t\t\teditor = Lib.BrickColorPicker.new() \t\t\teditor.Gui.DisplayOrder = Main.DisplayOrders.Menu \t\t\teditor.ReverseYOffset = 22 \t\t\teditor.OnSelect:Connect(function(col) \t\t\t\tif not editor.CurrentProp or editor.CurrentProp.ValueType.Name ~= \"BrickColor\" then return end \t\t\t\t \t\t\t\tif editor.CurrentProp == inputProp then inputProp = nil end \t\t\t\tProperties.SetProp(editor.CurrentProp,BrickColor.new(col)) \t\t\tend) \t\t\teditor.OnMoreColors:Connect(function() -- TODO: Special Case BasePart.BrickColor to BasePart.Color \t\t\t\teditor:Close() \t\t\t\tlocal colProp \t\t\t\tfor i,v in pairs(API.Classes.BasePart.Properties) do \t\t\t\t\tif v.Name == \"Color\" then \t\t\t\t\t\tcolProp = v \t\t\t\t\t\tbreak \t\t\t\t\tend \t\t\t\tend \t\t\t\tProperties.DisplayColorEditor(colProp,editor.SavedColor.Color) \t\t\tend) \t\t\t \t\t\tProperties.BrickColorEditor = editor \t\tend \t\tlocal entry = propEntries[entryIndex] \t\tlocal valueFrame = entry.GuiElems.ValueFrame \t\teditor.CurrentProp = prop \t\teditor.SavedColor = col \t\tif prop and prop.Class == \"BasePart\" and prop.Name == \"BrickColor\" then \t\t\teditor:SetMoreColorsVisible(true) \t\telse \t\t\teditor:SetMoreColorsVisible(false) \t\tend \t\teditor:Show(valueFrame.AbsolutePosition.X, valueFrame.AbsolutePosition.Y + 22) \tend \tProperties.DisplayColorEditor = function(prop,col) \t\tlocal editor = Properties.ColorEditor \t\tif not editor then \t\t\teditor = Lib.ColorPicker.new() \t\t\t \t\t\teditor.OnSelect:Connect(function(col) \t\t\t\tif not editor.CurrentProp then return end \t\t\t\tlocal typeName = editor.CurrentProp.ValueType.Name \t\t\t\tif typeName ~= \"Color3\" and typeName ~= \"BrickColor\" then return end \t\t\t\tlocal colVal = (typeName == \"Color3\" and col or BrickColor.new(col)) \t\t\t\tif editor.CurrentProp == inputProp then inputProp = nil end \t\t\t\tProperties.SetProp(editor.CurrentProp,colVal) \t\t\tend) \t\t\t \t\t\tProperties.ColorEditor = editor \t\tend \t\teditor.CurrentProp = prop \t\tif col then \t\t\teditor:SetColor(col) \t\telse \t\t\tlocal firstVal = Properties.GetFirstPropVal(prop) \t\t\tif firstVal then editor:SetColor(firstVal) end \t\tend \t\teditor:Show() \tend \t \tProperties.DisplayNumberSequenceEditor = function(prop,seq) \t\tlocal editor = Properties.NumberSequenceEditor \t\tif not editor then \t\t\teditor = Lib.NumberSequenceEditor.new() \t\t\teditor.OnSelect:Connect(function(val) \t\t\t\tif not editor.CurrentProp or editor.CurrentProp.ValueType.Name ~= \"NumberSequence\" then return end \t\t\t\tif editor.CurrentProp == inputProp then inputProp = nil end \t\t\t\tProperties.SetProp(editor.CurrentProp,val) \t\t\tend) \t\t\tProperties.NumberSequenceEditor = editor \t\tend \t\teditor.CurrentProp = prop \t\tif seq then \t\t\teditor:SetSequence(seq) \t\telse \t\t\tlocal firstVal = Properties.GetFirstPropVal(prop) \t\t\tif firstVal then editor:SetSequence(firstVal) end \t\tend \t\teditor:Show() \tend \tProperties.DisplayColorSequenceEditor = function(prop,seq) \t\tlocal editor = Properties.ColorSequenceEditor \t\tif not editor then \t\t\teditor = Lib.ColorSequenceEditor.new() \t\t\teditor.OnSelect:Connect(function(val) \t\t\t\tif not editor.CurrentProp or editor.CurrentProp.ValueType.Name ~= \"ColorSequence\" then return end \t\t\t\tif editor.CurrentProp == inputProp then inputProp = nil end \t\t\t\tProperties.SetProp(editor.CurrentProp,val) \t\t\tend) \t\t\tProperties.ColorSequenceEditor = editor \t\tend \t\teditor.CurrentProp = prop \t\tif seq then \t\t\teditor:SetSequence(seq) \t\telse \t\t\tlocal firstVal = Properties.GetFirstPropVal(prop) \t\t\tif firstVal then editor:SetSequence(firstVal) end \t\tend \t\teditor:Show() \tend \tProperties.GetFirstPropVal = function(prop) \t\tlocal first = Properties.FindFirstObjWhichIsA(prop.Class) \t\tif first then \t\t\treturn Properties.GetPropVal(prop,first) \t\tend \tend \tProperties.GetPropVal = function(prop,obj) \t\tif prop.MultiType then return \"<Multiple Types>\" end \t\tif not obj then return end \t\tlocal propVal \t\tif prop.IsAttribute then \t\t\tpropVal = getAttribute(obj,prop.AttributeName) \t\t\tif propVal == nil then return nil end \t\t\tlocal typ = typeof(propVal) \t\t\tlocal currentType = Properties.TypeNameConvert[typ] or typ \t\t\tif prop.RootType then \t\t\t\tif prop.RootType.Name ~= currentType then \t\t\t\t\treturn nil \t\t\t\tend \t\t\telseif prop.ValueType.Name ~= currentType then \t\t\t\treturn nil \t\t\tend \t\telse \t\t\tpropVal = obj[prop.Name] \t\tend \t\tif prop.SubName then \t\t\tlocal indexes = string.split(prop.SubName,\".\") \t\t\tfor i = 1,#indexes do \t\t\t\tlocal indexName = indexes[i] \t\t\t\tif #indexName > 0 and propVal then \t\t\t\t\tpropVal = propVal[indexName] \t\t\t\tend \t\t\tend \t\tend \t\treturn propVal \tend \tProperties.SelectObject = function(obj) \t\tif inputProp and inputProp.ValueType.Category == \"Class\" then \t\t\tlocal prop = inputProp \t\t\tinputProp = nil \t\t\tif isa(obj,prop.ValueType.Name) then \t\t\t\tProperties.SetProp(prop,obj) \t\t\telse \t\t\t\tProperties.Refresh() \t\t\tend \t\t\treturn true \t\tend \t\treturn false \tend \tProperties.DisplayProp = function(prop,entryIndex) \t\tlocal propName = prop.Name \t\tlocal typeData = prop.ValueType \t\tlocal typeName = typeData.Name \t\tlocal tags = prop.Tags \t\tlocal gName = prop.Class..\".\"..prop.Name..(prop.SubName or \"\") \t\tlocal propObj = autoUpdateObjs[gName] \t\tlocal entryData = propEntries[entryIndex] \t\tlocal UDim2 = UDim2 \t\tlocal guiElems = entryData.GuiElems \t\tlocal valueFrame = guiElems.ValueFrame \t\tlocal valueBox = guiElems.ValueBox \t\tlocal colorButton = guiElems.ColorButton \t\tlocal colorPreview = guiElems.ColorPreview \t\tlocal gradient = guiElems.Gradient \t\tlocal enumArrow = guiElems.EnumArrow \t\tlocal checkbox = guiElems.Checkbox \t\tlocal rightButton = guiElems.RightButton \t\tlocal soundPreview = guiElems.SoundPreview \t\tlocal propVal = Properties.GetPropVal(prop,propObj) \t\tlocal inputFullName = inputProp and (inputProp.Class..\".\"..inputProp.Name..(inputProp.SubName or \"\")) \t\tlocal offset = 4 \t\tlocal endOffset = 6 \t\t-- Offsetting the ValueBox for ValueType specific buttons \t\tif (typeName == \"Color3\" or typeName == \"BrickColor\" or typeName == \"ColorSequence\") then \t\t\tcolorButton.Visible = true \t\t\tenumArrow.Visible = false \t\t\tif propVal then \t\t\t\tgradient.Color = (typeName == \"Color3\" and ColorSequence.new(propVal)) or (typeName == \"BrickColor\" and ColorSequence.new(propVal.Color)) or propVal \t\t\telse \t\t\t\tgradient.Color = ColorSequence.new(Color3.new(1,1,1)) \t\t\tend \t\t\tcolorPreview.BorderColor3 = (typeName == \"ColorSequence\" and Color3.new(1,1,1) or Color3.new(0,0,0)) \t\t\toffset = 22 \t\t\tendOffset = 24 + (typeName == \"ColorSequence\" and 20 or 0) \t\telseif typeData.Category == \"Enum\" then \t\t\tcolorButton.Visible = false \t\t\tenumArrow.Visible = not prop.Tags.ReadOnly \t\t\tendOffset = 22 \t\telseif (gName == inputFullName and typeData.Category == \"Class\") or typeName == \"NumberSequence\" then \t\t\tcolorButton.Visible = false \t\t\tenumArrow.Visible = false \t\t\tendOffset = 26 \t\telse \t\t\tcolorButton.Visible = false \t\t\tenumArrow.Visible = false \t\tend \t\tvalueBox.Position = UDim2.new(0,offset,0,0) \t\tvalueBox.Size = UDim2.new(1,-endOffset,1,0) \t\t-- Right button \t\tif inputFullName == gName and typeData.Category == \"Class\" then \t\t\tMain.MiscIcons:DisplayByKey(guiElems.RightButtonIcon, \"Delete\") \t\t\tguiElems.RightButtonIcon.Visible = true \t\t\trightButton.Text = \"\" \t\t\trightButton.Visible = true \t\telseif typeName == \"NumberSequence\" or typeName == \"ColorSequence\" then \t\t\tguiElems.RightButtonIcon.Visible = false \t\t\trightButton.Text = \"...\" \t\t\trightButton.Visible = true \t\telse \t\t\trightButton.Visible = false \t\tend \t\t-- Displays the correct ValueBox for the ValueType, and sets it to the prop value \t\tif typeName == \"bool\" or typeName == \"PhysicalProperties\" then \t\t\tvalueBox.Visible = false \t\t\tcheckbox.Visible = true \t\t\tsoundPreview.Visible = false \t\t\tcheckboxes[entryIndex].Disabled = tags.ReadOnly \t\t\tif typeName == \"PhysicalProperties\" and autoUpdateObjs[gName] then \t\t\t\tcheckboxes[entryIndex]:SetState(propVal and true or false) \t\t\telse \t\t\t\tcheckboxes[entryIndex]:SetState(propVal) \t\t\tend \t\telseif typeName == \"SoundPlayer\" then \t\t\tvalueBox.Visible = false \t\t\tcheckbox.Visible = false \t\t\tsoundPreview.Visible = true \t\t\tlocal playing = Properties.PreviewSound and Properties.PreviewSound.Playing \t\t\tMain.MiscIcons:DisplayByKey(soundPreview.ControlButton.Icon, playing and \"Pause\" or \"Play\") \t\telse \t\t\tvalueBox.Visible = true \t\t\tcheckbox.Visible = false \t\t\tsoundPreview.Visible = false \t\t\t \t\t\tif propVal ~= nil then \t\t\t\tif typeName == \"Color3\" then \t\t\t\t\tvalueBox.Text = \"[\"..Lib.ColorToBytes(propVal)..\"]\" \t\t\t\telseif typeData.Category == \"Enum\" then \t\t\t\t\tvalueBox.Text = propVal.Name \t\t\t\telseif Properties.RoundableTypes[typeName] and Settings.Properties.NumberRounding then \t\t\t\t\tlocal rawStr = Properties.ValueToString(prop,propVal) \t\t\t\t\tvalueBox.Text = rawStr:gsub(\"-?%d+%.%d+\",function(num) \t\t\t\t\t\treturn tostring(tonumber((\"%.\"..Settings.Properties.NumberRounding..\"f\"):format(num))) \t\t\t\t\tend) \t\t\t\telse \t\t\t\t\tvalueBox.Text = Properties.ValueToString(prop,propVal) \t\t\t\tend \t\t\telse \t\t\t\tvalueBox.Text = \"\" \t\t\tend \t\t\tvalueBox.TextColor3 = tags.ReadOnly and Settings.Theme.PlaceholderText or Settings.Theme.Text \t\tend \tend \tProperties.Refresh = function() \t\tlocal maxEntries = math.max(math.ceil((propsFrame.AbsoluteSize.Y) / 23),0)\t \t\tlocal maxX = propsFrame.AbsoluteSize.X \t\tlocal valueWidth = math.max(Properties.MinInputWidth,maxX-Properties.ViewWidth) \t\tlocal inputPropVisible = false \t\tlocal isa = game.IsA \t\tlocal UDim2 = UDim2 \t\tlocal stringSplit = string.split \t\tlocal scaleType = Settings.Properties.ScaleType \t\t-- Clear connections \t\tfor i = 1,#propCons do \t\t\tpropCons[i]:Disconnect() \t\tend \t\ttable.clear(propCons) \t\t-- Hide full name viewer \t\tProperties.FullNameFrame.Visible = false \t\tProperties.FullNameFrameAttach.Disable() \t\tfor i = 1,maxEntries do \t\t\tlocal entryData = propEntries[i] \t\t\tif not propEntries[i] then entryData = Properties.NewPropEntry(i) propEntries[i] = entryData end \t\t\tlocal entry = entryData.Gui \t\t\tlocal guiElems = entryData.GuiElems \t\t\tlocal nameFrame = guiElems.NameFrame \t\t\tlocal propNameLabel = guiElems.PropName \t\t\tlocal valueFrame = guiElems.ValueFrame \t\t\tlocal expand = guiElems.Expand \t\t\tlocal valueBox = guiElems.ValueBox \t\t\tlocal propNameBox = guiElems.PropName \t\t\tlocal rightButton = guiElems.RightButton \t\t\tlocal editAttributeButton = guiElems.EditAttributeButton \t\t\tlocal toggleAttributes = guiElems.ToggleAttributes \t\t\tlocal prop = viewList[i + Properties.Index] \t\t\tif prop then \t\t\t\tlocal entryXOffset = (scaleType == 0 and scrollH.Index or 0) \t\t\t\tentry.Visible = true \t\t\t\tentry.Position = UDim2.new(0,-entryXOffset,0,entry.Position.Y.Offset) \t\t\t\tentry.Size = UDim2.new(scaleType == 0 and 0 or 1, scaleType == 0 and Properties.ViewWidth + valueWidth or 0,0,22) \t\t\t\tif prop.SpecialRow then \t\t\t\t\tif prop.SpecialRow == \"AddAttribute\" then \t\t\t\t\t\tnameFrame.Visible = false \t\t\t\t\t\tvalueFrame.Visible = false \t\t\t\t\t\tguiElems.RowButton.Visible = true \t\t\t\t\tend \t\t\t\telse \t\t\t\t\t-- Revert special row stuff \t\t\t\t\tnameFrame.Visible = true \t\t\t\t\tguiElems.RowButton.Visible = false \t\t\t\t\tlocal depth = Properties.EntryIndent*(prop.Depth or 1) \t\t\t\t\tlocal leftOffset = depth + Properties.EntryOffset \t\t\t\t\tnameFrame.Position = UDim2.new(0,leftOffset,0,0) \t\t\t\t\tpropNameLabel.Size = UDim2.new(1,-2 - (scaleType == 0 and 0 or 6),1,0) \t\t\t\t\tlocal gName = (prop.CategoryName and \"CAT_\"..prop.CategoryName) or prop.Class..\".\"..prop.Name..(prop.SubName or \"\") \t\t\t\t\tif prop.CategoryName then \t\t\t\t\t\tentry.BackgroundColor3 = Settings.Theme.Main1 \t\t\t\t\t\tvalueFrame.Visible = false \t\t\t\t\t\tpropNameBox.Text = prop.CategoryName \t\t\t\t\t\tpropNameBox.Font = Enum.Font.SourceSansBold \t\t\t\t\t\texpand.Visible = true \t\t\t\t\t\tpropNameBox.TextColor3 = Settings.Theme.Text \t\t\t\t\t\tnameFrame.BackgroundTransparency = 1 \t\t\t\t\t\tnameFrame.Size = UDim2.new(1,0,1,0) \t\t\t\t\t\teditAttributeButton.Visible = false \t\t\t\t\t\tlocal showingAttrs = Settings.Properties.ShowAttributes \t\t\t\t\t\ttoggleAttributes.Position = UDim2.new(1,-85-leftOffset,0,0) \t\t\t\t\t\ttoggleAttributes.Text = (showingAttrs and \"[Setting: ON]\" or \"[Setting: OFF]\") \t\t\t\t\t\ttoggleAttributes.TextColor3 = Settings.Theme.Text \t\t\t\t\t\ttoggleAttributes.Visible = (prop.CategoryName == \"Attributes\") \t\t\t\t\telse \t\t\t\t\t\tlocal propName = prop.Name \t\t\t\t\t\tlocal typeData = prop.ValueType \t\t\t\t\t\tlocal typeName = typeData.Name \t\t\t\t\t\tlocal tags = prop.Tags \t\t\t\t\t\tlocal propObj = autoUpdateObjs[gName] \t\t\t\t\t\tlocal attributeOffset = (prop.IsAttribute and 20 or 0) \t\t\t\t\t\teditAttributeButton.Visible = (prop.IsAttribute and not prop.RootType) \t\t\t\t\t\ttoggleAttributes.Visible = false \t\t\t\t\t\t-- Moving around the frames \t\t\t\t\t\tif scaleType == 0 then \t\t\t\t\t\t\tnameFrame.Size = UDim2.new(0,Properties.ViewWidth - leftOffset - 1,1,0) \t\t\t\t\t\t\tvalueFrame.Position = UDim2.new(0,Properties.ViewWidth,0,0) \t\t\t\t\t\t\tvalueFrame.Size = UDim2.new(0,valueWidth - attributeOffset,1,0) \t\t\t\t\t\telse \t\t\t\t\t\t\tnameFrame.Size = UDim2.new(0.5,-leftOffset - 1,1,0) \t\t\t\t\t\t\tvalueFrame.Position = UDim2.new(0.5,0,0,0) \t\t\t\t\t\t\tvalueFrame.Size = UDim2.new(0.5,-attributeOffset,1,0) \t\t\t\t\t\tend \t\t\t\t\t\tlocal nameArr = stringSplit(gName,\".\") \t\t\t\t\t\tpropNameBox.Text = prop.DisplayName or nameArr[#nameArr] \t\t\t\t\t\tpropNameBox.Font = Enum.Font.SourceSans \t\t\t\t\t\tentry.BackgroundColor3 = Settings.Theme.Main2 \t\t\t\t\t\tvalueFrame.Visible = true \t\t\t\t\t\texpand.Visible = typeData.Category == \"DataType\" and Properties.ExpandableTypes[typeName] or Properties.ExpandableProps[gName] \t\t\t\t\t\tpropNameBox.TextColor3 = tags.ReadOnly and Settings.Theme.PlaceholderText or Settings.Theme.Text \t\t\t\t\t\t-- Display property value \t\t\t\t\t\tProperties.DisplayProp(prop,i) \t\t\t\t\t\tif propObj then \t\t\t\t\t\t\tif prop.IsAttribute then \t\t\t\t\t\t\t\tpropCons[#propCons+1] = getAttributeChangedSignal(propObj,prop.AttributeName):Connect(function() \t\t\t\t\t\t\t\t\tProperties.DisplayProp(prop,i) \t\t\t\t\t\t\t\tend) \t\t\t\t\t\t\telse \t\t\t\t\t\t\t\tpropCons[#propCons+1] = getPropChangedSignal(propObj,propName):Connect(function() \t\t\t\t\t\t\t\t\tProperties.DisplayProp(prop,i) \t\t\t\t\t\t\t\tend) \t\t\t\t\t\t\tend \t\t\t\t\t\tend \t\t\t\t\t\t-- Position and resize Input Box \t\t\t\t\t\tlocal beforeVisible = valueBox.Visible \t\t\t\t\t\tlocal inputFullName = inputProp and (inputProp.Class..\".\"..inputProp.Name..(inputProp.SubName or \"\")) \t\t\t\t\t\tif gName == inputFullName then \t\t\t\t\t\t\tnameFrame.BackgroundColor3 = Settings.Theme.ListSelection \t\t\t\t\t\t\tnameFrame.BackgroundTransparency = 0 \t\t\t\t\t\t\tif typeData.Category == \"Class\" or typeData.Category == \"Enum\" or typeName == \"BrickColor\" then \t\t\t\t\t\t\t\tvalueFrame.BackgroundColor3 = Settings.Theme.TextBox \t\t\t\t\t\t\t\tvalueFrame.BackgroundTransparency = 0 \t\t\t\t\t\t\t\tvalueBox.Visible = true \t\t\t\t\t\t\telse \t\t\t\t\t\t\t\tinputPropVisible = true \t\t\t\t\t\t\t\tlocal scale = (scaleType == 0 and 0 or 0.5) \t\t\t\t\t\t\t\tlocal offset = (scaleType == 0 and Properties.ViewWidth-scrollH.Index or 0) \t\t\t\t\t\t\t\tlocal endOffset = 0 \t\t\t\t\t\t\t\tif typeName == \"Color3\" or typeName == \"ColorSequence\" then \t\t\t\t\t\t\t\t\toffset = offset + 22 \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tif typeName == \"NumberSequence\" or typeName == \"ColorSequence\" then \t\t\t\t\t\t\t\t\tendOffset = 20 \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tinputBox.Position = UDim2.new(scale,offset,0,entry.Position.Y.Offset) \t\t\t\t\t\t\t\tinputBox.Size = UDim2.new(1-scale,-offset-endOffset-attributeOffset,0,22) \t\t\t\t\t\t\t\tinputBox.Visible = true \t\t\t\t\t\t\t\tvalueBox.Visible = false \t\t\t\t\t\t\tend \t\t\t\t\t\telse \t\t\t\t\t\t\tnameFrame.BackgroundColor3 = Settings.Theme.Main1 \t\t\t\t\t\t\tnameFrame.BackgroundTransparency = 1 \t\t\t\t\t\t\tvalueFrame.BackgroundColor3 = Settings.Theme.Main1 \t\t\t\t\t\t\tvalueFrame.BackgroundTransparency = 1 \t\t\t\t\t\t\tvalueBox.Visible = beforeVisible \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\t-- Expand \t\t\t\t\tif prop.CategoryName or Properties.ExpandableTypes[prop.ValueType and prop.ValueType.Name] or Properties.ExpandableProps[gName] then \t\t\t\t\t\tif Lib.CheckMouseInGui(expand) then \t\t\t\t\t\t\tMain.MiscIcons:DisplayByKey(expand.Icon, expanded[gName] and \"Collapse_Over\" or \"Expand_Over\") \t\t\t\t\t\telse \t\t\t\t\t\t\tMain.MiscIcons:DisplayByKey(expand.Icon, expanded[gName] and \"Collapse\" or \"Expand\") \t\t\t\t\t\tend \t\t\t\t\t\texpand.Visible = true \t\t\t\t\telse \t\t\t\t\t\texpand.Visible = false \t\t\t\t\tend \t\t\t\tend \t\t\t\tentry.Visible = true \t\t\telse \t\t\t\tentry.Visible = false \t\t\tend \t\tend \t\tif not inputPropVisible then \t\t\tinputBox.Visible = false \t\tend \t\tfor i = maxEntries+1,#propEntries do \t\t\tpropEntries[i].Gui:Destroy() \t\t\tpropEntries[i] = nil \t\t\tcheckboxes[i] = nil \t\tend \tend \tProperties.SetProp = function(prop,val,noupdate,prevAttribute) \t\tlocal sList = Explorer.Selection.List \t\tlocal propName = prop.Name \t\tlocal subName = prop.SubName \t\tlocal propClass = prop.Class \t\tlocal typeData = prop.ValueType \t\tlocal typeName = typeData.Name \t\tlocal attributeName = prop.AttributeName \t\tlocal rootTypeData = prop.RootType \t\tlocal rootTypeName = rootTypeData and rootTypeData.Name \t\tlocal fullName = prop.Class..\".\"..prop.Name..(prop.SubName or \"\") \t\tlocal Vector3 = Vector3 \t\tfor i = 1,#sList do \t\t\tlocal node = sList[i] \t\t\tlocal obj = node.Obj \t\t\tif isa(obj,propClass) then \t\t\t\tpcall(function() \t\t\t\t\tlocal setVal = val \t\t\t\t\tlocal root \t\t\t\t\tif prop.IsAttribute then \t\t\t\t\t\troot = getAttribute(obj,attributeName) \t\t\t\t\telse \t\t\t\t\t\troot = obj[propName] \t\t\t\t\tend \t\t\t\t\tif prevAttribute then \t\t\t\t\t\tif prevAttribute.ValueType.Name == typeName then \t\t\t\t\t\t\tsetVal = getAttribute(obj,prevAttribute.AttributeName) or setVal \t\t\t\t\t\tend \t\t\t\t\t\tsetAttribute(obj,prevAttribute.AttributeName,nil) \t\t\t\t\tend \t\t\t\t\tif rootTypeName then \t\t\t\t\t\tif rootTypeName == \"Vector2\" then \t\t\t\t\t\t\tsetVal = Vector2.new((subName == \".X\" and setVal) or root.X, (subName == \".Y\" and setVal) or root.Y) \t\t\t\t\t\telseif rootTypeName == \"Vector3\" then \t\t\t\t\t\t\tsetVal = Vector3.new((subName == \".X\" and setVal) or root.X, (subName == \".Y\" and setVal) or root.Y, (subName == \".Z\" and setVal) or root.Z) \t\t\t\t\t\telseif rootTypeName == \"UDim\" then \t\t\t\t\t\t\tsetVal = UDim.new((subName == \".Scale\" and setVal) or root.Scale, (subName == \".Offset\" and setVal) or root.Offset) \t\t\t\t\t\telseif rootTypeName == \"UDim2\" then \t\t\t\t\t\t\tlocal rootX,rootY = root.X,root.Y \t\t\t\t\t\t\tlocal X_UDim = (subName == \".X\" and setVal) or UDim.new((subName == \".X.Scale\" and setVal) or rootX.Scale, (subName == \".X.Offset\" and setVal) or rootX.Offset) \t\t\t\t\t\t\tlocal Y_UDim = (subName == \".Y\" and setVal) or UDim.new((subName == \".Y.Scale\" and setVal) or rootY.Scale, (subName == \".Y.Offset\" and setVal) or rootY.Offset) \t\t\t\t\t\t\tsetVal = UDim2.new(X_UDim,Y_UDim) \t\t\t\t\t\telseif rootTypeName == \"CFrame\" then \t\t\t\t\t\t\tlocal rootPos,rootRight,rootUp,rootLook = root.Position,root.RightVector,root.UpVector,root.LookVector \t\t\t\t\t\t\tlocal pos = (subName == \".Position\" and setVal) or Vector3.new((subName == \".Position.X\" and setVal) or rootPos.X, (subName == \".Position.Y\" and setVal) or rootPos.Y, (subName == \".Position.Z\" and setVal) or rootPos.Z) \t\t\t\t\t\t\tlocal rightV = (subName == \".RightVector\" and setVal) or Vector3.new((subName == \".RightVector.X\" and setVal) or rootRight.X, (subName == \".RightVector.Y\" and setVal) or rootRight.Y, (subName == \".RightVector.Z\" and setVal) or rootRight.Z) \t\t\t\t\t\t\tlocal upV = (subName == \".UpVector\" and setVal) or Vector3.new((subName == \".UpVector.X\" and setVal) or rootUp.X, (subName == \".UpVector.Y\" and setVal) or rootUp.Y, (subName == \".UpVector.Z\" and setVal) or rootUp.Z) \t\t\t\t\t\t\tlocal lookV = (subName == \".LookVector\" and setVal) or Vector3.new((subName == \".LookVector.X\" and setVal) or rootLook.X, (subName == \".RightVector.Y\" and setVal) or rootLook.Y, (subName == \".RightVector.Z\" and setVal) or rootLook.Z) \t\t\t\t\t\t\tsetVal = CFrame.fromMatrix(pos,rightV,upV,-lookV) \t\t\t\t\t\telseif rootTypeName == \"Rect\" then \t\t\t\t\t\t\tlocal rootMin,rootMax = root.Min,root.Max \t\t\t\t\t\t\tlocal min = Vector2.new((subName == \".Min.X\" and setVal) or rootMin.X, (subName == \".Min.Y\" and setVal) or rootMin.Y) \t\t\t\t\t\t\tlocal max = Vector2.new((subName == \".Max.X\" and setVal) or rootMax.X, (subName == \".Max.Y\" and setVal) or rootMax.Y) \t\t\t\t\t\t\tsetVal = Rect.new(min,max) \t\t\t\t\t\telseif rootTypeName == \"PhysicalProperties\" then \t\t\t\t\t\t\tlocal rootProps = PhysicalProperties.new(obj.Material) \t\t\t\t\t\t\tlocal density = (subName == \".Density\" and setVal) or (root and root.Density) or rootProps.Density \t\t\t\t\t\t\tlocal friction = (subName == \".Friction\" and setVal) or (root and root.Friction) or rootProps.Friction \t\t\t\t\t\t\tlocal elasticity = (subName == \".Elasticity\" and setVal) or (root and root.Elasticity) or rootProps.Elasticity \t\t\t\t\t\t\tlocal frictionWeight = (subName == \".FrictionWeight\" and setVal) or (root and root.FrictionWeight) or rootProps.FrictionWeight \t\t\t\t\t\t\tlocal elasticityWeight = (subName == \".ElasticityWeight\" and setVal) or (root and root.ElasticityWeight) or rootProps.ElasticityWeight \t\t\t\t\t\t\tsetVal = PhysicalProperties.new(density,friction,elasticity,frictionWeight,elasticityWeight) \t\t\t\t\t\telseif rootTypeName == \"Ray\" then \t\t\t\t\t\t\tlocal rootOrigin,rootDirection = root.Origin,root.Direction \t\t\t\t\t\t\tlocal origin = (subName == \".Origin\" and setVal) or Vector3.new((subName == \".Origin.X\" and setVal) or rootOrigin.X, (subName == \".Origin.Y\" and setVal) or rootOrigin.Y, (subName == \".Origin.Z\" and setVal) or rootOrigin.Z) \t\t\t\t\t\t\tlocal direction = (subName == \".Direction\" and setVal) or Vector3.new((subName == \".Direction.X\" and setVal) or rootDirection.X, (subName == \".Direction.Y\" and setVal) or rootDirection.Y, (subName == \".Direction.Z\" and setVal) or rootDirection.Z) \t\t\t\t\t\t\tsetVal = Ray.new(origin,direction) \t\t\t\t\t\telseif rootTypeName == \"Faces\" then \t\t\t\t\t\t\tlocal faces = {} \t\t\t\t\t\t\tlocal faceList = {\"Back\",\"Bottom\",\"Front\",\"Left\",\"Right\",\"Top\"} \t\t\t\t\t\t\tfor _,face in pairs(faceList) do \t\t\t\t\t\t\t\tlocal val \t\t\t\t\t\t\t\tif subName == \".\"..face then \t\t\t\t\t\t\t\t\tval = setVal \t\t\t\t\t\t\t\telse \t\t\t\t\t\t\t\t\tval = root[face] \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tif val then faces[#faces+1] = Enum.NormalId[face] end \t\t\t\t\t\t\tend \t\t\t\t\t\t\tsetVal = Faces.new(unpack(faces)) \t\t\t\t\t\telseif rootTypeName == \"Axes\" then \t\t\t\t\t\t\tlocal axes = {} \t\t\t\t\t\t\tlocal axesList = {\"X\",\"Y\",\"Z\"} \t\t\t\t\t\t\tfor _,axe in pairs(axesList) do \t\t\t\t\t\t\t\tlocal val \t\t\t\t\t\t\t\tif subName == \".\"..axe then \t\t\t\t\t\t\t\t\tval = setVal \t\t\t\t\t\t\t\telse \t\t\t\t\t\t\t\t\tval = root[axe] \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\tif val then axes[#axes+1] = Enum.Axis[axe] end \t\t\t\t\t\t\tend \t\t\t\t\t\t\tsetVal = Axes.new(unpack(axes)) \t\t\t\t\t\telseif rootTypeName == \"NumberRange\" then \t\t\t\t\t\t\tsetVal = NumberRange.new(subName == \".Min\" and setVal or root.Min, subName == \".Max\" and setVal or root.Max) \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\tif typeName == \"PhysicalProperties\" and setVal then \t\t\t\t\t\tsetVal = root or PhysicalProperties.new(obj.Material) \t\t\t\t\tend \t\t\t\t\tif prop.IsAttribute then \t\t\t\t\t\tsetAttribute(obj,attributeName,setVal) \t\t\t\t\telse \t\t\t\t\t\tobj[propName] = setVal \t\t\t\t\tend \t\t\t\tend) \t\t\tend \t\tend \t\tif not noupdate then \t\t\tProperties.ComputeConflicts(prop) \t\tend \tend \tProperties.InitInputBox = function() \t\tinputBox = create({ \t\t\t{1,\"Frame\",{BackgroundColor3=Color3.new(0.14901961386204,0.14901961386204,0.14901961386204),BorderSizePixel=0,Name=\"InputBox\",Size=UDim2.new(0,200,0,22),Visible=false,ZIndex=2,}}, \t\t\t{2,\"TextBox\",{BackgroundColor3=Color3.new(0.17647059261799,0.17647059261799,0.17647059261799),BackgroundTransparency=1,BorderColor3=Color3.new(0.062745101749897,0.51764708757401,1),BorderSizePixel=0,ClearTextOnFocus=false,Font=3,Parent={1},PlaceholderColor3=Color3.new(0.69803923368454,0.69803923368454,0.69803923368454),Position=UDim2.new(0,3,0,0),Size=UDim2.new(1,-6,1,0),Text=\"\",TextColor3=Color3.new(1,1,1),TextSize=14,TextXAlignment=0,ZIndex=2,}}, \t\t}) \t\tinputTextBox = inputBox.TextBox \t\tinputBox.BackgroundColor3 = Settings.Theme.TextBox \t\tinputBox.Parent = Properties.Window.GuiElems.Content.List \t\tinputTextBox.FocusLost:Connect(function() \t\t\tif not inputProp then return end \t\t\tlocal prop = inputProp \t\t\tinputProp = nil \t\t\tlocal val = Properties.StringToValue(prop,inputTextBox.Text) \t\t\tif val then Properties.SetProp(prop,val) else Properties.Refresh() end \t\tend) \t\tinputTextBox.Focused:Connect(function() \t\t\tinputTextBox.SelectionStart = 1 \t\t\tinputTextBox.CursorPosition = #inputTextBox.Text + 1 \t\tend) \t\tLib.ViewportTextBox.convert(inputTextBox) \tend \tProperties.SetInputProp = function(prop,entryIndex,special) \t\tlocal typeData = prop.ValueType \t\tlocal typeName = typeData.Name \t\tlocal fullName = prop.Class..\".\"..prop.Name..(prop.SubName or \"\") \t\tlocal propObj = autoUpdateObjs[fullName] \t\tlocal propVal = Properties.GetPropVal(prop,propObj) \t\tif prop.Tags.ReadOnly then return end \t\tinputProp = prop \t\tif special then \t\t\tif special == \"color\" then \t\t\t\tif typeName == \"Color3\" then \t\t\t\t\tinputTextBox.Text = propVal and Properties.ValueToString(prop,propVal) or \"\" \t\t\t\t\tProperties.DisplayColorEditor(prop,propVal) \t\t\t\telseif typeName == \"BrickColor\" then \t\t\t\t\tProperties.DisplayBrickColorEditor(prop,entryIndex,propVal) \t\t\t\telseif typeName == \"ColorSequence\" then \t\t\t\t\tinputTextBox.Text = propVal and Properties.ValueToString(prop,propVal) or \"\" \t\t\t\t\tProperties.DisplayColorSequenceEditor(prop,propVal) \t\t\t\tend \t\t\telseif special == \"right\" then \t\t\t\tif typeName == \"NumberSequence\" then \t\t\t\t\tinputTextBox.Text = propVal and Properties.ValueToString(prop,propVal) or \"\" \t\t\t\t\tProperties.DisplayNumberSequenceEditor(prop,propVal) \t\t\t\telseif typeName == \"ColorSequence\" then \t\t\t\t\tinputTextBox.Text = propVal and Properties.ValueToString(prop,propVal) or \"\" \t\t\t\t\tProperties.DisplayColorSequenceEditor(prop,propVal) \t\t\t\tend \t\t\tend \t\telse \t\t\tif Properties.IsTextEditable(prop) then \t\t\t\tinputTextBox.Text = propVal and Properties.ValueToString(prop,propVal) or \"\" \t\t\t\tinputTextBox:CaptureFocus() \t\t\telseif typeData.Category == \"Enum\" then \t\t\t\tProperties.DisplayEnumDropdown(entryIndex) \t\t\telseif typeName == \"BrickColor\" then \t\t\t\tProperties.DisplayBrickColorEditor(prop,entryIndex,propVal) \t\t\tend \t\tend \t\tProperties.Refresh() \tend \tProperties.InitSearch = function() \t\tlocal TweenService = service.TweenService \t\tlocal SearchFrame = Properties.GuiElems.ToolBar.SearchFrame \t\tlocal searchBox = SearchFrame.SearchBox \t\t \t\tlocal TweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quint) \t\t \t\tlocal Tweens = { \t\t\tStart = TweenService:Create(SearchFrame.UIStroke, TweenInfo, { Color = Color3.fromRGB(0, 120, 215) }), \t\t\tEnd = TweenService:Create(SearchFrame.UIStroke, TweenInfo, { Color = Color3.fromRGB(42, 42, 42) }) \t\t} \t\t \t\tLib.ViewportTextBox.convert(searchBox) \t\t \t\tsearchBox.FocusLost:Connect(function() Tweens.End:Play() end) \t\tsearchBox.Focused:Connect(function() Tweens.Start:Play() end) \t\t \t\tsearchBox:GetPropertyChangedSignal(\"Text\"):Connect(function() \t\t\tProperties.SearchText = searchBox.Text \t\t\tProperties.Update() \t\t\tProperties.Refresh() \t\tend) \tend \tProperties.InitEntryStuff = function() \t\tProperties.EntryTemplate = create({ \t\t\t{1,\"TextButton\",{AutoButtonColor=false,BackgroundColor3=Color3.new(0.17647059261799,0.17647059261799,0.17647059261799),BorderColor3=Color3.new(0.1294117718935,0.1294117718935,0.1294117718935),Font=3,Name=\"Entry\",Position=UDim2.new(0,1,0,1),Size=UDim2.new(0,250,0,22),Text=\"\",TextSize=14,}}, \t\t\t{2,\"Frame\",{BackgroundColor3=Color3.new(0.04313725605607,0.35294118523598,0.68627452850342),BackgroundTransparency=1,BorderColor3=Color3.new(0.33725491166115,0.49019610881805,0.73725491762161),BorderSizePixel=0,Name=\"NameFrame\",Parent={1},Position=UDim2.new(0,20,0,0),Size=UDim2.new(1,-40,1,0),}}, \t\t\t{3,\"TextLabel\",{BackgroundColor3=Color3.new(1,1,1),BackgroundTransparency=1,Font=3,Name=\"PropName\",Parent={2},Position=UDim2.new(0,2,0,0),Size=UDim2.new(1,-2,1,0),Text=\"Anchored\",TextColor3=Color3.new(1,1,1),TextSize=14,TextTransparency=0.10000000149012,TextTr
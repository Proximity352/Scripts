      "Script": "-- Configuration variables\nlocal config = {\n    fpsCap = 9999,\n    disableChat = false,            -- Hide the chat if enabled\n    enableBigButton = false,        -- Make the shake button bigger\n    bigButtonScaleFactor = 1,      -- Button size multiplier\n    shakeSpeed = 0.01,             -- Lower = faster shaking (0.05 = fast, 0.1 = normal)\n    FreezeWhileFishing = false      -- Freeze character while fishing\n}\n\n-- Set FPS cap\nsetfpscap(config.fpsCap)\n\n-- Services\nlocal players = game:GetService(\"Players\")\nlocal vim = game:GetService(\"VirtualInputManager\")\nlocal run_service = game:GetService(\"RunService\")\nlocal replicated_storage = game:GetService(\"ReplicatedStorage\")\nlocal localplayer = players.LocalPlayer\nlocal playergui = localplayer:FindFirstChild(\"PlayerGui\")\nlocal StarterGui = game:GetService(\"StarterGui\")\n\n-- Disable chat if the option is enabled in config\nif config.disableChat then\n    StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Chat, false)\nend\n\n-- Utility functions\nlocal utility = {blacklisted_attachments = {\"bob\", \"bodyweld\"}}; do\n    function utility.simulate_click(button)\n        if button and button:IsA(\"ImageButton\") then\n            vim:SendMouseButtonEvent(\n                button.AbsolutePosition.X + button.AbsoluteSize.X / 2,\n                button.AbsolutePosition.Y + button.AbsoluteSize.Y / 2,\n                0, true, game, 1\n            )\n            vim:SendMouseButtonEvent(\n                button.AbsolutePosition.X + button.AbsoluteSize.X / 2,\n                button.AbsolutePosition.Y + button.AbsoluteSize.Y / 2,\n                0, false, game, 1\n            )\n        end\n    end\n\n    function utility.move_fix(bobber)\n        for _, value in pairs(bobber:GetDescendants()) do\n            if value:IsA(\"Attachment\") and table.find(utility.blacklisted_attachments, value.Name) then\n                value:Destroy()\n            end\n        end\n    end\nend\n\n-- Fishing functions\nlocal farm = {reel_tick = nil, cast_tick = nil}; do\n    function farm.find_rod()\n        local character = localplayer.Character\n        if not character then return nil end\n\n        for _, tool in ipairs(character:GetChildren()) do\n            if tool:IsA(\"Tool\") and (tool.Name:find(\"rod\") or tool.Name:find(\"Rod\")) then\n                return tool\n            end\n        end\n        return nil\n    end\n\n    function farm.freeze_character(freeze)\n        local character = localplayer.Character\n        if character then\n            local humanoid = character:FindFirstChildOfClass(\"Humanoid\")\n            if humanoid then\n                if freeze then\n                    humanoid.WalkSpeed = 0\n                    humanoid.JumpPower = 0\n                else\n                    humanoid.WalkSpeed = 16  -- Default WalkSpeed\n                    humanoid.JumpPower = 50  -- Default JumpPower\n                end\n            end\n        end\n    end\n\n    function farm.cast()\n        local rod = farm.find_rod()\n        if not rod then return end\n\n        rod.events.cast:FireServer(100, 1)\n        farm.cast_tick = 0\n    end\n\n    function farm.shake()\n        local shake_ui = playergui and playergui:FindFirstChild(\"shakeui\")\n        if shake_ui then\n            local safezone = shake_ui:FindFirstChild(\"safezone\")\n            local button = safezone and safezone:FindFirstChild(\"button\")\n\n            if button then\n                if config.enableBigButton then\n                    button.Size = UDim2.new(config.bigButtonScaleFactor, 0, config.bigButtonScaleFactor, 0)\n                else\n                    button.Size = UDim2.new(1, 0, 1, 0)  -- Default size\n                end\n\n                if button.Visible then\n                    utility.simulate_click(button)\n                end\n            end\n        end\n    end\n\n    function farm.reel()\n        local reel_ui = playergui and playergui:FindFirstChild(\"reel\")\n        if not reel_ui then return end\n\n        local reel_bar = reel_ui:FindFirstChild(\"bar\")\n        if not reel_bar then return end\n\n        local reel_client = reel_bar:FindFirstChild(\"reel\")\n        if not reel_client then return end\n\n        if reel_client.Disabled then\n            reel_client.Disabled = false\n        end\n\n        local update_colors = getsenv(reel_client).UpdateColors\n        if update_colors then\n            setupvalue(update_colors, 1, 100)\n            replicated_storage.events.reelfinished:FireServer(getupvalue(update_colors, 1), true)\n        end\n    end\nend\n\n-- Toggle Variables\nlocal autoCast = false\nlocal autoShake = false\nlocal autoReel = false\n\n-- Main loop with toggles\ntask.spawn(function()\n    while task.wait(config.shakeSpeed) do\n        local rod = farm.find_rod()\n        if rod then\n            if config.FreezeWhileFishing and autoCast then\n                farm.freeze_character(true)\n            end\n\n            if autoCast then farm.cast() end\n            if autoShake then farm.shake() end\n            if autoReel then farm.reel() end\n        else\n            if not autoCast and not autoShake and not autoReel then\n                farm.freeze_character(false)\n            end\n        end\n    end\nend)\n\n-- Load UI Library\nlocal lib = loadstring(game:HttpGet(\"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/Vape.txt\"))()\nlocal win = lib:Window(\"Forky Hub V0.02\", Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)\nlocal tab = win:Tab(\"Main\")\n\n-- Connect Toggles\ntab:Toggle(\"Auto Cast\", false, function(value)\n    autoCast = value\nend)\n\ntab:Toggle(\"Auto Shake\", false, function(value)\n    autoShake = value\nend)\n\ntab:Toggle(\"Auto Reel\", false, function(value)\n    autoReel = value\nend)\n\nlocal tab = win:Tab(\"Automation\")\n\n-- Bait List with Prices\nlocal baitOptions = {\n    {name = \"Bait Crate\", price = 120},\n    {name = \"Quality Bait\", price = 525}\n}\n\nlocal selectedBait = nil\nlocal autoBuyEnabled = false\n\n-- Convert Bait Table into Dropdown List with Price\nlocal baitDropdown = {}\nfor _, bait in ipairs(baitOptions) do\n    table.insert(baitDropdown, bait.name .. \" - \" .. bait.price .. \"C$\")\nend\n\n-- Dropdown for Selecting Bait (Shows Bait Name + Price)\ntab:Dropdown(\"Bait\", baitDropdown, function(Value)\n    for _, bait in ipairs(baitOptions) do\n        if Value == (bait.name .. \" - \" .. bait.price .. \"C$\") then\n            selectedBait = bait\n            lib:Notification(\"Bait Selected\", \"You selected: \" .. bait.name .. \"\\nPrice: \" .. bait.price .. \" C$\", \"OK\")\n            break\n        end\n    end\nend)\n\n-- Instant Buy Function\nlocal function buyBait()\n    if selectedBait then\n        replicated_storage.events.buyBait:FireServer(selectedBait.name, selectedBait.price)\n        lib:Notification(\"Purchase\", \"Bought \" .. selectedBait.name .. \" for \" .. selectedBait.price .. \" C$\", \"OK\")\n    else\n        lib:Notification(\"Error\", \"Select a bait first!\", \"OK\")\n    end\nend\n\n-- Buy Bait Button (Only Buys Once Per Click)\ntab:Button(\"Buy Bait\", function()\n    buyBait()\nend)\n\n-- Auto-Buy Toggle (Keeps Buying Until Turned Off)\ntab:Toggle(\"Auto Buy Bait\", false, function(Value)\n    autoBuyEnabled = Value\n    if autoBuyEnabled then\n        task.spawn(function()\n            while autoBuyEnabled do\n                buyBait()\n                task.wait(0.5) -- Faster delay for instant buying\n            end\n        end)\n    end\nend)\n\nlocal autoSellEnabled = false\nlocal sellEvent = Workspace:WaitForChild(\"world\"):WaitForChild(\"npcs\"):WaitForChild(\"Marc Merchant\"):WaitForChild(\"merchant\"):WaitForChild(\"sellall\")\n\n-- Function to sell all fish\nlocal function sellAllFish()\n    sellEvent:InvokeServer()\n    lib:Notification(\"Auto Sell\", \"Sold all fish in your backpack!\", \"OK\")\nend\n\n-- Function to sell currently held fish\nlocal function sellHeldFish()\n    local character = game.Players.LocalPlayer.Character\n    if character then\n        local tool = character:FindFirstChildOfClass(\"Tool\") -- Check if player is holding a tool (fish)\n        if tool then\n            sellEvent:InvokeServer(tool) -- Sell only the held fish\n            lib:Notification(\"Sell Fish\", \"Sold \" .. tool.Name .. \"!\", \"OK\")\n        else\n            lib:Notification(\"Error\", \"You're not holding a fish!\", \"OK\")\n        end\n    end\nend\n\n-- Auto-Sell Toggle (Keeps Selling Until Turned Off)\ntab:Toggle(\"Auto Sell Fish\", false, function(Value)\n    autoSellEnabled = Value\n    if autoSellEnabled then\n        task.spawn(function()\n            while autoSellEnabled do\n                sellAllFish()\n                task.wait(1) -- Prevents spamming\n            end\n        end)\n    end\nend)\n\n-- Sell Held Fish Button (Sells Only The Fish Player is Holding)\ntab:Button(\"Sell Held Fish\", function()\n    sellHeldFish()\nend)\n\nlocal tab = win:Tab(\"Event\")\n\nlocal function teleportPlayer(x, y, z)\n    local player = game.Players.LocalPlayer\n    if player and player.Character and player.Character:FindFirstChild(\"HumanoidRootPart\") then\n        player.Character.HumanoidRootPart.CFrame = CFrame.new(x, y, z)\n        lib:Notification(\"Notification\", \"Teleporting...\", \"Success!\")\n    else\n        lib:Notification(\"Notification\", \"Teleport Failed!\", \"Error!\")\n    end\nend\n\ntab:Button(\"Kraken Area\", function()\n    teleportPlayer(-4376.9, -996.3, 2036.1)\nend)\n\ntab:Button(\"Grand Reef\", function()\n    teleportPlayer(-3576, 151, 523)\nend)"
    },

er.BackgroundColor3 = self.Disabled and self.Colors.DisabledBackground or self.Colors.Background \t\t\t\tguiElems.Checkmark2.ImageColor3 = self.Disabled and self.Colors.DisabledCheck or self.Colors.Primary \t\t\tend \t\tend \t\tfuncs.SetState = function(self,val,anim) \t\t\tself.Toggled = val \t\t\tif self.OutlineColorTween then self.OutlineColorTween:Cancel() end \t\t\tlocal setStateTime = tick() \t\t\tself.LastSetStateTime = setStateTime \t\t\tif self.Toggled then \t\t\t\tif self.Style == 0 then \t\t\t\t\tif anim then \t\t\t\t\t\tself.OutlineColorTween = service.TweenService:Create(self.GuiElems.Outline, ti(4/15, Enum.EasingStyle.Circular, Enum.EasingDirection.Out), {BackgroundColor3 = self.Colors.Primary}) \t\t\t\t\t\tself.OutlineColorTween:Play() \t\t\t\t\t\tdelay(0.15, function() \t\t\t\t\t\t\tif setStateTime ~= self.LastSetStateTime then return end \t\t\t\t\t\t\tself:Paint() \t\t\t\t\t\t\tTweenSize(self.GuiElems.Checkmark, ud2o(14, 20), \"Out\", \"Bounce\", 2/15, true) \t\t\t\t\t\tend) \t\t\t\t\telse \t\t\t\t\t\tself.GuiElems.Outline.BackgroundColor3 = self.Colors.Primary \t\t\t\t\t\tself:Paint() \t\t\t\t\t\tself.GuiElems.Checkmark.Size = ud2o(14, 20) \t\t\t\t\tend \t\t\t\t\tself:Collapse(anim) \t\t\t\telse \t\t\t\t\tself:Paint() \t\t\t\t\tself.GuiElems.Checkmark2.Visible = true \t\t\t\t\tself.GuiElems.Middle.Visible = false \t\t\t\tend \t\t\telse \t\t\t\tif self.Style == 0 then \t\t\t\t\tif anim then \t\t\t\t\t\tself.OutlineColorTween = service.TweenService:Create(self.GuiElems.Outline, ti(4/15, Enum.EasingStyle.Circular, Enum.EasingDirection.In), {BackgroundColor3 = self.Colors.Secondary}) \t\t\t\t\t\tself.OutlineColorTween:Play() \t\t\t\t\t\tdelay(0.15, function() \t\t\t\t\t\t\tif setStateTime ~= self.LastSetStateTime then return end \t\t\t\t\t\t\tself:Paint() \t\t\t\t\t\t\tTweenSize(self.GuiElems.Checkmark, ud2o(0, 20), \"Out\", \"Quad\", 1/15, true) \t\t\t\t\t\tend) \t\t\t\t\telse \t\t\t\t\t\tself.GuiElems.Outline.BackgroundColor3 = self.Colors.Secondary \t\t\t\t\t\tself:Paint() \t\t\t\t\t\tself.GuiElems.Checkmark.Size = ud2o(0, 20) \t\t\t\t\tend \t\t\t\t\tself:Expand(anim) \t\t\t\telse \t\t\t\t\tself:Paint() \t\t\t\t\tself.GuiElems.Checkmark2.Visible = false \t\t\t\t\tself.GuiElems.Middle.Visible = self.Toggled == nil \t\t\t\tend \t\t\tend \t\tend \t\tlocal mt = {__index = funcs} \t\tlocal function new(style) \t\t\tlocal obj = setmetatable({ \t\t\t\tToggled = false, \t\t\t\tDisabled = false, \t\t\t\tOnInput = Lib.Signal.new(), \t\t\t\tStyle = style or 0, \t\t\t\tColors = { \t\t\t\t\tBackground = c3(36,36,36), \t\t\t\t\tPrimary = c3(49,176,230), \t\t\t\t\tSecondary = c3(25,25,25), \t\t\t\t\tDisabled = c3(64,64,64), \t\t\t\t\tDisabledBackground = c3(52,52,52), \t\t\t\t\tDisabledCheck = c3(80,80,80) \t\t\t\t} \t\t\t},mt) \t\t\tinitGui(obj) \t\t\treturn obj \t\tend \t\tlocal function fromFrame(frame) \t\t\tlocal obj = setmetatable({ \t\t\t\tToggled = false, \t\t\t\tDisabled = false, \t\t\t\tColors = { \t\t\t\t\tBackground = c3(36,36,36), \t\t\t\t\tPrimary = c3(49,176,230), \t\t\t\t\tSecondary = c3(25,25,25), \t\t\t\t\tDisabled = c3(64,64,64), \t\t\t\t\tDisabledBackground = c3(52,52,52) \t\t\t\t} \t\t\t},mt) \t\t\tinitGui(obj,frame) \t\t\treturn obj \t\tend \t\treturn {new = new, fromFrame} \tend)() \tLib.BrickColorPicker = (function() \t\tlocal funcs = {} \t\tlocal paletteCount = 0 \t\tlocal mouse = service.Players.LocalPlayer:GetMouse() \t\tlocal hexStartX = 4 \t\tlocal hexSizeX = 27 \t\tlocal hexTriangleStart = 1 \t\tlocal hexTriangleSize = 8 \t\tlocal bottomColors = { \t\t\tColor3.fromRGB(17,17,17), \t\t\tColor3.fromRGB(99,95,98), \t\t\tColor3.fromRGB(163,162,165), \t\t\tColor3.fromRGB(205,205,205), \t\t\tColor3.fromRGB(223,223,222), \t\t\tColor3.fromRGB(237,234,234), \t\t\tColor3.fromRGB(27,42,53), \t\t\tColor3.fromRGB(91,93,105), \t\t\tColor3.fromRGB(159,161,172), \t\t\tColor3.fromRGB(202,203,209), \t\t\tColor3.fromRGB(231,231,236), \t\t\tColor3.fromRGB(248,248,248) \t\t} \t\tlocal function isMouseInHexagon(hex, touchPos) \t\t\tlocal relativeX = touchPos.X - hex.AbsolutePosition.X \t\t\tlocal relativeY = touchPos.Y - hex.AbsolutePosition.Y \t\t\tif relativeX >= hexStartX and relativeX < hexStartX + hexSizeX then \t\t\t\trelativeX = relativeX - 4 \t\t\t\tlocal relativeWidth = (13 - math.min(relativeX, 26 - relativeX)) / 13 \t\t\t\tif relativeY >= hexTriangleStart + hexTriangleSize * relativeWidth and relativeY < hex.AbsoluteSize.Y - hexTriangleStart - hexTriangleSize * relativeWidth then \t\t\t\t\treturn true \t\t\t\tend \t\t\tend \t\t\treturn false \t\tend \t\t \t\tlocal function hexInput(self, hex, color) \t\t\thex.InputBegan:Connect(function(input) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\tif isMouseInHexagon(hex, input.Position) then \t\t\t\t\t\tself.OnSelect:Fire(color) \t\t\t\t\t\tself:Close() \t\t\t\t\tend \t\t\t\tend \t\t\tend) \t\t\t \t\t\thex.InputChanged:Connect(function(input) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\tif isMouseInHexagon(hex, input.Position) then \t\t\t\t\t\tself.OnPreview:Fire(color) \t\t\t\t\tend \t\t\t\tend \t\t\tend) \t\tend \t\t \t\tlocal function createGui(self) \t\t\tlocal gui = create({ \t\t\t\t{1,\"ScreenGui\",{Name=\"BrickColor\",}}, \t\t\t\t{2,\"Frame\",{Active=true,BackgroundColor3=Color3.new(0.17647059261799,0.17647059261799,0.17647059261799),BorderColor3=Color3.new(0.1294117718935,0.1294117718935,0.1294117718935),Parent={1},Position=UDim2.new(0.40000000596046,0,0.40000000596046,0),Size=UDim2.new(0,337,0,380),}}, \t\t\t\t{3,\"TextButton\",{BackgroundColor3=Color3.new(0.2352941185236,0.2352941185236,0.2352941185236),BorderColor3=Color3.new(0.21568627655506,0.21568627655506,0.21568627655506),BorderSizePixel=0,Font=3,Name=\"MoreColors\",Parent={2},Position=UDim2.new(0,5,1,-30),Size=UDim2.new(1,-10,0,25),Text=\"More Colors\",TextColor3=Color3.new(1,1,1),TextSiz
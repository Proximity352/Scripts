 Default = false,\n    Callback = function(value)\n        AutoBringPlayers = value\n        while AutoBringPlayers do\n            bringAllPlayersToMe() -- Bring all players to your location\n            wait(0.5) -- Add a delay to prevent too many teleports in a short time\n        end\n    end\n})\n\nMainTab:AddButton({\n    Name = \"Streamsnipa by zircon (fixed by lolbad)\",\n    Callback = streamsnipa\n})\n\nMainTab:AddToggle({\n    Name = \"Auto Sell Strength\",\n    Default = false,  -- Default state is off\n    Callback = function(value)\n        AutoSell = value\n        if value then\n            autoSellStrength()  -- Start the auto-selling if toggled on\n        end\n    end\n})\n\n-- TP to Map Section\nlocal TPMapTab = Window:MakeTab({Name = \"TP to Map\", Icon = \"rbxassetid://4483345998\", PremiumOnly = false})\n\nTPMapTab:AddButton({ Name = \"Main Map\", Callback = function() LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1685, 386, -1623) end })\nTPMapTab:AddButton({ Name = \"Map 2\", Callback = function() LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1673, 1343, 8693) end })\n\n-- TP to Pets Crystal Section\nlocal TPPetsTab = Window:MakeTab({Name = \"TP to Pets Crystal\", Icon = \"rbxassetid://4483345998\", PremiumOnly = false})\n\nTPPetsTab:AddButton({ Name = \"Common Pets\", Callback = function() LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1744, 387, -1577) end })\nTPPetsTab:AddButton({ Name = \"Uncommon Pets\", Callback = function() LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1564, 399, -2712) end })\nTPPetsTab:AddButton({ Name = \"Rare Pets\", Callback = function() LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2010, 440, -2747) end })\nTPPetsTab:AddButton({ Name = \"Epic Pets\", Callback = function() LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1493, 1339, 8572) end })\n\n-- TP to Pet Craft Section\nlocal TPCraftTab = Window:MakeTab({Name = \"TP to Pet Craft
0,1,0,1) \t\t\tlocal sequenceLine = Instance.new(\"Frame\") \t\t\tsequenceLine.BackgroundColor3 = Color3.new(0,0,0) \t\t\tsequenceLine.BorderSizePixel = 0 \t\t\tsequenceLine.Size = UDim2.new(0,1,0,0) \t\t\tfor i = 1,numberLine.AbsoluteSize.X do \t\t\t\tlocal line = sequenceLine:Clone() \t\t\t\teLines[i] = line \t\t\t\tline.Name = \"E\"..tostring(i) \t\t\t\tline.BackgroundTransparency = 0.5 \t\t\t\tline.BackgroundColor3 = Color3.new(199/255,44/255,28/255) \t\t\t\tline.Position = UDim2.new(0,i-1,0,0) \t\t\t\tline.Parent = numberLine \t\t\tend \t\t\tfor i = 1,numberLine.AbsoluteSize.X do \t\t\t\tlocal line = sequenceLine:Clone() \t\t\t\tlines[i] = line \t\t\t\tline.Name = tostring(i) \t\t\t\tline.Position = UDim2.new(0,i-1,0,0) \t\t\t\tline.Parent = numberLine \t\t\tend \t\t\tlocal envelopeDrag = Instance.new(\"Frame\") \t\t\tenvelopeDrag.BackgroundTransparency = 1 \t\t\tenvelopeDrag.BackgroundColor3 = Color3.new(0,0,0) \t\t\tenvelopeDrag.BorderSizePixel = 0 \t\t\tenvelopeDrag.Size = UDim2.new(0,7,0,20) \t\t\tenvelopeDrag.Visible = false \t\t\tenvelopeDrag.ZIndex = 2 \t\t\tlocal envelopeDragLine = Instance.new(\"Frame\",envelopeDrag) \t\t\tenvelopeDragLine.Name = \"Line\" \t\t\tenvelopeDragLine.BackgroundColor3 = Color3.new(0,0,0) \t\t\tenvelopeDragLine.BorderSizePixel = 0 \t\t\tenvelopeDragLine.Position = UDim2.new(0,3,0,0) \t\t\tenvelopeDragLine.Size = UDim2.new(0,1,0,20) \t\t\tenvelopeDragLine.ZIndex = 2 \t\t\tlocal envelopeDragTop,envelopeDragBottom = envelopeDrag:Clone(),envelopeDrag:Clone() \t\t\tenvelopeDragTop.Parent = numberLine \t\t\tenvelopeDragBottom.Parent = numberLine \t\t\tlocal function buildSequence() \t\t\t\tlocal newPoints = {} \t\t\t\tfor i,v in pairs(points) do \t\t\t\t\ttable.insert(newPoints,NumberSequenceKeypoint.new(v[2],v[1],v[3])) \t\t\t\tend \t\t\t\tnewMt.Sequence = NumberSequence.new(newPoints) \t\t\t\tnewMt.OnSelect:Fire(newMt.Sequence) \t\t\tend \t\t\tlocal function round(num,places) \t\t\t\tlocal multi = 10^places \t\t\t\treturn math.floor(num*multi + 0.5)/multi \t\t\tend \t\t\tlocal function updateInputs(point) \t\t\t\tif point then \t\t\t\t\tcurrentPoint = point \t\t\t\t\tlocal rawT,rawV,rawE = point[2],point[1],point[3] \t\t\t\t\ttimeBox.Text = round(rawT,(rawT < 0.01 and 5) or (rawT < 0.1 and 4) or 3) \t\t\t\t\tvalueBox.Text = round(rawV,(rawV < 0.01 and 5) or (rawV < 0.1 and 4) or (rawV < 1 and 3) or 2) \t\t\t\t\tenvelopeBox.Text = round(rawE,(rawE < 0.01 and 5) or (rawE < 0.1 and 4) or (rawV < 1 and 3) or 2) \t\t\t\t\tlocal envelopeDistance = numberLine.AbsoluteSize.Y*(point[3]/10) \t\t\t\t\tenvelopeDragTop.Position = UDim2.new(0,point[4].Position.X.Offset-1,0,point[4].Position.Y.Offset-envelopeDistance-17) \t\t\t\t\tenvelopeDragTop.Visible = true \t\t\t\t\tenvelopeDragBottom.Position = UDim2.new(0,point[4].Position.X.Offset-1,0,point[4].Position.Y.Offset+envelopeDistance+2) \t\t\t\t\tenvelopeDragBottom.Visible = true \t\t\t\tend \t\t\tend \t\t\tenvelopeDragTop.InputBegan:Connect(function(input) \t\t\t\tif (input.UserInputType ~= Enum.UserInputType.MouseButton1 and input.UserInputType ~= Enum.UserInputType.Touch) or not currentPoint or Lib.CheckMouseInGui(currentPoint[4].Select) then return end \t\t\t\t \t\t\t\tlocal mouseEvent, releaseEvent \t\t\t\tlocal maxSize = numberLine.AbsoluteSize.Y \t\t\t\tlocal mouseDelta = math.abs(envelopeDragTop.AbsolutePosition.Y - mouse.Y) \t\t\t\t \t\t\t\tenvelopeDragTop.Line.Position = UDim2.new(0, 2, 0, 0) \t\t\t\tenvelopeDragTop.Line.Size = UDim2.new(0, 3, 0, 20) \t\t\t\t \t\t\t\treleaseEvent = user.InputEnded:Connect(function(input) \t\t\t\t\tif input.UserInputType ~= Enum.UserInputType.MouseButton1 and input.UserInputType ~= Enum.UserInputType.Touch then return end \t\t\t\t\tmouseEvent:Disconnect() \t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\tenvelopeDragTop.Line.Position = UDim2.new(0, 3, 0, 0) \t\t\t\t\tenvelopeDragTop.Line.Size = UDim2.new(0, 1, 0, 20) \t\t\t\tend) \t\t\t\t \t\t\t\tmouseEvent = user.InputChanged:Connect(function(input) \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tlocal topDiff = (currentPoint[4].AbsolutePosition.Y + 2) - (mouse.Y - mouseDelta) - 19 \t\t\t\t\t\tlocal newEnvelope = 10 * (math.max(topDiff, 0) / maxSize) \t\t\t\t\t\tlocal maxEnvelope = math.min(currentPoint[1], 10 - currentPoint[1]) \t\t\t\t\t\tcurrentPoint[3] = math.min(newEnvelope, maxEnvelope) \t\t\t\t\t\tnewMt:Redraw() \t\t\t\t\t\tbuildSequence() \t\t\t\t\t\tupdateInputs(currentPoint) \t\t\t\t\tend \t\t\t\tend) \t\t\tend) \t\t\t \t\t\tenvelopeDragBottom.InputBegan:Connect(function(input) \t\t\t\tif (input.UserInputType ~= Enum.UserInputType.MouseButton1 and input.UserInputType ~= Enum.UserInputType.Touch) or not currentPoint or Lib.CheckMouseInGui(currentPoint[4].Select) then return end \t\t\t\t \t\t\t\tlocal mouseEvent, releaseEvent \t\t\t\tlocal maxSize = numberLine.AbsoluteSize.Y \t\t\t\tlocal mouseDelta = math.abs(envelopeDragBottom.AbsolutePosition.Y - mouse.Y) \t\t\t\t \t\t\t\tenvelopeDragBottom.Line.Position = UDim2.new(0, 2, 0, 0) \t\t\t\tenvelopeDragBottom.Line.Size = UDim2.new(0, 3, 0, 20) \t\t\t\t \t\t\t\treleaseEvent = user.InputEnded:Connect(function(input) \t\t\t\t\tif input.UserInputType ~= Enum.UserInputType.MouseButton1 and input.UserInputType ~= Enum.UserInputType.Touch then return end \t\t\t\t\tmouseEvent:Disconnect() \t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\tenvelopeDragBottom.Line.Position = UDim2.new(0, 3, 0, 0) \t\t\t\t\tenvelopeDragBottom.Line.Size = UDim2.new(0, 1, 0, 20) \t\t\t\tend) \t\t\t\t \t\t\t\tmouseEvent = user.InputChanged:Connect(function(input) \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tlocal bottomDiff = (mouse.Y + (20 - mouseDelta)) - (currentPoint[4].AbsolutePosition.Y + 2) - 19 \t\t\t\t\t\tlocal newEnvelope = 10 * (math.max(bottomDiff, 0) / maxSize) \t\t\t\t\t\tlocal maxEnvelope = math.min(currentPoint[1], 10 - currentPoint[1]) \t\t\t\t\t\tcurrentPoint[3] = math.min(newEnvelope, maxEnvelope) \t\t\t\t\t\tnewMt:Redraw() \t\t\t\t\t\tbuildSequence() \t\t\t\t\t\tupdateInputs(currentPoint) \t\t\t\t\tend \t\t\t\tend) \t\t\tend) \t\t\t \t\t\tlocal function placePoint(point) \t\t\t\tlocal newPoint = Instance.new(\"Frame\") \t\t\t\tnewPoint.Name = \"Point\" \t\t\t\tnewPoint.BorderSizePixel = 0 \t\t\t\tnewPoint.Size = UDim2.new(0,5,0,5) \t\t\t\tnewPoint.Position = UDim2.new(0,math.floor((numberLine.AbsoluteSize.X-1) * point[2])-2,0,numberLine.AbsoluteSize.Y*(10-point[1])/10-2) \t\t\t\tnewPoint.BackgroundColor3 = Color3.new(0,0,0) \t\t\t\tlocal newSelect = Instance.new(\"Frame\") \t\t\t\tnewSelect.Name = \"Select\" \t\t\t\tnewSelect.BackgroundTransparency = 1 \t\t\t\tnewSelect.BackgroundColor3 = Color3.new(199/255,44/255,28/255) \t\t\t\tnewSelect.Position = UDim2.new(0,-2,0,-2) \t\t\t\tnewSelect.Size = UDim2.new(0,9,0,9) \t\t\t\tnewSelect.Parent = newPoint \t\t\t\tnewPoint.Parent = numberLine \t\t\t\t \t\t\t\tnewSelect.InputBegan:Connect(function(input) \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tfor i, v in pairs(points) do \t\t\t\t\t\t\tv[4].Select.BackgroundTransparency = 1 \t\t\t\t\t\tend \t\t\t\t\t\t \t\t\t\t\t\tnewSelect.BackgroundTransparency = 0 \t\t\t\t\t\tupdateInputs(point) \t\t\t\t\tend \t\t\t\t\t \t\t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and not currentlySelected then \t\t\t\t\t\tcurrentPoint = point \t\t\t\t\t\tlocal mouseEvent, releaseEvent \t\t\t\t\t\tcurrentlySelected = true \t\t\t\t\t\tnewSelect.BackgroundColor3 = Color3.new(249/255, 191/255, 59/255) \t\t\t\t\t\t \t\t\t\t\t\tlocal oldEnvelope = point[3] \t\t\t\t\t\t \t\t\t\t\t\treleaseEvent = user.InputEnded:Connect(function(input) \t\t\t\t\t\t\tif input.UserInputType ~= Enum.UserInputType.MouseButton1 and input.UserInputType ~= Enum.UserInputType.Touch then\treturn end \t\t\t\t\t\t\t \t\t\t\t\t\t\tmouseEvent:Disconnect() \t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\tcurrentlySelected = nil \t\t\t\t\t\t\tnewSelect.BackgroundColor3 = Color3.new(199/255, 44/255, 28/255) \t\t\t\t\t\tend) \t\t\t\t\t\t \t\t\t\t\t\tmouseEvent = user.InputChanged:Connect(function(input) \t\t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\t\tlocal maxX = numberLine.AbsoluteSize.X - 1 \t\t\t\t\t\t\t\tlocal relativeX = (input.Position.X - numberLine.AbsolutePosition.X) \t\t\t\t\t\t\t\tif relativeX < 0 then relativeX = 0 end \t\t\t\t\t\t\t\tif relativeX > maxX then relativeX = maxX end \t\t\t\t\t\t\t\t \t\t\t\t\t\t\t\tlocal maxY = numberLine.AbsoluteSize.Y - 1 \t\t\t\t\t\t\t\tlocal relativeY = (input.Position.Y - numberLine.AbsolutePosition.Y) \t\t\t\t\t\t\t\tif relativeY < 0 then relativeY = 0 end \t\t\t\t\t\t\t\tif relativeY > maxY then relativeY = maxY end \t\t\t\t\t\t\t\t \t\t\t\t\t\t\t\tif point ~= beginPoint and point ~= endPoint then \t\t\t\t\t\t\t\t\tpoint[2] = relativeX / maxX \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t \t\t\t\t\t\t\t\tpoint[1] = 10 - (relativeY / maxY) * 10 \t\t\t\t\t\t\t\tlocal maxEnvelope = math.min(point[1], 10 - point[1]) \t\t\t\t\t\t\t\tpoint[3] = math.min(oldEnvelope, maxEnvelope) \t\t\t\t\t\t\t\tnewMt:Redraw() \t\t\t\t\t\t\t\tupdateInputs(point) \t\t\t\t\t\t\t\t \t\t\t\t\t\t\t\tfor i, v in pairs(points) do \t\t\t\t\t\t\t\t\tv[4].Select.BackgroundTransparency = 1 \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t \t\t\t\t\t\t\t\tnewSelect.BackgroundTransparency = 0 \t\t\t\t\t\t\t\tbuildSequence() \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t\t\tend \t\t\t\tend) \t\t\t\treturn newPoint \t\t\tend \t\t\tlocal function placePoints() \t\t\t\tfor i,v in pairs(points) do \t\t\t\t\tv[4] = placePoint(v) \t\t\t\tend \t\t\tend \t\t\tlocal function redraw(self) \t\t\t\tlocal numberLineSize = numberLine.AbsoluteSize \t\t\t\ttable.sort(points,function(a,b) return a[2] < b[2] end) \t\t\t\tfor i,v in pairs(points) do \t\t\t\t\tv[4].Position = UDim2.new(0,math.floor((numberLineSize.X-1) * v[2])-2,0,(numberLineSize.Y-1)*(10-v[1])/10-2) \t\t\t\tend \t\t\t\tlines[1].Size = UDim2.new(0,1,0,0) \t\t\t\tfor i = 1,#points-1 do \t\t\t\t\tlocal fromPoint = points[i] \t\t\t\t\tlocal toPoint = points[i+1] \t\t\t\t\tlocal deltaY = toPoint[4].Position.Y.Offset-fromPoint[4].Position.Y.Offset \t\t\t\t\tlocal deltaX = toPoint[4].Position.X.Offset-fromPoint[4].Position.X.Offset \t\t\t\t\tlocal slope = deltaY/deltaX \t\t\t\t\tlocal fromEnvelope = fromPoint[3] \t\t\t\t\tlocal nextEnvelope = toPoint[3] \t\t\t\t\tlocal currentRise = math.abs(slope) \t\t\t\t\tlocal totalRise = 0 \t\t\t\t\tlocal maxRise = math.abs(toPoint[4].Position.Y.Offset-fromPoint[4].Position.Y.Offset) \t\t\t\t\tfor lineCount = math.min(fromPoint[4].Position.X.Offset+1,toPoint[4].Position.X.Offset),toPoint[4].Position.X.Offset do \t\t\t\t\t\tif deltaX == 0 and deltaY == 0 then return end \t\t\t\t\t\tlocal riseNow = math.floor(currentRise) \t\t\t\t\t\tlocal line = lines[lineCount+3] \t\t\t\t\t\tif line then \t\t\t\t\t\t\tif totalRise+riseNow > maxRise then riseNow = maxRise-totalRise end \t\t\t\t\t\t\tif math.sign(slope) == -1 then \t\t\t\t\t\t\t\tline.Position = UDim2.new(0,lineCount+2,0,fromPoint[4].Position.Y.Offset + -(totalRise+riseNow)+2) \t\t\t\t\t\t\telse \t\t\t\t\t\t\t\tline.Position = UDim2.new(0,lineCount+2,0,fromPoint[4].Position.Y.Offset + totalRise+2) \t\t\t\t\t\t\tend \t\t\t\t\t\t\tline.Size = UDim2.new(0,1,0,math.max(riseNow,1)) \t\t\t\t\t\tend \t\t\t\t\t\ttotalRise = totalRise + riseNow \t\t\t\t\t\tcurrentRise = currentRise - riseNow + math.abs(slope) \t\t\t\t\t\tlocal envPercent = (lineCount-fromPoint[4].Position.X.Offset)/(toPoint[4].Position.X.Offset-fromPoint[4].Position.X.Offset) \t\t\t\t\t\tlocal envLerp = fromEnvelope+(nextEnvelope-fromEnvelope)*envPercent \t\t\t\t\t\tlocal relativeSize = (envLerp/10)*numberLineSize.Y\t\t\t\t\t\t \t\t\t\t\t\tlocal line = eLines[lineCount + 3] \t\t\t\t\t\tif line then \t\t\t\t\t\t\tline.Position = UDim2.new(0,lineCount+2,0,lines[lineCount+3].Position.Y.Offset-math.floor(relativeSize)) \t\t\t\t\t\t\tline.Size = UDim2.new(0,1,0,math.floor(relativeSize*2)) \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\tend \t\t\tend \t\t\tnewMt.Redraw = redraw \t\t\tlocal function loadSequence(self,seq) \t\t\t\tresetSequence = seq \t\t\t\tfor i,v in pairs(points) do if v[4] then v[4]:Destroy() end end \t\t\t\tpoints = {} \t\t\t\tfor i,v in pairs(seq.Keypoints) do \t\t\t\t\tlocal maxEnvelope = math.min(v.Value,10-v.Value) \t\t\t\t\tlocal newPoint = {v.Value,v.Time,math.min(v.Envelope,maxEnvelope)} \t\t\t\t\tnewPoint[4] = placePoint(newPoint) \t\t\t\t\ttable.insert(points,newPoint) \t\t\t\tend \t\t\t\tbeginPoint = points[1] \t\t\t\tendPoint = points[#points] \t\t\t\tcurrentlySelected = nil \t\t\t\tredraw() \t\t\t\tenvelopeDragTop.Visible = false \t\t\t\tenvelopeDragBottom.Visible = false \t\t\tend \t\t\tnewMt.SetSequence = loadSequence \t\t\ttimeBox.FocusLost:Connect(function() \t\t\t\tlocal point = currentPoint \t\t\t\tlocal num = tonumber(timeBox.Text) \t\t\t\tif point and num and point ~= beginPoint and point ~= endPoint then \t\t\t\t\tnum = math.clamp(num,0,1) \t\t\t\t\tpoint[2] = num \t\t\t\t\tredraw() \t\t\t\t\tbuildSequence() \t\t\t\t\tupdateInputs(point) \t\t\t\tend \t\t\tend) \t\t\tvalueBox.FocusLost:Connect(function() \t\t\t\tlocal point = currentPoint \t\t\t\tlocal num = tonumber(valueBox.Text) \t\t\t\tif point and num then \t\t\t\t\tlocal oldEnvelope = point[3] \t\t\t\t\tnum = math.clamp(num,0,10) \t\t\t\t\tpoint[1] = num \t\t\t\t\tlocal maxEnvelope = math.min(point[1],10-point[1]) \t\t\t\t\tpoint[3] = math.min(oldEnvelope,maxEnvelope) \t\t\t\t\tredraw() \t\t\t\t\tbuildSequence() \t\t\t\t\tupdateInputs(point) \t\t\t\tend \t\t\tend) \t\t\tenvelopeBox.FocusLost:Connect(function() \t\t\t\tlocal point = currentPoint \t\t\t\tlocal num = tonumber(envelopeBox.Text) \t\t\t\tif point and num then \t\t\t\t\tnum = math.clamp(num,0,5) \t\t\t\t\tlocal maxEnvelope = math.min(point[1],10-point[1]) \t\t\t\t\tpoint[3] = math.min(num,maxEnvelope) \t\t\t\t\tredraw() \t\t\t\t\tbuildSequence() \t\t\t\t\tupdateInputs(point) \t\t\t\tend \t\t\tend) \t\t\tlocal function buttonAnimations(button,inverse) \t\t\t\tbutton.InputBegan:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then button.BackgroundTransparency = (inverse and 0.5 or 0.4) end end) \t\t\t\tbutton.InputEnded:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then button.BackgroundTransparency = (inverse and 1 or 0) end end) \t\t\tend \t\t\t \t\t\tnumberLine.InputBegan:Connect(function(input) \t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and #points < 20 then \t\t\t\t\t \t\t\t\t\tif Lib.CheckMouseInGui(envelopeDragTop) or Lib.CheckMouseInGui(envelopeDragBottom) then return end \t\t\t\t\t \t\t\t\t\tfor i, v in pairs(points) do \t\t\t\t\t\tif Lib.CheckMouseInGui(v[4].Select) then \t\t\t\t\t\t\treturn \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\t \t\t\t\t\tlocal maxX = numberLine.AbsoluteSize.X - 1 \t\t\t\t\tlocal relativeX = (input.Position.X - numberLine.AbsolutePosition.X) \t\t\t\t\tif relativeX < 0 then relativeX = 0 end \t\t\t\t\tif relativeX > maxX then relativeX = maxX end \t\t\t\t\t \t\t\t\t\tlocal maxY = numberLine.AbsoluteSize.Y - 1 \t\t\t\t\tlocal relativeY = (input.Position.Y - numberLine.AbsolutePosition.Y) \t\t\t\t\tif relativeY < 0 then relativeY = 0 end \t\t\t\t\tif relativeY > maxY then relativeY = maxY end \t\t\t\t\t \t\t\t\t\tlocal raw = relativeX / maxX \t\t\t\t\tlocal newPoint = {10 - (relativeY / maxY) * 10, raw, 0} \t\t\t\t\tnewPoint[4] = placePoint(newPoint) \t\t\t\t\ttable.insert(points, newPoint) \t\t\t\t\tredraw() \t\t\t\t\tbuildSequence() \t\t\t\tend \t\t\tend) \t\t \t\t\tdeleteButton.MouseButton1Click:Connect(function() \t\t\t\tif currentPoint and currentPoint ~= beginPoint and currentPoint ~= endPoint then \t\t\t\t\tfor i,v in pairs(points) do \t\t\t\t\t\tif v == currentPoint then \t\t\t\t\t\t\tv[4]:Destroy() \t\t\t\t\t\t\ttable.remove(points,i) \t\t\t\t\t\t\tbreak \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\tcurrentlySelected = nil \t\t\t\t\tredraw() \t\t\t\t\tbuildSequence() \t\t\t\t\tupdateInputs(points[1]) \t\t\t\tend \t\t\tend) \t\t\tresetButton.MouseButton1Click:Connect(function() \t\t\t\tif resetSequence then \t\t\t\t\tnewMt:SetSequence(resetSequence) \t\t\t\t\tbuildSequence() \t\t\t\tend \t\t\tend) \t\t\tcloseButton.MouseButton1Click:Connect(function() \t\t\t\twindow:Close() \t\t\tend) \t\t\tbuttonAnimations(deleteButton) \t\t\tbuttonAnimations(resetButton) \t\t\tbuttonAnimations(closeButton) \t\t\tplacePoints() \t\t\tredraw() \t\t\tnewMt.Show = function(self) \t\t\t\twindow:Show() \t\t\tend \t\t\treturn newMt \t\tend \t\treturn {new = new} \tend)() \tLib.ColorSequenceEditor = (function() -- TODO: Convert to newer class model \t\tlocal function new() \t\t\tlocal newMt = setmetatable({},{}) \t\t\tnewMt.OnSelect = Lib.Signal.new() \t\t\tnewMt.OnCancel = Lib.Signal.new() \t\t\tnewMt.OnPreview = Lib.Signal.new() \t\t\tnewMt.OnPickColor = Lib.Signal.new() \t\t\tlocal guiContents = create({ \t\t\t\t{1,\"Frame\",{BackgroundColor3=Color3.new(0.17647059261799,0.17647059261799,0.17647059261799),BorderSizePixel=0,ClipsDescendants=true,Name=\"Content\",Position=UDim2.new(0,0,0,20),Size=UDim2.new(1,0,1,-20),}}, \t\t\t\t{2,\"Frame\",{BackgroundColor3=Color3.new(0.17647059261799,0.17647059261799,0.17647059261799),BorderColor3=Color3.new(0.21568627655506,0.21568627655506,0.21568627655506),Name=\"ColorLine\",Parent={1},Position=UDim2.new(0,10,0,5),Size=UDim2.new(1,-20,0,70),}}, \t\t\t\t{3,\"Frame\",{BackgroundColor3=Color3.new(1,1,1),BorderSizePixel=0,Name=\"Gradient\",Parent={2},Size=UDim2.new(1,0,1,0),}}, \t\t\t\t{4
lse end) \t\t\t \t\t\tguiTopBar.InputBegan:Connect(function(input) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\tif self.Draggable then \t\t\t\t\t\tlocal releaseEvent, mouseEvent \t\t\t\t\t\t \t\t\t\t\t\tlocal maxX = sidesGui.AbsoluteSize.X \t\t\t\t\t\tlocal initX = guiMain.AbsolutePosition.X \t\t\t\t\t\tlocal initY = guiMain.AbsolutePosition.Y \t\t\t\t\t\tlocal offX = input.Position.X - initX \t\t\t\t\t\tlocal offY = input.Position.Y - initY \t\t\t\t\t\t \t\t\t\t\t\tlocal alignInsertPos, alignInsertSide \t\t\t\t\t\t \t\t\t\t\t\tguiDragging = true \t\t\t\t\t\t \t\t\t\t\t\treleaseEvent = service.UserInputService.InputEnded:Connect(function(input) \t\t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\t\tif mouseEvent then mouseEvent:Disconnect() end \t\t\t\t\t\t\t\tguiDragging = false \t\t\t\t\t\t\t\talignIndicator.Parent = nil \t\t\t\t\t\t\t\tif alignInsertSide then \t\t\t\t\t\t\t\t\tlocal targetSide = (alignInsertSide == \"left\" and leftSide) or (alignInsertSide == \"right\" and rightSide) \t\t\t\t\t\t\t\t\tself:AlignTo(targetSide, alignInsertPos) \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t\t\t\t \t\t\t\t\t\tmouseEvent = service.UserInputService.InputChanged:Connect(function(input) \t\t\t\t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and self.Draggable and not self.Closed and ButtonDown then \t\t\t\t\t\t\t\tif self.Aligned then \t\t\t\t\t\t\t\t\tif leftSide.Resizing or rightSide.Resizing then return end \t\t\t\t\t\t\t\t\tlocal posX, posY = input.Position.X - offX, input.Position.Y - offY \t\t\t\t\t\t\t\t\tlocal delta = math.sqrt((posX - initX)^2 + (posY - initY)^2) \t\t\t\t\t\t\t\t\tif delta >= 5 then \t\t\t\t\t\t\t\t\t\tself:SetAligned(false) \t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\telse \t\t\t\t\t\t\t\t\tlocal inputX, inputY = input.Position.X, input.Position.Y \t\t\t\t\t\t\t\t\tlocal posX, posY = inputX - offX, inputY - offY \t\t\t\t\t\t\t\t\tif posY < 0 then posY = 0 end \t\t\t\t\t\t\t\t\tguiMain.Position = UDim2.new(0, posX, 0, posY) \t\t\t\t\t\t\t\t\t \t\t\t\t\t\t\t\t\tif self.Resizable and self.Alignable then \t\t\t\t\t\t\t\t\t\tif inputX < 25 then \t\t\t\t\t\t\t\t\t\t\tif sideHasRoom(leftSide, self.MinY or 100) then \t\t\t\t\t\t\t\t\t\t\t\tlocal insertPos, range = getSideInsertPos(leftSide, inputY) \t\t\t\t\t\t\t\t\t\t\t\talignIndicator.Indicator.Position = UDim2.new(0, -15, 0, range[1]) \t\t\t\t\t\t\t\t\t\t\t\talignIndicator.Indicator.Size = UDim2.new(0, 40, 0, range[2] - range[1]) \t\t\t\t\t\t\t\t\t\t\t\tLib.ShowGui(alignIndicator) \t\t\t\t\t\t\t\t\t\t\t\talignInsertPos = insertPos \t\t\t\t\t\t\t\t\t\t\t\talignInsertSide = \"left\" \t\t\t\t\t\t\t\t\t\t\t\treturn \t\t\t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\t\telseif inputX >= maxX - 25 then \t\t\t\t\t\t\t\t\t\t\tif sideHasRoom(rightSide, self.MinY or 100) then \t\t\t\t\t\t\t\t\t\t\t\tlocal insertPos, range = getSideInsertPos(rightSide, inputY) \t\t\t\t\t\t\t\t\t\t\t\talignIndicator.Indicator.Position = UDim2.new(0, maxX - 25, 0, range[1]) \t\t\t\t\t\t\t\t\t\t\t\talignIndicator.Indicator.Size = UDim2.new(0, 40, 0, range[2] - range[1]) \t\t\t\t\t\t\t\t\t\t\t\tLib.ShowGui(alignIndicator) \t\t\t\t\t\t\t\t\t\t\t\talignInsertPos = insertPos \t\t\t\t\t\t\t\t\t\t\t\talignInsertSide = \"right\" \t\t\t\t\t\t\t\t\t\t\t\treturn \t\t\t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\talignIndicator.Parent = nil \t\t\t\t\t\t\t\t\talignInsertPos = nil \t\t\t\t\t\t\t\t\talignInsertSide = nil \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t\t\tend \t\t\t\tend \t\t\tend) \t\t\t \t\t\tguiTopBar.Close.MouseButton1Click:Connect(function() \t\t\t\tif self.Closed then return end \t\t\t\tself:Close() \t\t\tend) \t\t\t \t\t\tguiTopBar.Minimize.MouseButton1Click:Connect(function() \t\t\t\tif self.Closed then return end \t\t\t\tif self.Aligned then \t\t\t\t\tself:SetAligned(false) \t\t\t\telse \t\t\t\t\tself:SetMinimized() \t\t\t\tend \t\t\tend) \t\t\t \t\t\tguiTopBar.Minimize.MouseButton2Click:Connect(function() \t\t\t\tif self.Closed then return end \t\t\t\tif not self.Aligned then \t\t\t\t\tself:SetMinimized(nil,2) \t\t\t\t\tguiTopBar.Minimize.BackgroundTransparency = 1 \t\t\t\tend \t\t\tend) \t\t\t \t\t\tguiMain.InputBegan:Connect(function(input) \t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and not self.Aligned and not self.Closed then \t\t\t\t\tmoveToTop(self) \t\t\t\tend \t\t\tend) \t\t\t \t\t\tguiMain:GetPropertyChangedSignal(\"AbsolutePosition\"):Connect(function() \t\t\t\tlocal absPos = guiMain.AbsolutePosition \t\t\t\tself.PosX = absPos.X \t\t\t\tself.PosY = absPos.Y \t\t\tend) \t\t\t \t\t\tresizeHook(self,guiResizeControls.North,\"N\") \t\t\tresizeHook(self,guiResizeControls.NorthEast,\"NE\") \t\t\tresizeHook(self,guiResizeControls.East,\"E\") \t\t\tresizeHook(self,guiResizeControls.SouthEast,\"SE\") \t\t\tresizeHook(self,guiResizeControls.South,\"S\") \t\t\tresizeHook(self,guiResizeControls.SouthWest,\"SW\") \t\t\tresizeHook(self,guiResizeControls.West,\"W\") \t\t\tresizeHook(self,guiResizeControls.NorthWest,\"NW\") \t\t\tguiMain.Size = UDim2.new(0,self.SizeX,0,self.SizeY) \t\t\tgui.DescendantAdded:Connect(function(obj) focusInput(self,obj) end) \t\t\tlocal descs = gui:GetDescendants() \t\t\tfor i = 1,#descs do \t\t\t\tfocusInput(self,descs[i]) \t\t\tend \t\t\t \t\t\tself.MinimizeAnim = Lib.ButtonAnim(guiTopBar.Minimize) \t\t\tself.CloseAnim = Lib.ButtonAnim(guiTopBar.Close) \t\t\t \t\t\treturn gui \t\tend \t\tlocal function updateSideFrames(noTween) \t\t\tstopTweens() \t\t\tleftSide.Frame.Size = UDim2.new(0,leftSide.Width,1,0) \t\t\trightSide.Frame.Size = UDim2.new(0,rightSide.Width,1,0) \t\t\tleftSide.Frame.Resizer.Position = UDim2.new(0,leftSide.Width,0,0) \t\t\trightSide.Frame.Resizer.Position = UDim2.new(0,-5,0,0) \t\t\t--leftSide.Frame.Visible = (#leftSide.Windows > 0) \t\t\t--rightSide.Frame.Visible = (#rightSide.Windows > 0) \t\t\t--[[if #leftSide.Windows > 0 and leftSide.Frame.Position == UDim2.new(0,-leftSide.Width-5,0,0) then \t\t\t\tleftSide.Frame:TweenPosition(UDim2.new(0,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.3,true) \t\t\telseif #leftSide.Windows == 0 and leftSide.Frame.Position == UDim2.new(0,0,0,0) then \t\t\t\tleftSide.Frame:TweenPosition(UDim2.new(0,-leftSide.Width-5,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.3,true) \t\t\tend \t\t\tlocal rightTweenPos = (#rightSide.Windows == 0 and UDim2.new(1,5,0,0) or UDim2.new(1,-rightSide.Width,0,0)) \t\t\trightSide.Frame:TweenPosition(rightTweenPos,Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.3,true)]] \t\t\tlocal leftHidden = #leftSide.Windows == 0 or leftSide.Hidden \t\t\tlocal rightHidden = #rightSide.Windows == 0 or rightSide.Hidden \t\t\tlocal leftPos = (leftHidden and UDim2.new(0,-leftSide.Width-10,0,0) or UDim2.new(0,0,0,0)) \t\t\tlocal rightPos = (rightHidden and UDim2.new(1,10,0,0) or UDim2.new(1,-rightSide.Width,0,0)) \t\t\tsidesGui.LeftToggle.Text = leftHidden and \">\" or \"<\" \t\t\tsidesGui.RightToggle.Text = rightHidden and \"<\" or \">\" \t\t\t \t\t\tif not noTween then \t\t\t\tlocal function insertTween(...) \t\t\t\t\tlocal tween = service.TweenService:Create(...) \t\t\t\t\ttweens[#tweens+1] = tween \t\t\t\t\ttween:Play() \t\t\t\tend \t\t\t\tinsertTween(leftSide.Frame,sideTweenInfo,{Position = leftPos}) \t\t\t\tinsertTween(rightSide.Frame,sideTweenInfo,{Position = rightPos}) \t\t\t\tinsertTween(sidesGui.LeftToggle,sideTweenInfo,{Position = UDim2.new(0,#leftSide.Windows == 0 and -16 or 0,0,-36)}) \t\t\t\tinsertTween(sidesGui.RightToggle,sideTweenInfo,{Position = UDim2.new(1,#rightSide.Windows == 0 and 0 or -16,0,-36)}) \t\t\telse \t\t\t\tleftSide.Frame.Position = leftPos \t\t\t\trightSide.Frame.Position = rightPos \t\t\t\tsidesGui.LeftToggle.Position = UDim2.new(0,#leftSide.Windows == 0 and -16 or 0,0,-36) \t\t\t\tsidesGui.RightToggle.Position = UDim2.new(1,#rightSide.Windows == 0 and 0 or -16,0,-36) \t\t\tend \t\tend \t\tlocal function getSideFramePos(side) \t\t\tlocal leftHidden = #leftSide.Windows == 0 or leftSide.Hidden \t\t\tlocal rightHidden = #rightSide.Windows == 0 or rightSide.Hidden \t\t\tif side == leftSide then \t\t\t\treturn (leftHidden and UDim2.new(0,-leftSide.Width-10,0,0) or UDim2.new(0,0,0,0)) \t\t\telse \t\t\t\treturn (rightHidden and UDim2.new(1,10,0,0) or UDim2.new(1,-rightSide.Width,0,0)) \t\t\tend \t\tend \t\tlocal function sideResized(side) \t\t\tlocal currentPos = 0 \t\t\tlocal sideFramePos = getSideFramePos(side) \t\t\tfor i,v in pairs(side.Windows) do \t\t\t\tv.SizeX = side.Width \t\t\t\tv.GuiElems.Main.Size = UDim2.new(0,side.Width,0,v.SizeY) \t\t\t\tv.GuiElems.Main.Position = UDim2.new(sideFramePos.X.Scale,sideFramePos.X.Offset,0,currentPos) \t\t\t\tcurrentPos = currentPos + v.SizeY+4 \t\t\tend \t\tend \t\tlocal function sideResizerHook(resizer,dir,side,pos) \t\t\tlocal mouse = Main.Mouse \t\t\tlocal windows = side.Windows \t\t\t \t\t\tresizer.InputBegan:Connect(function(input) \t\t\t\tif not side.Resizing then \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement then \t\t\t\t\t\tresizer.BackgroundColor3 = theme.MainColor2 \t\t\t\t\telseif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tlocal releaseEvent, inputEvent \t\t\t \t\t\t\t\t\tlocal offX = input.Position.X - resizer.AbsolutePosition.X \t\t\t\t\t\tlocal offY = input.Position.Y - resizer.AbsolutePosition.Y \t\t\t \t\t\t\t\t\tside.Resizing = resizer \t\t\t\t\t\tresizer.BackgroundColor3 = theme.MainColor2 \t\t\t \t\t\t\t\t\treleaseEvent = service.UserInputService.InputEnded:Connect(function(input) \t\t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\t\tif inputEvent then inputEvent:Disconnect() end \t\t\t\t\t\t\t\tside.Resizing = false \t\t\t\t\t\t\t\tresizer.BackgroundColor3 = theme.Button \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t \t\t\t\t\t\tinputEvent = service.UserInputService.InputChanged:Connect(function(input) \t\t\t\t\t\t\tif not resizer.Parent then \t\t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\t\tif inputEvent then inputEvent:Disconnect() end \t\t\t\t\t\t\t\tside.Resizing = false \t\t\t\t\t\t\t\treturn \t\t\t\t\t\t\tend \t\t\t \t\t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\t\tif dir == \"V\" then \t\t\t\t\t\t\t\t\tlocal delta = input.Position.Y - resizer.AbsolutePosition.Y - offY \t\t\t \t\t\t\t\t\t\t\t\tif delta > 0 then \t\t\t\t\t\t\t\t\t\tlocal neededSize = delta \t\t\t\t\t\t\t\t\t\tfor i = pos + 1, #windows do \t\t\t\t\t\t\t\t\t\t\tlocal window = windows[i] \t\t\t\t\t\t\t\t\t\t\tlocal newSize = math.max(window.SizeY - neededSize, (window.MinY or 100)) \t\t\t\t\t\t\t\t\t\t\tneededSize = neededSize - (window.SizeY - newSize) \t\t\t\t\t\t\t\t\t\t\twindow.SizeY = newSize \t\t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\t\twindows[pos].SizeY = windows[pos].SizeY + math.max(0, delta - neededSize) \t\t\t\t\t\t\t\t\telse \t\t\t\t\t\t\t\t\t\tlocal neededSize = -delta \t\t\t\t\t\t\t\t\t\tfor i = pos, 1, -1 do \t\t\t\t\t\t\t\t\t\t\tlocal window = windows[i] \t\t\t\t\t\t\t\t\t\t\tlocal newSize = math.max(window.SizeY - neededSize, (window.MinY or 100)) \t\t\t\t\t\t\t\t\t\t\tneededSize = neededSize - (window.SizeY - newSize) \t\t\t\t\t\t\t\t\t\t\twindow.SizeY = newSize \t\t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\t\twindows[pos + 1].SizeY = windows[pos + 1].SizeY + math.max(0, -delta - neededSize) \t\t\t\t\t\t\t\t\tend \t\t\t \t\t\t\t\t\t\t\t\tupdateSideFrames() \t\t\t\t\t\t\t\t\tsideResized(side) \t\t\t\t\t\t\t\telseif dir == \"H\" then \t\t\t\t\t\t\t\t\tlocal maxWidth = math.max(300, sidesGui.AbsoluteSize.X - static.FreeWidth) \t\t\t\t\t\t\t\t\tlocal otherSide = (side == leftSide and rightSide or leftSide) \t\t\t\t\t\t\t\t\tlocal delta = input.Position.X - resizer.AbsolutePosition.X - offX \t\t\t\t\t\t\t\t\tdelta = (side == leftSide and delta or -delta) \t\t\t \t\t\t\t\t\t\t\t\tlocal proposedSize = math.max(static.MinWidth, side.Width + delta) \t\t\t\t\t\t\t\t\tif proposedSize + otherSide.Width <= maxWidth then \t\t\t\t\t\t\t\t\t\tside.Width = proposedSize \t\t\t\t\t\t\t\t\telse \t\t\t\t\t\t\t\t\t\tlocal newOtherSize = maxWidth - proposedSize \t\t\t\t\t\t\t\t\t\tif newOtherSize >= static.MinWidth then \t\t\t\t\t\t\t\t\t\t\tside.Width = proposedSize \t\t\t\t\t\t\t\t\t\t\totherSide.Width = newOtherSize \t\t\t\t\t\t\t\t\t\telse \t\t\t\t\t\t\t\t\t\t\tside.Width = maxWidth - static.MinWidth \t\t\t\t\t\t\t\t\t\t\totherSide.Width = static.MinWidth \t\t\t\t\t\t\t\t\t\tend \t\t\t\t\t\t\t\t\tend \t\t\t \t\t\t\t\t\t\t\t\tupdateSideFrames(true) \t\t\t\t\t\t\t\t\tsideResized(side) \t\t\t\t\t\t\t\t\tsideResized(otherSide) \t\t\t\t\t\t\t\tend \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t\t\tend \t\t\t\tend \t\t\tend) \t\t\t \t\t\tresizer.InputEnded:Connect(function(input) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\tif side.Resizing ~= resizer then \t\t\t\t\t\tresizer.BackgroundColor3 = theme.Button \t\t\t\t\tend \t\t\t\tend \t\t\tend) \t\t\t \t\tend \t\tlocal function renderSide(side,noTween) -- TODO: Use existing resizers \t\t\tlocal currentPos = 0 \t\t\tlocal sideFramePos = getSideFramePos(side) \t\t\tlocal template = side.WindowResizer:Clone() \t\t\tfor i,v in pairs(side.ResizeCons) do v:Disconnect() end \t\t\tfor i,v in pairs(side.Frame:GetChildren()) do if v.Name == \"WindowResizer\" then v:Destroy() end end \t\t\tside.ResizeCons = {} \t\t\tside.Resizing = nil \t\t\tfor i,v in pairs(side.Windows) do \t\t\t\tv.SidePos = i \t\t\t\tlocal isEnd = i == #side.Windows \t\t\t\tlocal size = UDim2.new(0,side.Width,0,v.SizeY) \t\t\t\tlocal pos = UDim2.new(sideFramePos.X.Scale,sideFramePos.X.Offset,0,currentPos) \t\t\t\tLib.ShowGui(v.Gui) \t\t\t\t--v.GuiElems.Main:TweenSizeAndPosition(size,pos,Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.3,true) \t\t\t\tif noTween then \t\t\t\t\tv.GuiElems.Main.Size = size \t\t\t\t\tv.GuiElems.Main.Position = pos \t\t\t\telse \t\t\t\t\tlocal tween = service.TweenService:Create(v.GuiElems.Main,sideTweenInfo,{Size = size, Position = pos}) \t\t\t\t\ttweens[#tweens+1] = tween \t\t\t\t\ttween:Play() \t\t\t\tend \t\t\t\tcurrentPos = currentPos + v.SizeY+4 \t\t\t\tif not isEnd then \t\t\t\t\tlocal newTemplate = template:Clone() \t\t\t\t\tnewTemplate.Position = UDim2.new(1,-side.Width,0,currentPos-4) \t\t\t\t\tside.ResizeCons[#side.ResizeCons+1] = v.Gui.Main:GetPropertyChangedSignal(\"Size\"):Connect(function() \t\t\t\t\t\tnewTemplate.Position = UDim2.new(1,-side.Width,0, v.GuiElems.Main.Position.Y.Offset + v.GuiElems.Main.Size.Y.Offset) \t\t\t\t\tend) \t\t\t\t\tside.ResizeCons[#side.ResizeCons+1] = v.Gui.Main:GetPropertyChangedSignal(\"Position\"):Connect(function() \t\t\t\t\t\tnewTemplate.Position = UDim2.new(1,-side.Width,0, v.GuiElems.Main.Position.Y.Offset + v.GuiElems.Main.Size.Y.Offset) \t\t\t\t\tend) \t\t\t\t\tsideResizerHook(newTemplate,\"V\",side,i) \t\t\t\t\tnewTemplate.Parent = side.Frame \t\t\t\tend \t\t\tend \t\t\t--side.Frame.Back.Position = UDim2.new(0,0,0,0) \t\t\t--side.Frame.Back.Size = UDim2.new(0,side.Width,1,0) \t\tend \t\tlocal function updateSide(side,noTween) \t\t\tlocal oldHeight = 0 \t\t\tlocal currentPos = 0 \t\t\tlocal neededSize = 0 \t\t\tlocal windows = side.Windows \t\t\tlocal height = sidesGui.AbsoluteSize.Y - (math.max(0,#windows - 1) * 4) \t\t\tfor i,v in pairs(windows) do oldHeight = oldHeight + v.SizeY end \t\t\tfor i,v in pairs(windows) do \t\t\t\tif i == #windows then \t\t\t\t\tv.SizeY = height-currentPos \t\t\t\t\tneededSize = math.max(0,(v.MinY or 100)-v.SizeY) \t\t\t\telse \t\t\t\t\tv.SizeY = math.max(math.floor(v.SizeY/oldHeight*height),v.MinY or 100) \t\t\t\tend \t\t\t\tcurrentPos = currentPos + v.SizeY \t\t\tend \t\t\tif neededSize > 0 then \t\t\t\tfor i = #windows-1,1,-1 do \t\t\t\t\tlocal window = windows[i] \t\t\t\t\tlocal newSize = math.max(window.SizeY-neededSize,(window.MinY or 100)) \t\t\t\t\tneededSize = neededSize - (window.SizeY - newSize) \t\t\t\t\twindow.SizeY = newSize \t\t\t\tend \t\t\t\tlocal lastWindow = windows[#windows] \t\t\t\tlastWindow.SizeY = (lastWindow.MinY or 100)-neededSize \t\t\tend \t\t\trenderSide(side,noTween) \t\tend \t\tupdateWindows = function(noTween) \t\t\tupdateSideFrames(noTween) \t\t\tupdateSide(leftSide,noTween) \t\t\tupdateSide(rightSide,noTween) \t\t\tlocal count = 0 \t\t\tfor i = #visibleWindows,1,-1 do \t\t\t\tvisibleWindows[i].Gui.DisplayOrder = displayOrderStart + count \t\t\t\tLib.ShowGui(visibleWindows[i].Gui) \t\t\t\tcount = count + 1 \t\t\tend \t\t\t--[[local leftTweenPos = (#leftSide.Windows == 0 and UDim2.new(0,-leftSide.Width-5,0,0) or UDim2.new(0,0,0,0)) \t\t\tleftSide.Frame:TweenPosition(leftTweenPos,Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.3,true) \t\t\tlocal rightTweenPos = (#rightSide.Windows == 0 and UDim2.new(1,5,0,0) or UDim2.new(1,-rightSide.Width,0,0)) \t\t\trightSide.Frame:TweenPosition(rightTweenPos,Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.3,true)]] \t\tend \t\tfuncs.SetMinimized = function(self,set,mode) \t\t\tlocal oldVal = self.Minimized \t\t\tlocal newVal \t\t\tif set == nil then newVal = not self.Minimized else newVal = set end \t\t\tself.Minimized = newVal \t\t\tif not mode then mode = 1 end \t\t\tlocal resizeControls = self.GuiElems.ResizeControls \t\t\tlocal minimizeControls = {\"North\",\"NorthEast\",\"NorthWest\",\"South\",\"SouthEast\",\"SouthWest\"} \t\t\tfor i = 1,#minimizeControls do \t\t\t\tlocal control = resizeControls:FindFirstChild(minimizeControls[i]) \t\t\t\tif control then control.Visible = not newVal end \t\t\tend \t\t\tif mode == 1 or mode == 2 then \t\t\t\tself:StopTweens() \t\t\t\tif mode == 1 then \t\t\t\t\tself.GuiElems.Main:TweenSize(UDim2.new(0,self.SizeX,0,newVal and 20 or self.SizeY),Enum.EasingDirection.Out,Enum.EasingStyle.Quart,0.25,true) \t\t\t\telse \t\t\t\t\tlocal maxY = sidesGui.AbsoluteSize.Y \t\t\t\t\tlocal newPos = UDim2.new(0,self.PosX,0,newVal and math.min(maxY-20,self.PosY + self.SizeY - 20) or math.max(0,self.PosY - self.SizeY + 20)) \t\t\t\t\t \t\t\t\t\tself.GuiElems.Main:TweenPosition(newPos,Enum.EasingDirection.Out,Enum.EasingStyle.Quart,0.25,true) \t\t\t\t\tself.GuiElems.Main:TweenSize(UDim2.new(0,self.SizeX,0,newVal and 20 or self.SizeY),Enum.EasingDirection.Out,Enum.EasingStyle.Quart,0.25,true) \t\t\t\tend \t\t\t\tself.GuiElems.Minimize.ImageLabel.Image = newVal and \"rbxassetid://5060023708\" or \"rbxassetid://5034768003\" \t\t\tend \t\t\tif oldVal ~= newVal then \t\t\t\tif newVal then \t\t\t\t\tself.OnMinimize:Fire() \t\t\t\telse \t\t\t\t\tself.OnRestore:Fire() \t\t\t\tend \t\t\tend \t\tend \t\tfuncs.Resize = function(self,sizeX,sizeY) \t\t\tself.SizeX = sizeX or self.SizeX \t\t\tself.SizeY = sizeY or self.SizeY \t\t\tself.GuiElems.Main.Size = UDim2.new(0,self.SizeX,0,self.SizeY) \t\tend \t\tfuncs.SetSize = funcs.Resize \t\tfuncs.SetTitle = function(self,title) \t\t\tself.GuiElems.Title.Text = title \t\tend \t\tfuncs.SetResizable = function(self,val) \t\t\tself.Resizable = val \t\t\tself.GuiElems.ResizeControls.Visible = self.Resizable and self.ResizableInternal \t\tend \t\tfuncs.SetResizableInternal = function(self,val) \t\t\tself.ResizableInternal = val \t\t\tself.GuiElems.ResizeControls.Visible = self.Resizable and self.ResizableInternal \t\tend \t\tfuncs.SetAligned = function(self,val) \t\t\tself.Aligned = val \t\t\tself:SetResizableInternal(not val) \t\t\tself.GuiElems.Main.Active = not val \t\t\tself.GuiElems.Main.Outlines.Visible = not val \t\t\tif not val then \t\t\t\tfor i,v in pairs(leftSide.Windows) do if v == self then table.remove(leftSide.Windows,i) break end end \t\t\t\tfor i,v in pairs(rightSide.Windows) do if v == self then table.remove(rightSide.Windows,i) break end end \t\t\t\tif not table.find(visibleWindows,self) then table.insert(visibleWindows,1,self) end \t\t\t\tself.GuiElems.Minimize.ImageLabel.Image = \"rbxassetid://5034768003\" \t\t\t\tself.Side = nil \t\t\t\tupdateWindows() \t\t\telse \t\t\t\tself:SetMinimized(false,3) \t\t\t\tfor i,v in pairs(visibleWindows) do if v == self then table.remove(visibleWindows,i) break end end \t\t\t\tself.GuiElems.Minimize.ImageLabel.Image = \"rbxassetid://5448127505\" \t\t\tend \t\tend \t\tfuncs.Add = function(self,obj,name) \t\t\tif type(obj) == \"table\" and obj.Gui and obj.Gui:IsA(\"GuiObject\") then \t\t\t\tobj.Gui.Parent = self.ContentPane \t\t\telse \t\t\t\tobj.Parent = self.ContentPane \t\t\tend \t\t\tif name then self.Elements[name] = obj end \t\tend \t\tfuncs.GetElement = function(self,obj,name) \t\t\treturn self.Elements[name] \t\tend \t\tfuncs.AlignTo = function(self,side,pos,size,silent) \t\t\tif table.find(side.Windows,self) or self.Closed then return end \t\t\tsize = size or self.SizeY \t\t\tif size > 0 and size <= 1 then \t\t\t\tlocal totalSideHeight = 0 \t\t\t\tfor i,v in pairs(side.Windows) do totalSideHeight = totalSideHeight + v.SizeY end \t\t\t\tself.SizeY = (totalSideHeight > 0 and totalSideHeight * size * 2) or size \t\t\telse \t\t\t\tself.SizeY = (size > 0 and size or 100) \t\t\tend \t\t\tself:SetAligned(true) \t\t\tself.Side = side \t\t\tself.SizeX = side.Width \t\t\tself.Gui.DisplayOrder = sideDisplayOrder + 1 \t\t\tfor i,v in pairs(side.Windows) do v.Gui.DisplayOrder = sideDisplayOrder end \t\t\tpos = math.min(#side.Windows+1, pos or 1) \t\t\tself.SidePos = pos \t\t\ttable.insert(side.Windows, pos, self) \t\t\tif not silent then \t\t\t\tside.Hidden = false \t\t\tend \t\t\t-- updateWindows(silent) \t\tend \t\tfuncs.Close = function(self) \t\t\tself.Closed = true \t\t\tself:SetResizableInternal(false) \t\t\tLib.FindAndRemove(leftSide.Windows,self) \t\t\tLib.FindAndRemove(rightSide.Windows,self) \t\t\tLib.FindAndRemove(visibleWindows,self) \t\t\tself.MinimizeAnim.Disable() \t\t\tself.CloseAnim.Disable() \t\t\tself.ClosedSide = self.Side \t\t\tself.Side = nil \t\t\tself.OnDeactivate:Fire() \t\t\tif not self.Aligned then \t\t\t\tself:StopTweens() \t\t\t\tlocal ti = TweenInfo.new(0.2,Enum.EasingStyle.Quad,Enum.EasingDirection.Out) \t\t\t\tlocal closeTime = tick() \t\t\t\tself.LastClose = closeTime \t\t\t\tself:DoTween(self.GuiElems.Main,ti,{Size = UDim2.new(0,self.SizeX,0,20)}) \t\t\t\tself:DoTween(self.GuiElems.Title,ti,{TextTransparency = 1}) \t\t\t\tself:DoTween(self.GuiElems.Minimize.ImageLabel,ti,{ImageTransparency = 1}) \t\t\t\tself:DoTween(self.GuiElems.Close.ImageLabel,ti,{ImageTransparency = 1}) \t\t\t\tLib.FastWait(0.2) \t\t\t\tif closeTime ~= self.LastClose then return end \t\t\t\tself:DoTween(self.GuiElems.TopBar,ti,{BackgroundTransparency = 1}) \t\t\t\tself:DoTween(self.GuiElems.Outlines,ti,{ImageTransparency = 1}) \t\t\t\tLib.FastWait(0.2) \t\t\t\tif closeTime ~= self.LastClose then return end \t\t\tend \t\t\tself.Aligned = false \t\t\tself.Gui.Parent = nil \t\t\tupdateWindows(true) \t\tend \t\tfuncs.Hide = funcs.Close \t\tfuncs.IsVisible = function(self) \t\t\treturn not self.Closed and ((self.Side and not self.Side.Hidden) or not self.Side) \t\tend \t\tfuncs.IsContentVisible = function(self) \t\t\treturn self:IsVisible() and not self.Minimized \t\tend \t\tfuncs.Focus = function(self) \t\t\tmoveToTop(self) \t\tend \t\tfuncs.MoveInBoundary = function(self) \t\t\tlocal posX,posY = self.PosX,self.PosY \t\t\tlocal maxX,maxY = sidesGui.AbsoluteSize.X,sidesGui.AbsoluteSize.Y \t\t\tposX = math.min(posX,maxX-self.SizeX) \t\t\tposY = math.min(posY,maxY-20) \t\t\tself.GuiElems.Main.Position = UDim2.new(0,posX,0,posY) \t\tend \t\tfuncs.DoTween = function(self,...) \t\t\tlocal tween = service.TweenService:Create(...) \t\t\tself.Tweens[#self.Tweens+1] = tween \t\t\ttween:Play() \t\tend \t\tfuncs.StopTweens = function(self) \t\t\tfor i,v in pairs(self.Tweens) do \t\t\t\tv:Cancel() \t\t\tend \t\t\tself.Tweens = {} \t\tend \t\tfuncs.Show = function(self,data) \t\t\treturn static.ShowWindow(self,data) \t\tend \t\tfuncs.ShowAndFocus = function(self,data) \t\t\tstatic.ShowWindow(self,data) \t\t\tservice.RunService.RenderStepped:wait() \t\t\tself:Focus() \t\tend \t\tstatic.ShowWindow = function(window,data) \t\t\tdata = data or {} \t\t\tlocal align = data.Align \t\t\tlocal pos = data.Pos \t\t\tlocal size = data.Size \t\t\tlocal targetSide = (align == \"left\" and leftSide) or (align == \"right\" and rightSide) \t\t\tif not window.Closed then \t\t\t\tif not window.Aligned then \t\t\t\t\twindow:SetMinimized(false) \t\t\t\telseif window.Side and not data.Silent then \t\t\t\t\tstatic.SetSideVisible(window.Side,true) \t\t\t\tend \t\t\t\treturn \t\t\tend \t\t\t \t\t\twindow.Closed = false \t\t\twindow.LastClose = tick() \t\t\twindow.GuiElems.Title.TextTransparency = 0 \t\t\twindow.GuiElems.Minimize.ImageLabel.ImageTransparency = 0 \t\t\twindow.GuiElems.Close.ImageLabel.ImageTransparency = 0 \t\t\twindow.GuiElems.TopBar.BackgroundTransparency = 0 \t\t\twindow.GuiElems.Outlines.ImageTransparency = 0 \t\t\twindow.GuiElems.Minimize.ImageLabel.Image = \"rbxassetid://5034768003\" \t\t\twindow.GuiElems.Main.Active = true \t\t\twindow.GuiElems.Main.Outlines.Visible = true \t\t\twindow:SetMinimized(false,3) \t\t\twindow:SetResizableInternal(true) \t\t\twindow.MinimizeAnim.Enable() \t\t\twindow.CloseAnim.Enable() \t\t\tif align then \t\t\t\twindow:AlignTo(targetSide,pos,size,data.Silent) \t\t\telse \t\t\t\tif align == nil and window.ClosedSide then -- Regular open \t\t\t\t\twindow:AlignTo(window.ClosedSide,window.SidePos,size,true) \t\t\t\t\tstatic.SetSideVisible(window.ClosedSide,true) \t\t\t\telse \t\t\t\t\tif table.find(visibleWindows,window) then return end \t\t\t\t\t-- TODO: make better \t\t\t\t\twindow.GuiElems.Main.Size = UDim2.new(0,window.SizeX,0,20) \t\t\t\t\tlocal ti = TweenInfo.new(0.2,Enum.EasingStyle.Quad,Enum.EasingDirection.Out) \t\t\t\t\twindow:StopTweens() \t\t\t\t\twindow:DoTween(window.GuiElems.Main,ti,{Size = UDim2.new(0,window.SizeX,0,window.SizeY)}) \t\t\t\t\twindow.SizeY = size or window.SizeY \t\t\t\t\ttable.insert(visibleWindows,1,window) \t\t\t\t\tupdateWindows() \t\t\t\tend \t\t\tend \t\t\twindow.ClosedSide = nil \t\t\twindow.OnActivate:Fire() \t\tend \t\tstatic.ToggleSide = function(name) \t\t\tlocal side = (name == \"left\" and leftSide or rightSide) \t\t\tside.Hidden = not side.Hidden \t\t\tfor i,v in pairs(side.Windows) do \t\t\t\tif side.Hidden then \t\t\t\t\tv.OnDeactivate:Fire() \t\t\t\telse \t\t\t\t\tv.OnActivate:Fire() \t\t\t\tend \t\t\tend \t\t\tupdateWindows() \t\tend \t\tstatic.SetSideVisible = function(s,vis) \t\t\tlocal side = (type(s) == \"table\" and s) or (s == \"left\" and leftSide or rightSide) \t\t\tside.Hidden = not vis \t\t\tfor i,v in pairs(side.Windows) do \t\t\t\tif side.Hidden then \t\t\t\t\tv.OnDeactivate:Fire() \t\t\t\telse \t\t\t\t\tv.OnActivate:Fire() \t\t\t\tend \t\t\tend \t\t\tupdateWindows() \t\tend \t\tstatic.Init = function() \t\t\tdisplayOrderStart = Main.DisplayOrders.Window \t\t\tsideDisplayOrder = Main.DisplayOrders.SideWindow \t\t\tsidesGui = Instance.new(\"ScreenGui\") \t\t\tlocal leftFrame = create({ \t\t\t\t{1,\"Frame\",{Active=true,Name=\"LeftSide\",BackgroundColor3=Color3.new(0.17647059261799,0.17647059261799,0.17647059261799),BorderSizePixel=0,}}, \t\t\t\t{2,\"TextButton\",{AutoButtonColor=false,BackgroundColor3=Color3.new(0.2549019753933,0.2549019753933,0.2549019753933),BorderSizePixel=0,Font=3,Name=\"Resizer\",Parent={1},Size=UDim2.new(0,5,1,0),Text=\"\",TextColor3=Color3.new(0,0,0),TextSize=14,}}, \t\t\t\t{3,\"Frame\",{BackgroundColor3=Color3.new(0.14117647707462,0.14117647707462,0.14117647707462),BorderSizePixel=0,Name=\"Line\",Parent={2},Position=UDim2.new(0,0,0,0),Size=UDim2.new(0,1,1,0),}}, \t\
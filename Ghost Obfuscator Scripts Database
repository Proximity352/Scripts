n \t\t\t\tscrollThumb.Size = UDim2.new(visible/total,0,1,0) \t\t\t\tif scrollThumb.AbsoluteSize.X < 16 then \t\t\t\t\tscrollThumb.Size = UDim2.new(0,16,1,0) \t\t\t\tend \t\t\t\tlocal fs = scrollThumbFrame.AbsoluteSize.X \t\t\t\tlocal bs = scrollThumb.AbsoluteSize.X \t\t\t\tscrollThumb.Position = UDim2.new(self:GetScrollPercent()*(fs-bs)/fs,0,0,0) \t\t\telse \t\t\t\tscrollThumb.Size = UDim2.new(1,0,visible/total,0) \t\t\t\tif scrollThumb.AbsoluteSize.Y < 16 then \t\t\t\t\tscrollThumb.Size = UDim2.new(1,0,0,16) \t\t\t\tend \t\t\t\tlocal fs = scrollThumbFrame.AbsoluteSize.Y \t\t\t\tlocal bs = scrollThumb.AbsoluteSize.Y \t\t\t\tscrollThumb.Position = UDim2.new(0,0,self:GetScrollPercent()*(fs-bs)/fs,0) \t\t\tend \t\tend \t\tlocal function createFrame(self) \t\t\tlocal newFrame = createSimple(\"Frame\",{Style=0,Active=true,AnchorPoint=Vector2.new(0,0),BackgroundColor3=Color3.new(0.35294118523598,0.35294118523598,0.35294118523598),BackgroundTransparency=0,BorderColor3=Color3.new(0.10588236153126,0.16470588743687,0.20784315466881),BorderSizePixel=0,ClipsDescendants=false,Draggable=false,Position=UDim2.new(1,-16,0,0),Rotation=0,Selectable=false,Size=UDim2.new(0,16,1,0),SizeConstraint=0,Visible=true,ZIndex=1,Name=\"ScrollBar\",}) \t\t\tlocal button1, button2 \t\t\tif self.Horizontal then \t\t\t\tnewFrame.Size = UDim2.new(1,0,0,16) \t\t\t\tbutton1 = createSimple(\"ImageButton\",{ \t\t\t\t\tParent = newFrame, \t\t\t\t\tName = \"Left\", \t\t\t\t\tSize = UDim2.new(0,16,0,16), \t\t\t\t\tBackgroundTransparency = 1, \t\t\t\t\tBorderSizePixel = 0, \t\t\t\t\tAutoButtonColor = false \t\t\t\t}) \t\t\t\tcreateArrow(16,4,\"left\").Parent = button1 \t\t\t\tbutton2 = createSimple(\"ImageButton\",{ \t\t\t\t\tParent = newFrame, \t\t\t\t\tName = \"Right\", \t\t\t\t\tPosition = UDim2.new(1,-16,0,0), \t\t\t\t\tSize = UDim2.new(0,16,0,16), \t\t\t\t\tBackgroundTransparency = 1, \t\t\t\t\tBorderSizePixel = 0, \t\t\t\t\tAutoButtonColor = false \t\t\t\t}) \t\t\t\tcreateArrow(16,4,\"right\").Parent = button2 \t\t\telse \t\t\t\tnewFrame.Size = UDim2.new(0,16,1,0) \t\t\t\tbutton1 = createSimple(\"ImageButton\",{ \t\t\t\t\tParent = newFrame, \t\t\t\t\tName = \"Up\", \t\t\t\t\tSize = UDim2.new(0,16,0,16), \t\t\t\t\tBackgroundTransparency = 1, \t\t\t\t\tBorderSizePixel = 0, \t\t\t\t\tAutoButtonColor = false \t\t\t\t}) \t\t\t\tcreateArrow(16,4,\"up\").Parent = button1 \t\t\t\tbutton2 = createSimple(\"ImageButton\",{ \t\t\t\t\tParent = newFrame, \t\t\t\t\tName = \"Down\", \t\t\t\t\tPosition = UDim2.new(0,0,1,-16), \t\t\t\t\tSize = UDim2.new(0,16,0,16), \t\t\t\t\tBackgroundTransparency = 1, \t\t\t\t\tBorderSizePixel = 0, \t\t\t\t\tAutoButtonColor = false \t\t\t\t}) \t\t\t\tcreateArrow(16,4,\"down\").Parent = button2 \t\t\tend \t\t\tlocal scrollThumbFrame = createSimple(\"ImageButton\", { \t\t\t\tBackgroundTransparency = 1, \t\t\t\tParent = newFrame \t\t\t}) \t\t\tif self.Horizontal then \t\t\t\tscrollThumbFrame.Position = UDim2.new(0,16,0,0) \t\t\t\tscrollThumbFrame.Size = UDim2.new(1,-32,1,0) \t\t\telse \t\t\t\tscrollThumbFrame.Position = UDim2.new(0,0,0,16) \t\t\t\tscrollThumbFrame.Size = UDim2.new(1,0,1,-32) \t\t\tend \t\t\t \t\t\tlocal scrollThumb = createSimple(\"Frame\", { \t\t\t\tBackgroundColor3 = Color3.new(120/255, 120/255, 120/255), \t\t\t\tBorderSizePixel = 0, \t\t\t\tParent = scrollThumbFrame \t\t\t}) \t\t\t \t\t\tlocal markerFrame = createSimple(\"Frame\", { \t\t\t\tBackgroundTransparency = 1, \t\t\t\tName = \"Markers\", \t\t\t\tSize = UDim2.new(1, 0, 1, 0), \t\t\t\tParent = scrollThumbFrame \t\t\t}) \t\t\t \t\t\tlocal buttonPress = false \t\t\tlocal thumbPress = false \t\t\tlocal thumbFramePress = false \t\t\t \t\t\tlocal function handleButtonPress(button, scrollDirection) \t\t\t\tif self:CanScroll(scrollDirection) then \t\t\t\t\tbutton.BackgroundTransparency = 0.5 \t\t\t\t\tself:ScrollToDirection(scrollDirection) \t\t\t\t\tself.Scrolled:Fire() \t\t\t\t\tlocal buttonTick = tick() \t\t\t\t\tlocal releaseEvent \t\t\t\t\treleaseEvent = user.InputEnded:Connect(function(input) \t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\tbutton.BackgroundTransparency = checkMouseInGui(button) and 0.8 or 1 \t\t\t\t\t\t\tbuttonPress = false \t\t\t\t\t\tend \t\t\t\t\tend) \t\t\t\t\twhile buttonPress do \t\t\t\t\t\tif tick() - buttonTick >= 0.25 and self:CanScroll(scrollDirection) then \t\t\t\t\t\t\tself:ScrollToDirection(scrollDirection) \t\t\t\t\t\t\tself.Scrolled:Fire() \t\t\t\t\t\tend \t\t\t\t\t\ttask.wait() \t\t\t\t\tend \t\t\t\tend \t\t\tend \t\t\t \t\t\tbutton1.MouseButton1Down:Connect(function(input) \t\t\t\tbuttonPress = true \t\t\t\thandleButtonPress(button1, \"Up\") \t\t\tend) \t\t\t \t\t\tbutton1.InputEnded:Connect(function(input) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\tbutton1.BackgroundTransparency = 1 \t\t\t\tend \t\t\tend) \t\t\t \t\t\tbutton2.MouseButton1Down:Connect(function(input) \t\t\t\tbuttonPress = true \t\t\t\thandleButtonPress(button2, \"Down\") \t\t\tend) \t\t\t \t\t\tbutton2.InputEnded:Connect(function(input) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\tbutton2.BackgroundTransparency = 1 \t\t\t\tend \t\t\tend) \t\t\t \t\t\tscrollThumb.InputBegan:Connect(function(input) \t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then \t\t\t\t\tlocal dir = self.Horizontal and \"X\" or \"Y\" \t\t\t\t\tlocal lastThumbPos = nil \t\t\t\t\tthumbPress = true \t\t\t\t\tscrollThumb.BackgroundTransparency = 0 \t\t\t\t\tlocal mouseOffset = mouse[dir] - scrollThumb.AbsolutePosition[dir] \t\t\t\t\tlocal releaseEvent \t\t\t\t\tlocal mouseEvent \t\t\t\t\t \t\t\t\t\treleaseEvent = user.InputEnded:Connect(function(input) \t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\tif mouseEvent then mouseEvent:Disconnect() end \t\t\t\t\t\t\tscrollThumb.BackgroundTransparency = 0.2 \t\t\t\t\t\t\tthumbPress = false \t\t\t\t\t\tend \t\t\t\t\tend) \t\t\t\t\t \t\t\t\t\tmouseEvent = user.InputChanged:Connect(function(input) \t\t\t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and thumbPress then \t\t\t\t\t\t\tlocal thumbFrameSize = scrollThumbFrame.AbsoluteSize[dir] - scrollThumb.AbsoluteSize[dir] \t\t\t\t\t\t\tlocal pos = mouse[dir] - scrollThumbFrame.AbsolutePosition[dir] - mouseOffset \t\t\t\t\t\t\tif pos > thumbFrameSize then pos = thumbFrameSize \t\t\t\t\t\t\telseif pos < 0 then pos = 0 end \t\t\t\t\t\t\tif lastThumbPos ~= pos then \t\t\t\t\t\t\t\tlastThumbPos = pos \t\t\t\t\t\t\t\tself:ScrollTo(math.floor(0.5 + pos / thumbFrameSize * (self.TotalSpace - self.VisibleSpace))) \t\t\t\t\t\t\tend \t\t\t\t\t\tend \t\t\t\t\tend) \t\t\t\tend \t\t\tend) \t\t\t \t\t\tscrollThumb.InputEnded:Connect(function(input) \t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\tscrollThumb.BackgroundTransparency = 0 \t\t\t\tend \t\t\tend) \t\t\t \t\t\tscrollThumbFrame.InputBegan:Connect(function(input) \t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and not checkMouseInGui(scrollThumb) then \t\t\t\t\tlocal dir = self.Horizontal and \"X\" or \"Y\" \t\t\t\t\tlocal scrollDir = (mouse[dir] >= scrollThumb.AbsolutePosition[dir] + scrollThumb.AbsoluteSize[dir]) and 1 or 0 \t\t\t\t\tlocal function doTick() \t\t\t\t\t\tlocal scrollSize = self.VisibleSpace - 1 \t\t\t\t\t\tif scrollDir == 0 and mouse[dir] < scrollThumb.AbsolutePosition[dir] then \t\t\t\t\t\t\tself:ScrollTo(self.Index - scrollSize) \t\t\t\t\t\telseif scrollDir == 1 and mouse[dir] >= scrollThumb.AbsolutePosition[dir] + scrollThumb.AbsoluteSize[dir] then \t\t\t\t\t\t\tself:ScrollTo(self.Index + scrollSize) \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\t \t\t\t\t\tthumbPress = false \t\t\t\t\tthumbFramePress = true \t\t\t\t\tdoTick() \t\t\t\t\tlocal thumbFrameTick = tick() \t\t\t\t\tlocal releaseEvent \t\t\t\t\treleaseEvent = user.InputEnded:Connect(function(input) \t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\tthumbFramePress = false \t\t\t\t\t\tend \t\t\t\t\tend) \t\t\t\t\t \t\t\t\t\twhile thumbFramePress do \t\t\t\t\t\tif tick() - thumbFrameTick >= 0.3 and checkMouseInGui(scrollThumbFrame) then \t\t\t\t\t\t\tdoTick() \t\t\t\t\t\tend \t\t\t\t\t\ttask.wait() \t\t\t\t\tend \t\t\t\tend \t\t\tend) \t\t\t \t\t\tnewFrame.MouseWheelForward:Connect(function() \t\t\t\tself:ScrollTo(self.Index - self.WheelIncrement) \t\t\tend) \t\t\t \t\t\tnewFrame.MouseWheelBackward:Connect(function() \t\t\t\tself:ScrollTo(self.Index + self.WheelIncrement) \t\t\tend) \t\t\t \t\t\tself.GuiElems.ScrollThumb = scrollThumb \t\t\tself.GuiElems.ScrollThumbFrame = scrollThumbFrame \t\t\tself.GuiElems.Button1 = button1 \t\t\tself.GuiElems.Button2 = button2 \t\t\tself.GuiElems.MarkerFrame = markerFrame \t\t\t \t\t\treturn newFrame \t\tend \t\t \t\tfuncs.Update = function(self,nocallback) \t\t\tlocal total = self.TotalSpace \t\t\tlocal visible = self.VisibleSpace \t\t\tlocal index = self.Index \t\t\tlocal button1 = self.GuiElems.Button1 \t\t\tlocal button2 = self.GuiElems.Button2 \t\t\t \t\t\tself.Index = math.clamp(self.Index, 0, math.max(0, total - visible)) \t\t\t \t\t\tif self.LastTotalSpace ~= self.TotalSpace then \t\t\t\tself.LastTotalSpace = self.TotalSpace \t\t\t\tself:UpdateMarkers() \t\t\tend \t\t\t \t\t\tif self:CanScrollUp() then \t\t\t\tfor i,v in pairs(button1.Arrow:GetChildren()) do \t\t\t\t\tv.BackgroundTransparency = 0 \t\t\t\tend \t\t\telse \t\t\t\tbutton1.BackgroundTransparency = 1 \t\t\t\tfor i,v in pairs(button1.Arrow:GetChildren()) do \t\t\t\t\tv.BackgroundTransparency = 0.5 \t\t\t\tend \t\t\tend \t\t\tif self:CanScrollDown() then \t\t\t\tfor i,v in pairs(button2.Arrow:GetChildren()) do \t\t\t\t\tv.BackgroundTransparency = 0 \t\t\t\tend \t\t\telse \t\t\t\tbutton2.BackgroundTransparency = 1 \t\t\t\tfor i,v in pairs(button2.Arrow:GetChildren()) do \t\t\t\t\tv.BackgroundTransparency = 0.5 \t\t\t\tend \t\t\tend \t\t\tdrawThumb(self) \t\tend \t\t \t\tfuncs.UpdateMarkers = function(self) \t\t\tlocal markerFrame = self.GuiElems.MarkerFrame \t\t\tmarkerFrame:ClearAllChildren() \t\t\tfor i,v in pairs(self.Markers) do \t\t\t\tif i < self.TotalSpace then \t\t\t\t\tcreateSimple(\"Frame\", { \t\t\t\t\t\tBackgroundTransparency = 0, \t\t\t\t\t\tBackgroundColor3 = v, \t\t\t\t\t\tBorderSizePixel = 0, \t\t\t\t\t\tPosition = self.Horizontal and UDim2.new(i/self.TotalSpace,0,1,-6) or UDim2.new(1,-6,i/self.TotalSpace,0), \t\t\t\t\t\tSize = self.Horizontal and UDim2.new(0,1,0,6) or UDim2.new(0,6,0,1), \t\t\t\t\t\tName = \"Marker\"..tostring(i), \t\t\t\t\t\tParent = markerFrame \t\t\t\t\t}) \t\t\t\tend \t\t\tend \t\tend \t\t \t\tfuncs.AddMarker = function(self,ind,color) \t\t\tself.Markers[ind] = color or Color3.new(0,0,0) \t\tend \t\tfuncs.ScrollTo = function(self, ind, nocallback) \t\t\tself.Index = ind \t\t\tself:Update() \t\t\tif not nocallback then \t\t\t\tself.Scrolled:Fire() \t\t\tend \t\tend \t\tfuncs.ScrollUp = function(self) \t\t\tself.Index = self.Index - self.Increment \t\t\tself:Update() \t\tend \t\tfuncs.CanScroll = function(self, direction) \t\t\tif direction == \"Up\" then \t\t\t\treturn self:CanScrollUp() \t\t\telseif direction == \"Down\" then \t\t\t\treturn self:CanScrollDown() \t\t\tend \t\t\treturn false \t\tend \t\tfuncs.ScrollDown = function(self) \t\t\tself.Index = self.Index + self.Increment \t\t\tself:Update() \t\tend \t\tfuncs.CanScrollUp = function(self) \t\t\treturn self.Index > 0 \t\tend \t\tfuncs.CanScrollDown = function(self) \t\t\treturn self.Index + self.VisibleSpace < self.TotalSpace \t\tend \t\tfuncs.GetScrollPercent = function(self) \t\t\treturn self.Index/(self.TotalSpace-self.VisibleSpace) \t\tend \t\tfuncs.SetScrollPercent = function(self,perc) \t\t\tself.Index = math.floor(perc*(self.TotalSpace-self.VisibleSpace)) \t\t\tself:Update() \t\tend \t\tfuncs.ScrollToDirection = function(self, Direaction) \t\t\tif Direaction == \"Up\" then \t\t\t\tself:ScrollUp() \t\t\telseif Direaction == \"Down\" then \t\t\t\tself:ScrollDown() \t\t\tend \t\tend \t\t \t\tfuncs.Texture = function(self,data) \t\t\tself.ThumbColor = data.ThumbColor or Color3.new(0,0,0) \t\t\tself.ThumbSelectColor = data.ThumbSelectColor or Color3.new(0,0,0) \t\t\tself.GuiElems.ScrollThumb.BackgroundColor3 = data.ThumbColor or Color3.new(0,0,0) \t\t\tself.Gui.BackgroundColor3 = data.FrameColor or Color3.new(0,0,0) \t\t\tself.GuiElems.Button1.BackgroundColor3 = data.ButtonColor or Color3.new(0,0,0) \t\t\tself.GuiElems.Button2.BackgroundColor3 = data.ButtonColor or Color3.new(0,0,0) \t\t\tfor i,v in pairs(self.GuiElems.Button1.Arrow:GetChildren()) do \t\t\t\tv.BackgroundColor3 = data.ArrowColor or Color3.new(0,0,0) \t\t\tend \t\t\tfor i,v in pairs(self.GuiElems.Button2.Arrow:GetChildren()) do \t\t\t\tv.BackgroundColor3 = data.ArrowColor or Color3.new(0,0,0) \t\t\tend \t\tend \t\tfuncs.SetScrollFrame = function(self,frame) \t\t\tif self.ScrollUpEvent then self.ScrollUpEvent:Disconnect() self.ScrollUpEvent = nil end \t\t\tif self.ScrollDownEvent then self.ScrollDownEvent:Disconnect() self.ScrollDownEvent = nil end \t\t\tself.ScrollUpEvent = frame.MouseWheelForward:Connect(function() self:ScrollTo(self.Index - self.WheelIncrement) end) \t\t\tself.ScrollDownEvent = frame.MouseWheelBackward:Connect(function() self:ScrollTo(self.Index + self.WheelIncrement) end) \t\tend \t\tlocal mt = {} \t\tmt.__index = funcs \t\tlocal function new(hor) \t\t\tlocal obj = setmetatable({ \t\t\t\tIndex = 0, \t\t\t\tVisibleSpace = 0, \t\t\t\tTotalSpace = 0, \t\t\t\tIncrement = 1, \t\t\t\tWheelIncrement = 1, \t\t\t\tMarkers = {}, \t\t\t\tGuiElems = {}, \t\t\t\tHorizontal = hor, \t\t\t\tLastTotalSpace = 0, \t\t\t\tScrolled = Lib.Signal.new() \t\t\t},mt) \t\t\tobj.Gui = createFrame(obj) \t\t\tobj:Texture({ \t\t\t\tThumbColor = Color3.fromRGB(60,60,60), \t\t\t\tThumbSelectColor = Color3.fromRGB(75,75,75), \t\t\t\tArrowColor = Color3.new(1,1,1), \t\t\t\tFrameColor = Color3.fromRGB(40,40,40), \t\t\t\tButtonColor = Color3.fromRGB(75,75,75) \t\t\t}) \t\t\treturn obj \t\tend \t\treturn {new = new} \tend)() \tLib.Window = (function() \t\tlocal funcs = {} \t\tlocal static = {MinWidth = 200, FreeWidth = 200} \t\tlocal mouse = plr:GetMouse() \t\tlocal sidesGui, alignIndicator \t\tlocal visibleWindows = {} \t\tlocal leftSide = {Width = 300, Windows = {}, ResizeCons = {}, Hidden = true} \t\tlocal rightSide = {Width = 300, Windows = {}, ResizeCons = {}, Hidden = true} \t\t \t\tlocal displayOrderStart \t\tlocal sideDisplayOrder \t\tlocal sideTweenInfo = TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out) \t\tlocal tweens = {} \t\tlocal isA = game.IsA \t\tlocal theme = { \t\t\tMainColor1 = Color3.fromRGB(52,52,52), \t\t\tMainColor2 = Color3.fromRGB(45,45,45), \t\t\tButton = Color3.fromRGB(60,60,60) \t\t} \t\t \t\tlocal function stopTweens() \t\t\tfor i = 1,#tweens do \t\t\t\ttweens[i]:Cancel() \t\t\tend \t\t\ttweens = {} \t\tend \t\tlocal function resizeHook(self,resizer,dir) \t\t\tlocal guiMain = self.GuiElems.Main \t\t\tresizer.InputBegan:Connect(function(input) \t\t\t\tif not self.Dragging and not self.Resizing and self.Resizable and self.ResizableInternal then \t\t\t\t\tlocal isH = dir:find(\"[WE]\") and true \t\t\t\t\tlocal isV = dir:find(\"[NS]\") and true \t\t\t\t\tlocal signX = dir:find(\"W\",1,true) and -1 or 1 \t\t\t\t\tlocal signY = dir:find(\"N\",1,true) and -1 or 1 \t\t\t \t\t\t\t\tif self.Minimized and isV then return end \t\t\t \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tresizer.BackgroundTransparency = 0.5 \t\t\t\t\telseif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tlocal releaseEvent, mouseEvent \t\t\t \t\t\t\t\t\tlocal offX = input.Position.X - resizer.AbsolutePosition.X \t\t\t\t\t\tlocal offY = input.Position.Y - resizer.AbsolutePosition.Y \t\t\t \t\t\t\t\t\tself.Resizing = resizer \t\t\t\t\t\tresizer.BackgroundTransparency = 1 \t\t\t \t\t\t\t\t\treleaseEvent = service.UserInputService.InputEnded:Connect(function(input) \t\t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\t\tif mouseEvent then mouseEvent:Disconnect() end \t\t\t\t\t\t\t\tself.Resizing = false \t\t\t\t\t\t\t\tresizer.BackgroundTransparency = 1 \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t \t\t\t\t\t\tmouseEvent = service.UserInputService.InputChanged:Connect(function(input) \t\t\t\t\t\t\tif self.Resizable and self.ResizableInternal and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then \t\t\t\t\t\t\t\tself:StopTweens() \t\t\t\t\t\t\t\tlocal deltaX = input.Position.X - resizer.AbsolutePosition.X - offX \t\t\t\t\t\t\t\tlocal deltaY = input.Position.Y - resizer.AbsolutePosition.Y - offY \t\t\t \t\t\t\t\t\t\t\tif guiMain.AbsoluteSize.X + deltaX * signX < self.MinX then deltaX = signX * (self.MinX - guiMain.AbsoluteSize.X) end \t\t\t\t\t\t\t\tif guiMain.AbsoluteSize.Y + deltaY * signY < self.MinY then deltaY = signY * (self.MinY - guiMain.AbsoluteSize.Y) end \t\t\t\t\t\t\t\tif signY < 0 and guiMain.AbsolutePosition.Y + deltaY < 0 then deltaY = -guiMain.AbsolutePosition.Y end \t\t\t \t\t\t\t\t\t\t\tguiMain.Position = guiMain.Position + UDim2.new(0, (signX < 0 and deltaX or 0), 0, (signY < 0 and deltaY or 0)) \t\t\t\t\t\t\t\tself.SizeX = self.SizeX + (isH and deltaX * signX or 0) \t\t\t\t\t\t\t\tself.SizeY = self.SizeY + (isV and deltaY * signY or 0) \t\t\t\t\t\t\t\tguiMain.Size = UDim2.new(0, self.SizeX, 0, self.Minimized and 20 or self.SizeY) \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t\t\tend \t\t\t\tend \t\t\tend) \t\t\t \t\t\tresizer.InputEnded:Connect(function(input) \t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and self.Resizing ~= resizer then \t\t\t\t\tresizer.BackgroundTransparency = 1 \t\t\t\tend \t\t\tend) \t\tend \t\tlocal updateWindows \t\tlocal function moveToTop(window) \t\t\tlocal found = table.find(visibleWindows,window) \t\t\tif found then \t\t\t\ttable.remove(visibleWindows,found) \t\t\t\ttable.insert(visibleWindows,1,window) \t\t\t\tupdateWindows() \t\t\tend \t\tend \t\tlocal function sideHasRoom(side,neededSize) \t\t\tlocal maxY = sidesGui.AbsoluteSize.Y - (math.max(0,#side.Windows - 1) * 4) \t\t\tlocal inc = 0 \t\t\tfor i,v in pairs(side.Windows) do \t\t\t\tinc = inc + (v.MinY or 100) \t\t\t\tif inc > maxY - neededSize then return false end \t\t\tend \t\t\treturn true \t\tend \t\tlocal function getSideInsertPos(side,curY) \t\t\tlocal pos = #side.Windows + 1 \t\t\tlocal range = {0,sidesGui.AbsoluteSize.Y} \t\t\tfor i,v in pairs(side.Windows) do \t\t\t\tlocal midPos = v.PosY + v.SizeY/2 \t\t\t\tif curY <= midPos then \t\t\t\t\tpos = i \t\t\t\t\trange[2] = midPos \t\t\t\t\tbreak \t\t\t\telse \t\t\t\t\trange[1] = midPos \t\t\t\tend \t\t\tend \t\t\treturn pos,range \t\tend \t\tlocal function focusInput(self,obj) \t\t\tif isA(obj,\"GuiButton\") then \t\t\t\tobj.MouseButton1Down:Connect(function() \t\t\t\t\tmoveToTop(self) \t\t\t\tend) \t\t\telseif isA(obj,\"TextBox\") then \t\t\t\tobj.Focused:Connect(function() \t\t\t\t\tmoveToTop(self) \t\t\t\tend) \t\t\tend \t\tend \t\tlocal createGui = function(self) \t\t\tlocal gui = create({ \t\t\t\t{1,\"ScreenGui\",{Nam
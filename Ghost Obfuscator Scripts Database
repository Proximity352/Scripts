equenceKeypoint.new(v[2],v[1])) \t\t\t\tend \t\t\t\tnewMt.Sequence = ColorSequence.new(newPoints) \t\t\t\tif not noupdate then newMt.OnSelect:Fire(newMt.Sequence) end \t\t\tend \t\t\tlocal function round(num,places) \t\t\t\tlocal multi = 10^places \t\t\t\treturn math.floor(num*multi + 0.5)/multi \t\t\tend \t\t\tlocal function updateInputs(point) \t\t\t\tif point then \t\t\t\t\tcurrentPoint = point \t\t\t\t\tlocal raw = point[2] \t\t\t\t\ttimeBox.Text = round(raw,(raw < 0.01 and 5) or (raw < 0.1 and 4) or 3) \t\t\t\t\tcolorBox.BackgroundColor3 = point[1] \t\t\t\tend \t\t\tend \t\t\tlocal function placeArrow(ind,point) \t\t\t\tlocal newArrow = arrow:Clone() \t\t\t\tnewArrow.Position = UDim2.new(0,ind-1,0,0) \t\t\t\tnewArrow.Visible = true \t\t\t\tnewArrow.Parent = arrowFrame \t\t\t\tnewArrow.InputBegan:Connect(function(input) \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tcursor.Visible = true \t\t\t\t\t\tcursor.Position = UDim2.new(0, 9 + newArrow.Position.X.Offset, 0, 0) \t\t\t\t\tend \t\t\t\t\t \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tupdateInputs(point) \t\t\t\t\t\tif point == beginPoint or point == endPoint or currentlySelected then return end \t\t\t\t\t\t \t\t\t\t\t\tlocal mouseEvent, releaseEvent \t\t\t\t\t\tcurrentlySelected = true \t\t\t\t\t\t \t\t\t\t\t\treleaseEvent = user.InputEnded:Connect(function(input) \t\t\t\t\t\t\tif input.UserInputType ~= Enum.UserInputType.MouseButton1 and input.UserInputType ~= Enum.UserInputType.Touch then return end \t\t\t\t\t\t\tmouseEvent:Disconnect() \t\t\t\t\t\t\treleaseEvent:Disconnect() \t\t\t\t\t\t\tcurrentlySelected = nil \t\t\t\t\t\t\tcursor.Visible = false \t\t\t\t\t\tend) \t\t\t\t\t\t \t\t\t\t\t\tmouseEvent = user.InputChanged:Connect(function(input) \t\t\t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\t\t\tlocal maxSize = colorLine.AbsoluteSize.X - 1 \t\t\t\t\t\t\t\tlocal relativeX = (input.Position.X - colorLine.AbsolutePosition.X) \t\t\t\t\t\t\t\tif relativeX < 0 then relativeX = 0 end \t\t\t\t\t\t\t\tif relativeX > maxSize then relativeX = maxSize end \t\t\t\t\t\t\t\tlocal raw = relativeX / maxSize \t\t\t\t\t\t\t\tpoint[2] = relativeX / maxSize \t\t\t\t\t\t\t\tupdateInputs(point) \t\t\t\t\t\t\t\tcursor.Visible = true \t\t\t\t\t\t\t\tcursor.Position = UDim2.new(0, 9 + newArrow.Position.X.Offset, 0, 0) \t\t\t\t\t\t\t\tbuildSequence() \t\t\t\t\t\t\t\tnewMt:Redraw() \t\t\t\t\t\t\tend \t\t\t\t\t\tend) \t\t\t\t\tend \t\t\t\tend) \t\t\t\t \t\t\t\tnewArrow.InputEnded:Connect(function(input) \t\t\t\t\tif input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then \t\t\t\t\t\tcursor.Visible = false \t\t\t\t\tend \t\t\t\tend) \t\t\t\t \t\t\t\t \t\t\t\treturn newArrow \t\t\tend \t\t\tlocal function placeArrows() \t\t\t\tfor i,v in pairs(colors) do \t\t\t\t\tv[3] = placeArrow(math.floor((colorLine.AbsoluteSize.X-1) * v[2]) + 1,v) \t\t\t\tend \t\t\tend \t\t\tlocal function redraw(self) \t\t\t\tgradient.Color = newMt.Sequence or ColorSequence.new(Color3.new(1,1,1)) \t\t\t\tfor i = 2,#colors do \t\t\t\t\tlocal nextColor = colors[i] \t\t\t\t\tlocal endPos = math.floor((colorLine.AbsoluteSize.X-1) * nextColor[2]) + 1 \t\t\t\t\tnextColor[3].Position = UDim2.new(0,endPos,0,0) \t\t\t\tend\t\t \t\t\tend \t\t\tnewMt.Redraw = redraw \t\t\tlocal function loadSequence(self,seq) \t\t\t\tresetSequence = seq \t\t\t\tfor i,v in pairs(colors) do if v[3] then v[3]:Destroy() end end \t\t\t\tcolors = {} \t\t\t\tcurrentlySelected = nil \t\t\t\tfor i,v in pairs(seq.Keypoints) do \t\t\t\t\tlocal newPoint = {v.Value,v.Time} \t\t\t\t\tnewPoint[3] = placeArrow(v.Time,newPoint) \t\t\t\t\ttable.insert(colors,newPoint) \t\t\t\tend \t\t\t\tbeginPoint = colors[1] \t\t\t\tendPoint = colors[#colors] \t\t\t\tcurrentlySelected = nil \t\t\t\tupdateInputs(colors[1]) \t\t\t\tbuildSequence(true) \t\t\t\tredraw() \t\t\tend \t\t\tnewMt.SetSequence = loadSequence \t\t\tlocal function buttonAnimations(button,inverse) \t\t\t\tbutton.InputBegan:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then button.BackgroundTransparency = (inverse and 0.5 or 0.4) end end) \t\t\t\tbutton.InputEnded:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then button.BackgroundTransparency = (inverse and 1 or 0) end end) \t\t\tend \t\t\t \t\t\tcolorLine.InputBegan:Connect(function(input) \t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and #colors < 20 then \t\t\t\t\tlocal maxSize = colorLine.AbsoluteSize.X - 1 \t\t\t\t\tlocal relativeX = (input.Position.X - colorLine.AbsolutePosition.X) \t\t\t\t\tif relativeX < 0 then relativeX = 0 end \t\t\t\t\tif relativeX > maxSize then relativeX = maxSize end \t\t\t\t\t \t\t\t\t\tlocal raw = relativeX / maxSize \t\t\t\t\tlocal fromColor = nil \t\t\t\t\tlocal toColor = nil \t\t\t\t\tfor i, col in pairs(colors) do \t\t\t\t\t\tif col[2] >= raw then \t\t\t\t\t\t\tfromColor = colors[math.max(i - 1, 1)] \t\t\t\t\t\t\ttoColor = colors[i] \t\t\t\t\t\t\tbreak \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\tlocal lerpColor = fromColor[1]:lerp(toColor[1], (raw - fromColor[2]) / (toColor[2] - fromColor[2])) \t\t\t\t\tlocal newPoint = {lerpColor, raw} \t\t\t\t\tnewPoint[3] = placeArrow(newPoint[2], newPoint) \t\t\t\t\ttable.insert(colors, newPoint) \t\t\t\t\tupdateInputs(newPoint) \t\t\t\t\tbuildSequence() \t\t\t\t\tredraw() \t\t\t\tend \t\t\tend) \t\t\t \t\t\tcolorLine.InputChanged:Connect(function(input) \t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then \t\t\t\t\tlocal maxSize = colorLine.AbsoluteSize.X - 1 \t\t\t\t\tlocal relativeX = (input.Position.X - colorLine.AbsolutePosition.X) \t\t\t\t\tif relativeX < 0 then relativeX = 0 end \t\t\t\t\tif relativeX > maxSize then relativeX = maxSize end \t\t\t\t\tcursor.Visible = true \t\t\t\t\tcursor.Position = UDim2.new(0, 10 + relativeX, 0, 0) \t\t\t\tend \t\t\tend) \t\t\t \t\t\tcolorLine.InputEnded:Connect(function(input) \t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then \t\t\t\t\tlocal inArrow = false \t\t\t\t\tfor i, v in pairs(colors) do \t\t\t\t\t\tif Lib.CheckMouseInGui(v[3]) then \t\t\t\t\t\t\tinArrow = v[3] \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\tcursor.Visible = inArrow and true or false \t\t\t\t\tif inArrow then cursor.Position = UDim2.new(0, 9 + inArrow.Position.X.Offset, 0, 0) end \t\t\t\tend \t\t\tend) \t\t\t \t\t\ttimeBox:GetPropertyChangedSignal(\"Text\"):Connect(function() \t\t\t\tlocal point = currentPoint \t\t\t\tlocal num = tonumber(timeBox.Text) \t\t\t\tif point and num and point ~= beginPoint and point ~= endPoint then \t\t\t\t\tnum = math.clamp(num,0,1) \t\t\t\t\tpoint[2] = num \t\t\t\t\tbuildSequence() \t\t\t\t\tredraw() \t\t\t\tend \t\t\tend) \t\t\t \t\t\tcolorBox.InputBegan:Connect(function(input) \t\t\t\tif (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then \t\t\t\t\tlocal editor = newMt.ColorPicker \t\t\t\t\tif not editor then \t\t\t\t\t\teditor = Lib.ColorPicker.new() \t\t\t\t\t\teditor.Window:SetTitle(\"ColorSequence Color Picker\") \t\t\t\t\t\t \t\t\t\t\t\teditor.OnSelect:Connect(function(col) \t\t\t\t\t\t\tif currentPoint then \t\t\t\t\t\t\t\tcurrentPoint[1] = col \t\t\t\t\t\t\tend \t\t\t\t\t\t\tbuildSequence() \t\t\t\t\t\t\tredraw() \t\t\t\t\t\tend) \t\t\t\t\t\t \t\t\t\t\t\tnewMt.ColorPicker = editor \t\t\t\t\tend \t\t\t\t\t \t\t\t\t\teditor.Window:ShowAndFocus() \t\t\t\tend \t\t\tend) \t\t\t \t\t\tdeleteButton.MouseButton1Click:Connect(function() \t\t\t\tif currentPoint and currentPoint ~= beginPoint and currentPoint ~= endPoint then \t\t\t\t\tfor i,v in pairs(colors) do \t\t\t\t\t\tif v == currentPoint then \t\t\t\t\t\t\tv[3]:Destroy() \t\t\t\t\t\t\ttable.remove(colors,i) \t\t\t\t\t\t\tbreak \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\t\tcurrentlySelected = nil \t\t\t\t\tupdateInputs(colors[1]) \t\t\t\t\tbuildSequence() \t\t\t\t\tredraw() \t\t\t\tend \t\t\tend) \t\t\t \t\t\tresetButton.MouseButton1Click:Connect(function() \t\t\t\tif resetSequence then \t\t\t\t\tnewMt:SetSequence(resetSequence) \t\t\t\tend \t\t\tend) \t\t\t \t\t\tcloseButton.MouseButton1Click:Connect(function() \t\t\t\twindow:Close() \t\t\tend) \t\t\ttopClose.MouseButton1Click:Connect(function() \t\t\t\twindow:Close() \t\t\tend) \t\t\tbuttonAnimations(deleteButton) \t\t\tbuttonAnimations(resetButton) \t\t\tbuttonAnimations(closeButton) \t\t\tplaceArrows() \t\t\tredraw() \t\t\tnewMt.Show = function(self) \t\t\t\twindow:Show() \t\t\tend \t\t\treturn newMt \t\tend \t\treturn {new = new} \tend)() \tLib.ViewportTextBox = (function() \t\tlocal textService = service.TextService \t\t \t\tlocal props = { \t\t\tOffsetX = 0, \t\t\tTextBox = PH, \t\t\tCursorPos = -1, \t\t\tGui = PH, \t\t\tView = PH \t\t} \t\tlocal funcs = {} \t\tfuncs.Update = function(self) \t\t\tlocal cursorPos = self.CursorPos or -1 \t\t\tlocal text = self.TextBox.Text \t\t\tif text == \"\" then self.TextBox.Position = UDim2.new(0,0,0,0) return end \t\t\tif cursorPos == -1 then return end \t\t\tlocal cursorText = text:sub(1,cursorPos-1) \t\t\tlocal pos = nil \t\t\tlocal leftEnd = -self.TextBox.Position.X.Offset \t\t\tlocal rightEnd = leftEnd + self.View.AbsoluteSize.X \t\t\tlocal totalTextSize = textService:GetTextSize(text,self.TextBox.TextSize,self.TextBox.Font,Vector2.new(999999999,100)).X \t\t\tlocal cursorTextSize = textService:GetTextSize(cursorText,self.TextBox.TextSize,self.TextBox.Font,Vector2.new(999999999,100)).X \t\t\tif cursorTextSize > rightEnd then \t\t\t\tpos = math.max(-1,cursorTextSize - self.View.AbsoluteSize.X + 2) \t\t\telseif cursorTextSize < leftEnd then \t\t\t\tpos = math.max(-1,cursorTextSize-2) \t\t\telseif totalTextSize < rightEnd then \t\t\t\tpos = math.max(-1,totalTextSize - self.View.AbsoluteSize.X + 2) \t\t\tend \t\t\tif pos then \t\t\t\tself.TextBox.Position = UDim2.new(0,-pos,0,0) \t\t\t\tself.TextBox.Size = UDim2.new(1,pos,1,0) \t\t\tend \t\tend \t\t \t\tfuncs.GetText = function(self) \t\t\treturn self.TextBox.Text \t\tend \t\t \t\tfuncs.SetText = function(self,text) \t\t\tself.TextBox.Text = text \t\tend \t\t \t\tlocal mt = getGuiMT(props,funcs) \t\t \t\tlocal function convert(textbox) \t\t\tlocal obj = initObj(props,mt) \t\t\tlocal view = Instance.new(\"Frame\") \t\t\tview.BackgroundTransparency = textbox.BackgroundTransparency \t\t\tview.BackgroundColor3 = textbox.BackgroundColor3 \t\t\tview.BorderSizePixel = textbox.BorderSizePixel \t\t\tview.BorderColor3 = textbox.BorderColor3 \t\t\tview.Position = textbox.Position \t\t\tview.Size = textbox.Size \t\t\tview.ClipsDescendants = true \t\t\tview.Name = textbox.Name \t\t\ttextbox.BackgroundTransparency = 1 \t\t\ttextbox.Position = UDim2.new(0,0,0,0) \t\t\ttextbox.Size = UDim2.new(1,0,1,0) \t\t\ttextbox.TextXAlignment = Enum.TextXAlignment.Left \t\t\ttextbox.Name = \"Input\" \t\t\tobj.TextBox = textbox \t\t\tobj.View = view \t\t\tobj.Gui = view \t\t\ttextbox.Changed:Connect(function(prop) \t\t\t\tif prop == \"Text\" or prop == \"CursorPosition\" or prop == \"AbsoluteSize\" then \t\t\t\t\tlocal cursorPos = obj.TextBox.CursorPosition \t\t\t\t\tif cursorPos ~= -1 then obj.CursorPos = cursorPos end \t\t\t\t\tobj:Update() \t\t\t\tend \t\t\tend) \t\t\tobj:Update() \t\t\tview.Parent = textbox.Parent \t\t\ttextbox.Parent = view \t\t\treturn obj \t\tend \t\t \t\tlocal function new() \t\t\tlocal textBox = Instance.new(\"TextBox\") \t\t\ttextBox.Size = UDim2.new(0,100,0,20) \t\t\ttextBox.BackgroundColor3 = Settings.Theme.TextBox \t\t\ttextBox.BorderColor3 = Settings.Theme.Outline3 \t\t\ttextBox.ClearTextOnFocus = false \t\t\ttextBox.TextColor3 = Settings.Theme.Text \t\t\ttextBox.Font = Enum.Font.SourceSans \t\t\ttextBox.TextSize = 14 \t\t\ttextBox.Text = \"\" \t\t\treturn convert(textBox) \t\tend \t\t \t\treturn {new = new, convert = convert} \tend)() \tLib.Label = (function() \t\tlocal props,funcs = {},{} \t\tlocal mt = getGuiMT(props,funcs) \t\tlocal function new() \t\t\tlocal label = Instance.new(\"TextLabel\") \t\t\tlabel.BackgroundTransparency = 1 \t\t\tlabel.TextXAlignment = Enum.TextXAlignment.Left \t\t\tlabel.TextColor3 = Settings.Theme.Text \t\t\tlabel.TextTransparency = 0.1 \t\t\tlabel.Size = UDim2.new(0,100,0,20) \t\t\tlabel.Font = Enum.Font.SourceSans \t\t\tlabel.TextSize = 14 \t\t\tlocal obj = setmetatable({ \t\t\t\tGui = label \t\t\t},mt) \t\t\treturn obj \t\tend \t\treturn {new = new} \tend)() \tLib.Frame = (function() \t\tlocal props,funcs = {},{} \t\tlocal mt = getGuiMT(props,funcs) \t\tlocal function new() \t\t\tlocal fr = Instance.new(\"Frame\") \t\t\tfr.BackgroundColor3 = Settings.Theme.Main1 \t\t\tfr.BorderColor3 = Settings.Theme.Outline1 \t\t\tfr.Size = UDim2.new(0,50,0,50) \t\t\tlocal obj = setmetatable({ \t\t\t\tGui = fr \t\t\t},mt) \t\t\treturn obj \t\tend \t\treturn {new = new} \tend)() \tLib.Button = (function() \t\tlocal props = { \t\t\tGui = PH, \t\t\tAnim = PH, \t\t\tDisabled = false, \t\t\tOnClick = SIGNAL, \t\t\tOnDown = SIGNAL, \t\t\tOnUp = SIGNAL, \t\t\tAllowedButtons = {1} \t\t} \t\tlocal funcs = {} \t\tlocal tableFind = table.find \t\tfuncs.Trigger = function(self,event,button) \t\t\tif not self.Disabled and tableFind(self.AllowedButtons,button) then \t\t\t\tself[\"On\"..event]:Fire(button) \t\t\tend \t\tend \t\tfuncs.SetDisabled = function(self,dis) \t\t\tself.Disabled = dis \t\t\tif dis then \t\t\t\tself.Anim:Disable() \t\t\t\tself.Gui.TextTransparency = 0.5 \t\t\telse \t\t\t\tself.Anim.Enable() \t\t\t\tself.Gui.TextTransparency = 0 \t\t\tend \t\tend \t\tlocal mt = getGuiMT(props,funcs) \t\tlocal function new() \t\t\tlocal b = Instance.new(\"TextButton\") \t\t\tb.AutoButtonColor = false \t\t\tb.TextColor3 = Settings.Theme.Text \t\t\tb.TextTransparency = 0.1 \t\t\tb.Size = UDim2.new(0,100,0,20) \t\t\tb.Font = Enum.Font.SourceSans \t\t\tb.TextSize = 14 \t\t\tb.BackgroundColor3 = Settings.Theme.Button \t\t\tb.BorderColor3 = Settings.Theme.Outline2 \t\t\tlocal obj = initObj(props,mt) \t\t\tobj.Gui = b \t\t\tobj.Anim = Lib.ButtonAnim(b,{Mode = 2, StartColor = Settings.Theme.Button, HoverColor = Settings.Theme.ButtonHover, PressColor = Settings.Theme.ButtonPress, OutlineColor = Settings.Theme.Outline2}) \t\t\t \t\t\tb.MouseButton1Click:Connect(function() obj:Trigger(\"Click\",1) end) \t\t\tb.MouseButton1Down:Connect(function() obj:Trigger(\"Down\",1) end) \t\t\tb.MouseButton1Up:Connect(function() obj:Trigger(\"Up\",1) end) \t\t\t \t\t\tb.MouseButton2Click:Connect(function() obj:Trigger(\"Click\",2) end) \t\t\tb.MouseButton2Down:Connect(function() obj:Trigger(\"Down\",2) end) \t\t\tb.MouseButton2Up:Connect(function() obj:Trigger(\"Up\",2) end) \t\t\treturn obj \t\tend \t\treturn {new = new} \tend)() \tLib.DropDown = (function() \t\tlocal props = { \t\t\tGui = PH, \t\t\tAnim = PH, \t\t\tContext = PH, \t\t\tSelected = PH, \t\t\tDisabled = false, \t\t\tCanBeEmpty = true, \t\t\tOptions = {}, \t\t\tGuiElems = {}, \t\t\tOnSelect = SIGNAL \t\t} \t\tlocal funcs = {} \t\t \t\tfuncs.Update = function(self) \t\t\tlocal options = self.Options \t\t\tif #options > 0 then \t\t\t\tif not self.Selected then \t\t\t\t\tif not self.CanBeEmpty then \t\t\t\t\t\tself.Selected = options[1] \t\t\t\t\t\tself.GuiElems.Label.Text = options[1] \t\t\t\t\telse \t\t\t\t\t\tself.GuiElems.Label.Text = \"- Select -\" \t\t\t\t\tend \t\t\t\telse \t\t\t\t\tself.GuiElems.Label.Text = self.Selected \t\t\t\tend \t\t\telse \t\t\t\tself.GuiElems.Label.Text = \"- Select -\" \t\t\tend \t\tend \t\t \t\tfuncs.ShowOptions = function(self) \t\t\tlocal context = self.Context \t\t\tcontext.Width = self.Gui.AbsoluteSize.X \t\t\tcontext.ReverseYOffset = self.Gui.AbsoluteSize.Y \t\t\tcontext:Show(self.Gui.AbsolutePosition.X, self.Gui.AbsolutePosition.Y + context.ReverseYOffset) \t\tend \t\t \t\tfuncs.SetOptions = function(self,opts) \t\t\tself.Options = opts \t\t\t \t\t\tlocal context = self.Context \t\t\tlocal options = self.Options \t\t\tcontext:Clear() \t\t\t \t\t\tlocal onClick = function(option) self.Selected = option self.OnSelect:Fire(option) self:Update() end \t\t\t \t\t\tif self.CanBeEmpty then \t\t\t\tcontext:Add({Name = \"- Select -\", function() self.Selected = nil self.OnSelect:Fire(nil) self:Update() end}) \t\t\tend \t\t\t \t\t\tfor i = 1,#options do \t\t\t\tcontext:Add({Name = options[i], OnClick = onClick}) \t\t\tend \t\t\t \t\t\tself:Update() \t\tend \t\t \t\tfuncs.SetSelected = function(self,opt) \t\t\tself.Selected = type(opt) == \"number\" and self.Options[opt] or opt \t\t\tself:Update() \t\tend \t\t \t\tlocal mt = getGuiMT(props,funcs) \t\t \t\tlocal function new() \t\t\tlocal f = Instance.new(\"TextButton\") \t\t\tf.AutoButtonColor = false \t\t\tf.Text = \"\" \t\t\tf.Size = UDim2.new(0,100,0,20) \t\t\tf.BackgroundColor3 = Settings.Theme.TextBox \t\t\tf.BorderColor3 = Settings.Theme.Outline3 \t\t\t \t\t\tlocal label = Lib.Label.new() \t\t\tlabel.Position = UDim2.new(0,2,0,0) \t\t\tlabel.Size = UDim2.new(1,-22,1,0) \t\t\tlabel.TextTruncate = Enum.TextTruncate.AtEnd \t\t\tlabel.Parent = f \t\t\tlocal arrow = create({ \t\t\t\t{1,\"Frame\",{BackgroundTransparency=1,Name=\"EnumArrow\",Position=UDim2.new(1,-16,0,2),Size=UDim2.new(0,16,0,16),}}, \t\t\t\t{2,\"Frame\",{BackgroundColor3=Color3.new(0.86274510622025,0.86274510622025,0.86274510622025),BorderSizePixel=0,Parent={1},Position=UDim2.new(0,8,0,9),Size=UDim2.new(0,1,0,1),}}, \t\t\t\t{3,\"Frame\",{BackgroundColor3=Color3.new(0.86274510622025,0.86274510622025,0.86274510622025),BorderSizePixel=0,Parent={1},Position=UDim2.new(0,7,0,8),Size=UDim2.new(0,3,0,1),}}, \t\t\t\t{4,\"Frame\",{BackgroundColor3=Color3.new(0.86274510622025,0.86274510622025,0.86274510622025),BorderSizePixel=0,Parent={1},Position=UDim2.new(0,6,0,7),Size=UDim2.new(0,5,0,1),}}, \t\t\t}) \t\t\tarrow.Parent = f \t\t\tlocal obj = initObj(props,mt) \t\t\tobj.Gui = f \t\t\tobj.Anim = Lib.ButtonAnim(f,{Mode = 2, StartColor = Settings.Theme.TextBox, LerpTo = Settings.Theme.Button, LerpDelta = 0.15}) \t\t\tobj.Context = Lib.ContextMenu.new() \t\t\tobj.Context.Iconless = true \t\t\tobj.Context.MaxHeight = 200 \t\t\tobj.Selected = nil \t\t\tobj.GuiElems = {Label = label} \t\t\tf.MouseButton1Down:Connect(function() obj:ShowOptions() end) \t\t\tobj:Update() \t\t\treturn obj \t\tend \t\treturn {new = new} \tend)() \tLib.ClickSystem = (function() \t\tlocal props = { \t\t\tLastItem = PH, \t\t\tOnDown = SIGNAL, \t\t\tOnRelease = SIGNAL, \t\t\tAllowedButtons = {1}, \t\t\tCombo = 0, \t\t\tMaxCombo = 2, \t\t\tComboTime = 0.5, \t\t\tItems = {}, \t\t\tItemCons = {}, \t\t\tClickId = -1, \t\t\tLastButton = \"\" \t\t} \t\tlocal funcs = {} \t\tlocal tostring = tostring \t\t \t\tlocal disconnect = function(con) \t\t\tlocal pos = table.find(con.Signal.Connections,con) \t\t\tif pos then table.remove(con.Signal.Connections,pos) end \t\tend \t\t \t\tfuncs.Trigger = function(self, item, button, X, Y) \t\t\tif table.find(self.AllowedButtons, button) then \t\t\t\tif self.LastButton ~= button or self.LastItem ~= item or self.Combo == self.MaxCombo or tick() - self.ClickId > self.ComboTime then \t\t\t\t\tself.Combo = 0 \t\t\t\t\tself.LastButton = button \t\t\t\t\tself.LastItem = item \t\t\t\tend \t\t\t\t \t\t\t\tself.Combo = self.Combo + 1 \t\t\t\tself.ClickId = tick() \t\t\t\t \t\t\t\ttask.spawn(function() \t\t\t\t\tif self.InputDown then \t\t\t\t\t\tself.InputDown = false \t\t\t\t\telse \t\t\t\t\t\tself.InputDown = tick() \t\t\t\t\t\t \t\t\t\t\t\tlocal Connection = item.MouseButton1Up:Once(function() \t\t\t\t\t\t\tself.InputDown = false \t\t\t\t\t\tend) \t\t\t\t\t\t \t\t\t\t\t\twhile self.InputDown and not Explorer.Dragging do \t\t\t\t\t\t\tif (tick() - self.InputDown) >= 0.4 then \t\t\t\t\t\t\t\tself.InputDown = false \t\t\t\t\t\t\t\tself[\"OnRelease\"]:Fire(item, self.Combo, 2, Vector2.new(X, Y)) \t\t\t\t\t\t\t\tbreak \t\t\t\t\t\t\tend;task.wait() \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\tend) \t\t\t\t \t\t\t\tlocal release \t\t\t\trelease = service.UserInputService.InputEnded:Connect(function(input) \t\t\t\t\tif input.UserInputType == Enum.UserInputType[\"MouseButton\" .. button] then \t\t\t\t\t\trelease:Disconnect() \t\t\t\t\t\tif Lib.CheckMouseInGui(item) and self.LastButton == button and self.LastItem == item then \t\t\t\t\t\t\tself[\"OnRelease\"]:Fire(item,self.Combo,button) \t\t\t\t\t\tend \t\t\t\t\tend \t\t\t\tend) \t\t\t\t \t\t\t\tself[\"OnDown\"]:Fire(item,self.Combo,button) \t\t\tend \t\tend \t\t \t\tfuncs.Add = function(self,item) \t\t\tif table.find(self.Items,item) then return end \t\t\tlocal cons = {} \t\t\tcons[1] = item.MouseButton1Down:Connect(function(X, Y) self:Trigger(item, 1, X, Y) end) \t\t\tcons[2] = item.MouseButton2Down:Connect(function(X, Y) self:Trigger(item, 2, X, Y) end) \t\t\t \t\t\tself.ItemCons[item] = cons \t\t\tself.Items[#self.Items+1] = item \t\tend \t\t \t\tfuncs.Remove = function(self,item) \t\t\tlocal ind = table.find(self.Items,item) \t\t\tif not ind then return end \t\t\tfor i,v in pairs(self.ItemCons[item]) do \t\t\t\tv:Disconnect() \t\t\tend \t\t\tself.ItemCons[item] = nil \t\t\ttable.remove(self.Items,ind) \t\tend \t\t \t\tlocal mt = {__index = funcs} \t\tlocal function new() \t\t\tlocal obj = initObj(props,mt) \t\t\treturn obj \t\tend \t\treturn {new = new} \tend)() \treturn Lib end return {InitDeps = initDeps, InitAfterMain = initAfterMain, Main = main} end } -- Main vars local Main, Explorer, Properties, ScriptViewer, DefaultSettings, Notebook, Serializer, Lib local API, RM -- Default Settings DefaultSettings = (function() \tlocal rgb = Color3.fromRGB \treturn { \t\tExplorer = { \t\t\t_Recurse = true, \t\t\tSorting = true, \t\t\tTeleportToOffset = Vector3.new(0,0,0), \t\t\tClickToRename = true, \t\t\tAutoUpdateSearch = true, \t\t\tAutoUpdateMode = 0, -- 0 Default, 1 no tree update, 2 no descendant events, 3 frozen \t\t\tPartSelectionBox = true, \t\t\tGuiSelectionBox = true, \t\t\tCopyPathUseGetChildren = true \t\t}, \t\tProperties = { \t\t\t_Recurse = true, \t\t\tMaxConflictCheck = 50, \t\t\tShowDeprecated = false, \t\t\tShowHidden = false, \t\t\tClearOnFocus = false, \t\t\tLoadstringInput = true, \t\t\tNumberRounding = 3, \t\t\tShowAttributes = false, \t\t\tMaxAttributes = 50, \t\t\tScaleType = 1 -- 0 Full Name Shown, 1 Equal Halves \t\t}, \t\tTheme = { \t\t\t_Recurse = true, \t\t\tMain1 = rgb(52,52,52), \t\t\tMain2 = rgb(45,45,45), \t\t\tOutline1 = rgb(33,33,33), -- Mainly frames \t\t\tOutline2 = rgb(55,55,55), -- Mainly button \t\t\tOutline3 = rgb(30,30,30), -- Mainly textbox \t\t\tTextBox = rgb(38,38,38), \t\t\tMenu = rgb(32,32,32), \t\t\tListSelection = rgb(11,90,175), \t\t\tButton = rgb(60,60,60), \t\t\tButtonHover = rgb(68,68,68), \t\t\tButtonPress = rgb(40,40,40), \t\t\tHighlight = rgb(75,75,75), \t\t\tText = rgb(255,255,255), \t\t\tPlaceholderText = rgb(100,100,100), \t\t\tImportant = rgb(255,0,0), \t\t\tExplorerIconMap = \"\", \t\t\tMiscIconMap = \"\", \t\t\tSyntax = { \t\t\t\tText = rgb(204,204,204), \t\t\t\tBackground = rgb(36,36,36), \t\t\t\tSelection = rgb(255,255,255), \t\t\t\tSelectionBack = rgb(11,90,175), \t\t\t\tOperator = rgb(204,204,204), \t\t\t\tNumber = rgb(255,198,0), \t\t\t\tString = rgb(173,241,149), \t\t\t\tComment = rgb(102,102,102), \t\t\t\tKeyword = rgb(248,109,124), \t\t\t\tError = rgb(255,0,0), \t\t\t\tFindBackground = rgb(141,118,0), \t\t\t\tMatchingWord = rgb(85,85,85), \t\t\t\tBuiltIn = rgb(132,214,247), \t\t\t\tCurrentLine = rgb(45,50,65), \t\t\t\tLocalMethod = rgb(253,251,172), \t\t\t\tLocalProperty = rgb(97,161,241), \t\t\t\tNil = rgb(255,198,0), \t\t\t\tBool = rgb(255,198,0), \t\t\t\tFunction = rgb(248,109,124), \t\t\t\tLocal = rgb(248,109,124), \t\t\t\tSelf = rgb(248,109,124), \t\t\t\tFunctionName = rgb(253,251,172), \t\t\t\tBracket = rgb(204,204,204) \t\t\t}, \t\t} \t} end)() -- Vars local Settings = {} local Apps = {} local env = {} local plr = service.Players.LocalPlayer or service.Players.PlayerAdded:wait() local create = function(data) \tlocal insts = {} \tfor i,v in pairs(data) do insts[v[1]] = Instance.new(v[2]) end \t \tfor _,v in pairs(data) do \t\tfor prop,val in pairs(v[3]) do \t\t\tif type(val) == \"table\" then \t\t\t\tinsts[v[1]][prop] = insts[val[1]] \t\t\telse \t\t\t\tinsts[v[1]][prop] = val \t\t\tend \t\tend \tend \t \treturn insts[1] end local createSimple = function(class,props) \tlocal inst = Instance.new(class) \tfor i,v in next,props do \t\tinst[i] = v \tend \treturn inst end Main = (function() \tlocal Main = {} \t \tMain.ModuleList = {\"Explorer\", \"Properties\", \"ScriptViewer\"} \tMain.Elevated = false \tMain.MissingEnv = {} \tMain.Version = \"\" -- Beta 1.0.0 \tMain.Mouse = plr:GetMouse() \tMain.AppControls = {} \tMain.Apps = Apps \tMain.MenuApps = {} \t \tMain.DisplayOrders = { \t\tSideWindow = 8, \t\tWindow = 10, \t\tMenu = 100000, \t\tCore = 101000 \t} \t \tMain.GetInitDeps = function() \t\treturn { \t\t\tMain = Main, \t\t\tLib = Lib, \t\t\tApps = Apps, \t\t\tSettings = Settings, \t\t\t \t\t\tAPI = API, \t\t\tRMD = RMD, \t\t\tenv = env, \t\t\tservice = service, \t\t\tplr = plr, \t\t\tcreate = create, \t\t\tcreateSimple = createSimple \t\t} \tend \t \tMain.Error = function(str) \t\tif rconsoleprint then \t\t\trconsoleprint(\"DEX ERROR: \"..tostring(str)..\"\\n\") \t\t\twait(9e9) \t\telse \t\t\terror(str) \t\tend \tend \t \tMain.LoadModule = function(name) \t\tif Main.Elevated then -- If you don't have filesystem api then ur outta luck tbh \t\t\tlocal control \t\t\t \t\t\tif EmbeddedModules then -- Offline Modules \t\t\t\tcontrol = EmbeddedModules[name]() \t\t\t\t \t\t\t\tif not control then Main.Error(\"Missing Embedded Module: \"..name) end \t\t\tend \t\t\t \t\t\tMain.AppControls[name] = control \t\t\tcontrol.InitDeps(Main.GetInitDeps()) \t\t\tlocal moduleData = control.Main() \t\t\tApps[name] = moduleData \t\t\treturn moduleData \t\telse \t\t\tlocal module = script:WaitForChild(\"Modules\"):WaitForChild(name, 2) \t\t\tif not module then Main.Error(\"CANNOT FIND MODULE \" .. name) end \t\t\t \t\t\tlocal control = require(module) \t\t\tMain.AppControls[name] = control \t\t\tcontrol.InitDeps(Main.GetInitDeps()) \t\t\t \t\t\tlocal moduleData = control.Main() \t\t\tApps[name] = moduleData \t\t\treturn moduleData \t\tend \tend \t \tMain.LoadModules = function() \t\tfor i,v in pairs(Main.ModuleList) do \t\t\tlocal s,e = pcall(Main.LoadModule,v) \t\t\tif not s then \t\t\t\tMain.Error((\"FAILED LOADING %s CAUSE %s\"):format(v, e)) \t\t\tend \t\tend \t\t \t\t-- Init Major Apps and define them in modules \t\tExplorer = Apps.Explorer \t\tProperties = Apps.Properties \t\tScriptViewer = Apps.ScriptViewer \t\tNotebook = Apps.Notebook \t\tlocal appTable = { \t\t\tExplorer = Explorer, \t\t\tProperties = Properties, \t\t\tScriptViewer = ScriptViewer, \t\t\tNotebook = Notebook \t\t} \t\t \t\tMain.AppControls.Lib.InitAfterMain(appTable) \t\tfor i,v in pairs(Main.ModuleList) do \t\t\tlocal control = Main.AppControls[v] \t\t\tif control then \t\t\t\tcontrol.InitAfterMain(appTable) \t\t\tend \t\tend \tend \t \tMain.InitEnv = function() \t\t\t\tsetmetatable(env, {__newindex = function(self, name, func) \t\t\t\t\t\tif not func then Main.MissingEnv[#Main.MissingEnv + 1] = name return end \t\t\t\t\t\trawset(self, name, func) \t\t\t\tend}) \t\t\t\t-- file \t\t\t\tenv.readfile = readfile \t\t\t\tenv.writefile = writefile \t\t\t\tenv.appendfile = appendfile \t\t\t\tenv.makefolder = makefolder \t\t\t\tenv.listfiles = listfiles \t\t\t\tenv.loadfile = loadfile \t\t\t\tenv.movefileas = movefileas \t\t\t\tenv.saveinstance = saveinstance \t\t\t\t-- debug \t\t\t\tenv.getupvalues = (debug and debug.getupvalues) or getupvalues or getupvals \t\t\t\tenv.getconstants = (debug and debug.ge